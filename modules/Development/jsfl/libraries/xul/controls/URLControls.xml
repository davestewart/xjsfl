<?xml version="1.0" encoding="UTF-8"?>
 <dialog id="dialog" title="Invisible Button" buttons="accept,cancel">
 	<block id="URL">	 
	 	<label id="URL_LABEL" value="URL:" align="right" control="URL_EXPRESSION"/>
		<expression id="URL_EXPRESSION" size="60" prompt="URL"/>
	</block>

	<block id="URL_WINDOW">
		<label value="Window:"  align="right" control="URL_WINDOW_MENU"/>
		<menulist id="URL_WINDOW_MENU" required="false" editable="true" width="100" oncreate="fl.runScript(fl.configURI + '/Javascript/url.jsfl', 'setControlToURLWindowOptions','URL_WINDOW_MENU');">
			<menuitem label=""/>                     
		</menulist>
	</block>
	
 </dialog>