<?xml version="1.0" encoding="UTF-8"?>
<codeintel version="2.0" name="JSFL" description="Adobe Flash JSAPI (CS4)">
 	<file lang="JavaScript" path="javascript.cix" doc="JSFL API for Flash">
		<scope name="*" ilk="blob" lang="JavaScript">
			<!--

				Top-level variables

			-->
			<variables name="flash" citdl="Flash"/>
			<variables name="fl" citdl="Flash"/>
			<variables name="App" citdl="Flash"/>
			<variables name="app" citdl="Flash"/>
			<variables name="window" citdl="Window"/>
			<variables name="document" citdl="Document"/>
			<!--

				Top-level functions

			-->
			<scope name="activate" ilk="function" signature="function activate() { // statements }" doc="Called when the extensible tool becomes active (that is, when the tool is selected in the Tools panel). Use this function to perform any initialization tasks the tool requires. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7765.html"/>
			<scope name="alert" ilk="function" signature="alert ( alertText )" doc="Displays a string in a modal Alert dialog box, along with an OK button. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f12.html">
				<scope name="alertText" ilk="argument" citdl="String" doc="A string that specifies the message you want to display in the Alert dialog box."/>
			</scope>
			<scope name="configureTool" ilk="function" signature="function configureTool() { // statements }" doc="Called when Flash opens and the extensible tool is loaded into the Tools panel. Use this function to set any information Flash needs to know about the tool. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77e9.html"/>
			<scope name="confirm" ilk="function" returns="Boolean" signature="confirm ( strAlert ):Boolean" doc="Displays a string in a modal Alert dialog box, along with OK and Cancel buttons." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f16.html">
				<scope name="strAlert" ilk="argument" citdl="String" doc="A string that specifies the message you want to display in the Alert dialog box."/>
			</scope>
			<scope name="deactivate" ilk="function" signature="function deactivate() { // statements }" doc="Called when the extensible tool becomes inactive (that is, when the active tool changes from this tool to another one). Use this function to perform any cleanup the tool needs. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7760.html"/>
			<scope name="keyDown" ilk="function" signature="function keyDown() { // statements }" doc="Called when the extensible tool is active and the user presses a key. The script should call tools.getKeyDown() to determine which key was pressed." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f0b.html"/>
			<scope name="keyUp" ilk="function" signature="function keyUp() { // statements }" doc="Called when the extensible tool is active and a key is released. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f0d.html"/>
			<scope name="mouseDoubleClick" ilk="function" signature="function mouseDoubleClick() { // statements }" doc="Called when the extensible tool is active and the mouse button is double-clicked on the Stage. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-775c.html"/>
			<scope name="mouseDown" ilk="function" signature="function mouseDown( [ pt ] ) { // statements }" doc="Called when the extensible tool is active and the mouse button is pressed while the pointer is over the Stage. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-775b.html">
				<scope name="pt" ilk="argument" citdl="Object" doc="A point that specifies the location of the mouse when the button is pressed. It is passed to the function when the mouse button is pressed. This parameter is optional."/>
			</scope>
			<scope name="mouseMove" ilk="function" signature="function mouseMove( [ pt ] ) { // statements }" doc="Called whenever the extensible tool is active and the mouse moves over a specified point on the Stage. The mouse button can be down or up." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-775a.html">
				<scope name="pt" ilk="argument" citdl="Object" doc="A point that specifies the current location of the mouse. It is passed to the function whenever the mouse moves, which tracks the mouse location. If the Stage is in edit or edit‑in‑place mode, the point coordinates are relative to the object being edited. Otherwise, the point coordinates are relative to the Stage. This parameter is optional."/>
			</scope>
			<scope name="mouseUp" ilk="function" signature="function mouseUp() { // statements }" doc="Called whenever the extensible tool is active and the mouse button is released after being pressed on the Stage." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7759.html"/>
			<scope name="notifySettingsChanged" ilk="function" signature="function notifySettingsChanged() { // statements }" doc="Called when the extensible tool is active and the user changes its options in the Property inspector. You can use the tools.activeTool property to query the current values of the options (see tools.activeTool)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7758.html"/>
			<scope name="prompt" ilk="function" returns="String" signature="prompt(promptMsg [,text]):String" doc="Displays a prompt and optional text in a modal Alert dialog box, along with OK and Cancel buttons." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f15.html">
				<scope name="promptMsg" ilk="argument" citdl="String" doc="A string to display in the Prompt dialog box (limited to 256 characters in Mac OS X)."/>
				<scope name="text" ilk="argument" citdl="String" doc="An optional string to display as a default value for the text field."/>
			</scope>
			<scope name="setCursor" ilk="function" signature="function setCursor() { // statements }" doc="Called when the extensible tool is active and the mouse moves, to allow the script to set custom pointers. The script should call tools.setCursor() to specify the pointer to use. For a list that shows which pointers correspond to which integer values, see tools.setCursor()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7755.html"/>
			<!--

				ActionsPanel

			-->
			<scope name="ActionsPanel" ilk="class" doc="The actionsPanel object, which represents the currently displayed Actions panel, is a property of the flash object (see fl.actionsPanel)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-8000.html">
				<scope name="getClassForObject" ilk="function" returns="String" signature="getClassForObject(ASvariableName):String" doc="Returns the class of the specified variable, which must be defined in the currently displayed Actions panel. In addition, the cursor or selected text in the Actions panel must be positioned after the variable definition." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb9.html">
					<scope name="ASvariableName" ilk="argument" citdl="String" doc="A string that represents the name of an ActionScript variable."/>
				</scope>
				<scope name="getScriptAssistMode" ilk="function" returns="Boolean" signature="getScriptAssistMode():Boolean" doc="Specifies whether Script Assist mode is enabled." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb8.html"/>
				<scope name="getSelectedText" ilk="function" returns="String" signature="getSelectedText():String" doc="Returns the text that is currently selected in the Actions panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eaf.html"/>
				<scope name="getText" ilk="function" returns="String" signature="getText():String" doc="Returns the text in the Actions panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb6.html"/>
				<scope name="hasSelection" ilk="function" returns="Boolean" signature="hasSelection():Boolean" doc="Specifies whether any text is currently selected in the Actions panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb5.html"/>
				<scope name="replaceSelectedText" ilk="function" returns="Boolean" signature="replaceSelectedText(replacementText):Boolean" doc="Replaces the currently selected text with the text specified in replacementText. If replacementText contains more characters than the selected text, any characters following the selected text now follow replacementText; that is, they are not overwritten." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb4.html">
					<scope name="replacementText" ilk="argument" citdl="String" doc="A string that represents text to replace selected text in the Actions panel."/>
				</scope>
				<scope name="setScriptAssistMode" ilk="function" returns="Boolean" signature="setScriptAssistMode(bScriptAssist):Boolean" doc="Enables or disables Script Assist mode." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb1.html">
					<scope name="bScriptAssist" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to enable or disable Script Assist mode."/>
				</scope>
				<scope name="setSelection" ilk="function" returns="Boolean" signature="setSelection(startIndex, numberOfChars):Boolean" doc="Selects a specified set of characters in the Actions panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb3.html">
					<scope name="startIndex" ilk="argument" citdl="Number" doc="A zero-based integer that specifies the first character to be selected."/>
					<scope name="numberOfChars" ilk="argument" citdl="Number" doc="An integer that specifies how many characters to select."/>
				</scope>
				<scope name="setText" ilk="function" returns="Boolean" signature="setText(replacementText):Boolean" doc="Clears any text in the Actions panel and then adds the text specified in replacementText." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7eb2.html">
					<scope name="replacementText" ilk="argument" citdl="String" doc="A string that represents text to place in the Actions panel."/>
				</scope>
			</scope>
			<!--

				BitmapInstance

			-->
			<scope name="BitmapInstance" ilk="class" classrefs="Instance" doc="The BitmapInstance object is a subclass of the Instance object and represents a bitmap in a frame (see Instance object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f67.html">
				<scope name="getBits" ilk="function" returns="Object" signature="getBits():Object" doc="Lets you create bitmap effects by getting the bits out of the bitmap, manipulating them, and then returning them to Flash. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e98.html"/>
				<variable name="hPixels" citdl="Number" doc="Read-only property; an integer that represents the width of the bitmap—that is, the number of pixels in the horizontal dimension. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e94.html"/>
				<scope name="setBits" ilk="function" signature="setBits(bitmap)" doc="Nothing. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e97.html">
					<scope name="bitmap" ilk="argument" citdl="Object" doc="An object that contains height, width, depth, bits, and cTab properties. The height, width, and depth propertiesare integers. The bits property is a byte array. The cTab property is required only for bitmaps with a bit depth of 8 or less and is a string that represents a color value in the form &quot;#RRGGBB&quot;. "/>
				</scope>
				<variable name="vPixels" citdl="Number" doc="Read-only property; an integer that represents the height of the bitmap—that is, the number of pixels in the vertical dimension." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e99.html"/>
			</scope>
			<!--

				BitmapItem

			-->
			<scope name="BitmapItem" ilk="class" classrefs="Item" doc="A BitmapItem object refers to a bitmap in the library of a document. The BitmapItem object is a subclass of the Item object (see Item object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS19090337-2BC1-4f93-AABC-F30CFEA62378.html">
				<variable name="allowSmoothing" citdl="Boolean" doc="A Boolean value that specifies whether to allow smoothing of a bitmap (true) or not (false). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS97878519-AD12-4bf9-802F-4240F4EDE9CE.html"/>
				<variable name="compressionType" citdl="String" doc="A string that determines the type of image compression applied to the bitmap. Acceptable values are &quot;photo&quot; or &quot;lossless&quot;. If the value of bitmapItem.useImportedJPEGQuality is false, &quot;photo&quot; corresponds to JPEG with a quality from 0 to 100; if bitmapItem.useImportedJPEGQuality is true, &quot;photo&quot; corresponds to JPEG using the default document quality value. The value &quot;lossless&quot; corresponds to GIF or PNG format (see bitmapItem.useImportedJPEGQuality)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA4775F51-754E-4c47-8AAA-A3AB025E32E5.html"/>
				<scope name="exportToFile" ilk="function" returns="Boolean" signature="exportToFile(fileURI):Boolean" doc="Exports the specified item to a PNG or JPG file." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS2154BF21-FF36-4a0a-BBB5-5F25617BB614.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path and name of the exported file."/>
				</scope>
				<variable name="fileLastModifiedDate" citdl="String" doc="Read-only property; a string containing a hexadecimal number that represents the number of seconds that have elapsed between January 1, 1970 and the modification date of the original file at the time the file was imported to the library. If the file no longer exists, this value is &quot;00000000&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS3460C497-80BC-4884-8BBE-DD88342A7BAD.html"/>
				<variable name="originalCompressionType" citdl="String" doc="Read-only property; a string that specifies whether the specified item was imported as an jpeg file. Possible values for this property are “photo” (for jpeg files) and “lossless” (for uncompressed file types such as GIF and PNG)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSF4DB032F-93A5-4e89-933E-4ACB855B1CCC.html"/>
				<variable name="quality" citdl="Number" doc="An integer that specifies the quality of the bitmap. To use the default document quality, specify -1; otherwise, specify an integer from 0 to 100. Available only for JPEG compression. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS9327FDA5-B70D-4fd7-893E-EDCB18CC3F69.html"/>
				<variable name="sourceFileExists" citdl="Boolean" doc="Read-only property; a Boolean value of true if the file that was imported to the Library still exists in the location from where it was imported; false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSF88F23B4-E96D-4c26-BF2D-1903613945D0.html"/>
				<variable name="sourceFileIsCurrent" citdl="Boolean" doc="Read-only property; a Boolean value of true if the file modification date of the Library item is the same as the modification date on disk of the file that was imported ;false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS11658D99-29D4-407f-9AD8-6E467B037E5E.html"/>
				<variable name="sourceFilePath" citdl="String" doc="Read-only property; a string, expressed as a file:/// URI, that represents the path and name of the file that was imported into the Library. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS0CAF79B3-56F1-4ad5-A56D-77E5B4C14449.html"/>
				<variable name="useDeblocking" citdl="Boolean" doc="A Boolean value that specifies whether deblocking is enabled (true) or not (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA758F84C-9325-479d-AEFF-ABE5396B19A5.html"/>
				<variable name="useImportedJPEGQuality" citdl="Boolean" doc="A Boolean value that specifies whether to use the default imported JPEG quality (true) or not (false). Available only for JPEG compression. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS6415D1B8-4B48-43eb-BC07-7175D26058A7.html"/>
			</scope>
			<!--

				CompiledClipInstance

			-->
			<scope name="CompiledClipInstance" ilk="class" classrefs="Instance" doc="The CompiledClipInstance object is a subclass of the Instance object. It is essentially an instance of a movie clip that has been converted to a compiled clip library item (see Instance object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f65.html">
				<variable name="accName" citdl="String" doc="A string that is equivalent to the Name field in the Accessibility panel. Screen readers identify objects by reading the name aloud." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e78.html"/>
				<variable name="actionScript" citdl="String" doc="A string that represents the ActionScript for this instance; equivalent to symbolInstance.actionScript. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e81.html"/>
				<variable name="description" citdl="String" doc="A string that is equivalent to the Description field in the Accessibility panel. The description is read by the screen reader." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e82.html"/>
				<variable name="forceSimple" citdl="Boolean" doc="A Boolean value that enables and disables the children of the object to be accessible. This is equivalent to the inverse logic of the Make Child Objects Accessible setting in the Accessibility panel. If forceSimple is true, it is the same as the Make Child Objects Accessible option being unchecked. If forceSimple is false, it is the same as the Make Child Object Accessible option being checked." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e80.html"/>
				<variable name="shortcut" citdl="String" doc="A string that is equivalent to the Shortcut field in the Accessibility panel. The shortcut is read by the screen reader. This property is not available for dynamic text fields. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e7f.html"/>
				<variable name="silent" citdl="Boolean" doc="A Boolean value that enables or disables the accessibility of the object; equivalent to the inverse logic of Make Object Accessible setting in the Accessibility panel. That is, if silent is true, then Make Object Accessible is unchecked. If silent is false, then Make Object Accessible is checked. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e7e.html"/>
				<variable name="tabIndex" citdl="Number" doc="An integer that is equivalent to the Tab Index field in the Accessibility panel. Creates a tab order in which objects are accessed when the user presses the Tab key. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e7d.html"/>
			</scope>
			<!--

				CompilerErrors

			-->
			<scope name="CompilerErrors" ilk="class" doc="The compilerErrors object, which represents the Compiler Errors panel, is a property of the flash object (fl) and can be accessed by fl.compilerErrors (see flash object (fl))." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ffe.html">
				<scope name="clear" ilk="function" signature="clear()" doc="Clears the contents of the Compiler Errors panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e76.html"/>
				<scope name="save" ilk="function" signature="save(fileURI [, bAppendToFile [, bUseSystemEncoding]])" doc="Saves the contents of the Compiler Errors panel to a local text file. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e75.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the filename for the saved file. If fileURI already exists, and you haven’t specified a value of true for bAppendToFile, fileURI is overwritten without warning."/>
					<scope name="bAppendToFile" ilk="argument" citdl="Boolean" doc="An optional Boolean value that specifies whether the contents of the Compiler Errors panel should be appended to fileURI (true) or not (false). The default value is false."/>
					<scope name="bUseSystemEncoding" ilk="argument" citdl="Boolean" doc="An optional Boolean value that specifies whether to save the Compiler Errors panel text using the system encoding. If this value is false (the default), the Compiler Errors panel text is saved using UTF-8 encoding, with Byte Order Mark characters at the beginning of the text. The default value is false."/>
				</scope>
			</scope>
			<!--

				ComponentInstance

			-->
			<scope name="ComponentInstance" ilk="class" classrefs="SymbolInstance" doc="TheComponentInstance object is a subclass of the SymbolInstance object and represents a component in a frame (see SymbolInstance object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f63.html">
				<variable name="parameters" citdl="Array" doc="Read-only property; an array of ActionScript 2.0 properties that are accessible from the component Property inspector. See Parameter object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f60.html"/>
			</scope>
			<!--

				ComponentsPanel

			-->
			<scope name="ComponentsPanel" ilk="class" doc="The componentsPanel object, which represents the Components panel, is a property of the flash object (fl) and can be accessed by fl.componentsPanel (see flash object (fl))." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ffc.html">
				<scope name="addItemToDocument" ilk="function" signature="addItemToDocument(position, categoryName, componentName)" doc="Adds the specified component to the document at the specified position. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e69.html">
					<scope name="position" ilk="argument" citdl="Object" doc="A point (for example, {x:0, y:100}) that specifies the location at which to add the component. Specify position relative to the center point of the component—not the component’s registration point (also origin point or zero point). "/>
					<scope name="categoryName" ilk="argument" citdl="String" doc="A string that specifies the name of the component category (for example, &quot;Data&quot;). The valid category names are listed in the Components panel."/>
					<scope name="componentName" ilk="argument" citdl="String" doc="A string that specifies the name of the component in the specified category (for example, &quot;WebServiceConnector&quot;). The valid component names are listed in the Components panel."/>
				</scope>
				<scope name="reload" ilk="function" returns="Boolean" signature="reload():Boolean" doc="Refreshes the Components panel’s list of components." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e68.html"/>
			</scope>
			<!--

				Contour

			-->
			<scope name="Contour" ilk="class" doc="A Contour object represents a closed path of half edges on the boundary of a shape. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f53.html">
				<variable name="fill" citdl="Fill" doc="A Fill object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSBF153568-7BC5-4165-8515-312F5FB2E722.html"/>
				<scope name="getHalfEdge" ilk="function" returns="HalfEdge" doc="Returns a HalfEdge object on the contour of the selection. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e63.html"/>
				<variable name="interior" citdl="value" doc="Read-only property; the value is true if the contour encloses an area; false otherwise." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e61.html"/>
				<variable name="orientation" citdl="Number" doc="Read-only property; an integer indicating the orientation of the contour. The value of the integer is -1 if the orientation is counterclockwise, 1 if it is clockwise, and 0 if it is a contour with no area." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e62.html"/>
			</scope>
			<!--

				Document

			-->
			<scope name="Document" ilk="class" doc="The Document object represents the Stage. That is, only FLA files are considered documents. To return the Document object for the current document, use fl.getDocumentDOM()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ffa.html">
				<variable name="accName" citdl="String" doc="A string that is equivalent to the Name field in the Accessibility panel. Screen readers identify objects by reading the name aloud. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e60.html"/>
				<scope name="addDataToDocument" ilk="function" signature="addDataToDocument(name, type, data)" doc="Stores specified data with a document. Data is written to the FLA file and is available to JavaScript when the file reopens. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e47.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to add."/>
					<scope name="type" ilk="argument" citdl="String" doc="A string that defines the type of data to add. Acceptable values are &quot;integer&quot;, &quot;integerArray&quot;, &quot;double&quot;, &quot;doubleArray&quot;, &quot;string&quot;, and &quot;byteArray&quot;. "/>
					<scope name="data" ilk="argument" citdl="value" doc="The value to add. Valid types depend on the type parameter."/>
				</scope>
				<scope name="addDataToSelection" ilk="function" signature="addDataToSelection(name, type, data)" doc="Stores specified data with the selected object(s). Data is written to the FLA file and is available to JavaScript when the file reopens. Only symbols and bitmaps support persistent data. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e46.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the persistent data."/>
					<scope name="type" ilk="argument" citdl="value" doc="Defines the type of data. Acceptable values are &quot;integer&quot;, &quot;integerArray&quot;, &quot;double&quot;, &quot;doubleArray&quot;, &quot;string&quot;, and &quot;byteArray&quot;. "/>
					<scope name="data" ilk="argument" citdl="value" doc="The value to add. Valid types depend on the type parameter."/>
				</scope>
				<scope name="addFilter" ilk="function" signature="addFilter(filterName)" doc="Applies a filter to the selected objects and places the filter at the end of the Filters list." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e45.html">
					<scope name="filterName" ilk="argument" citdl="String" doc="A string specifying the filter to be added to the Filters list and enabled for the selected object(s). Acceptable values are &quot;adjustColorFilter&quot;, &quot;bevelFilter&quot;, &quot;blurFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientBevelFilter&quot;, and &quot;gradientGlowFilter&quot;. "/>
				</scope>
				<scope name="addItem" ilk="function" returns="Boolean" signature="addItem(position, item):Boolean" doc="Adds an item from any open document or library to the specified Document object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e44.html">
					<scope name="position" ilk="argument" citdl="Object" doc="A point that specifies the x and y coordinates of the location at which to add the item. It uses the center of a symbol or the upper left corner of a bitmap or video. "/>
					<scope name="item" ilk="argument" citdl="Item" doc="An Item object that specifies the item to add and the library from which to add it (see Item object)."/>
				</scope>
				<scope name="addNewLine" ilk="function" signature="addNewLine(startPoint, endpoint)" doc="Adds a new path between two points. The method uses the document’s current stroke attributes and adds the path on the current frame and current layer. This method works in the same way as clicking on the line tool and drawing a line." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e43.html">
					<scope name="startpoint" ilk="argument" citdl="Number" doc="A pair of floating-point numbers that specify the x and y coordinates where the line starts. "/>
					<scope name="endpoint" ilk="argument" citdl="Number" doc="A pair of floating-point numbers that specify the x and y coordinates where the line ends. "/>
				</scope>
				<scope name="addNewOval" ilk="function" signature="addNewOval(boundingRectangle [, bSuppressFill [, bSuppressStroke ]])" doc="Adds a new Oval object in the specified bounding rectangle. This method performs the same operation as the Oval tool. The method uses the document’s current default stroke and fill attributes and adds the oval on the current frame and layer. If both bSuppressFill and bSuppressStroke are set to true, the method has no effect. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e42.html">
					<scope name="boundingRectangle" ilk="argument" citdl="Rectangle" doc="A rectangle that specifies the bounds of the oval to be added. For information on the format of boundingRectangle, see document.addNewRectangle(). "/>
					<scope name="bSuppressFill" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the shape without a fill. The default value is false. This parameter is optional."/>
					<scope name="bSuppressStroke" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the shape without a stroke. The default value is false. This parameter is optional."/>
				</scope>
				<scope name="addNewPrimitiveOval" ilk="function" signature="addNewPrimitiveOval( boundingRectangle [, bSpupressFill [, bSuppressStroke ]] )" doc="Adds a new oval primitive fitting into the specified bounds. This method performs the same operation as the Oval Primitive tool. The oval primitive uses the document's current default stroke and fill attributes and is added on the current frame and layer. If both bSuppressFill and bSuppressStroke are set to true, the method has no effect." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSCF572308-4FC6-4809-BF4F-1145EC7EEA37.html">
					<scope name="boundingRectangle" ilk="argument" citdl="Rectangle" doc="A rectangle that specifies the bounds within which the new oval primitive is added. For information on the format of boundingRectangle, see document.addNewRectangle()."/>
					<scope name="bSuppressFill" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the oval without a fill. The default value is false. This parameter is optional."/>
					<scope name="bSuppressStroke" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the oval without a stroke. The default value is false. This parameter is optional."/>
				</scope>
				<scope name="addNewPrimitiveRectangle" ilk="function" signature="addNewPrimitiveRectangle( boundingRectangle, roundness [, bSuppressFill [, bSuppressStroke ]] )" doc="Adds a new rectangle primitive fitting into the specified bounds. This method performs the same operation as the Rectangle Primitive tool. The rectangle primitive uses the document's current default stroke and fill attributes and is added on the current frame and layer. If both bSuppressFill and bSuppressStroke are set to true, the method has no effect." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS89493EAA-1C9F-471d-A011-14511DA42162.html">
					<scope name="rect" ilk="argument" citdl="Rectangle" doc="A rectangle that specifies the bounds within which the new rectangle primitive is added. For information on the format of boundingRectangle, see document.addNewRectangle()."/>
					<scope name="roundness" ilk="argument" citdl="Number" doc=" An integer between 0 and 999 that represents the number of points used to specify how much the corners should be rounded."/>
					<scope name="bSuppressFill" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the rectangle without a fill. The default value is false. This parameter is optional."/>
					<scope name="bSuppressStroke" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the rectangle without a stroke. The default value is false. This parameter is optional."/>
				</scope>
				<scope name="addNewPublishProfile" ilk="function" returns="Number" signature="addNewPublishProfile([profileName]):Number" doc="Adds a new publish profile and makes it the current one. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e41.html">
					<scope name="profileName" ilk="argument" citdl="Parameter" doc="The unique name of the new profile. If you do not specify a name, a default name is provided. This parameter is optional."/>
				</scope>
				<scope name="addNewRectangle" ilk="function" signature="addNewRectangle(boundingRectangle, roundness [, bSuppressFill [, bSuppressStroke]])" doc="Adds a new rectangle or rounded rectangle, fitting it into the specified bounds. This method performs the same operation as the Rectangle tool. The method uses the document’s current default stroke and fill attributes and adds the rectangle on the current frame and layer. If both bSuppressFill and bSuppressStroke are set to true, the method has no effect." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e40.html">
					<scope name="boundingRectangle" ilk="argument" citdl="Rectangle" doc="A rectangle that specifies the bounds within which the new rectangle is added, in the format {left:value1,top:value2,right:value3,bottom:value4}. The left and top values specify the location of the upper left corner (e.g., left:0,top:0 represents the upper left corner of the Stage) and the right and bottom values specify the location of the lower-right corner. Therefore, the width of the rectangle is the difference in value between left and right, and the height of the rectangle is the difference in value between top and bottom. In other words, the rectangle bounds do not all correspond to the values shown in the Property inspector. The left and top values correspond to the X and Y values in the Property inspector, respectively. However, the right and bottom values don’t correspond to the W and H values in the Property inspector. For example, consider a rectangle with the following bounds: {left:10,top:10,right:50,bottom:100} This rectangle would display the following values in the Property inspector: X = 10, Y = 10, W = 40, H = 90 "/>
					<scope name="roundness" ilk="argument" citdl="Number" doc="An integer value from 0 to 999 that specifies the roundness to use for the corners. The value is specified as number of points. The greater the value, the greater the roundness."/>
					<scope name="bSuppressFill" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the shape without a fill. The default value is false. This parameter is optional."/>
					<scope name="bSuppressStroke" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to create the rectangle without a stroke. The default value is false. This parameter is optional."/>
				</scope>
				<scope name="addNewScene" ilk="function" returns="Boolean" signature="addNewScene([name]):Boolean" doc="Adds a new scene (Timeline object) as the next scene after the currently selected scene and makes the new scene the currently selected scene. If the specified scene name already exists, the scene is not added and the method returns an error. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e3f.html">
					<scope name="name" ilk="argument" doc="Specifies the name of the scene. If you do not specify a name, a new scene name is generated. "/>
				</scope>
				<scope name="addNewText" ilk="function" signature="addNewText(boundingRectangle [, text ])" doc="Inserts a new text field and optionally places text into the field. If you omit the text parameter, you can call document.setTextString() to populate the text field. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa4.html">
					<scope name="boundingRectangle" ilk="argument" citdl="Text" doc="Specifies the size and location of the text field. For information on the format of boundingRectangle, see document.addNewRectangle()."/>
					<scope name="text" ilk="argument" citdl="String" doc="An optional string that specifies the text to place in the field. If you omit this parameter, the selection in the Tools panel switches to the Text tool. Therefore, if you don’t want the selected tool to change, pass a value for text."/>
				</scope>
				<scope name="align" ilk="function" signature="align(alignmode [, bUseDocumentBounds])" doc="Aligns the selection. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e3c.html">
					<scope name="alignmode" ilk="argument" citdl="String" doc="A string that specifies how to align the selection. Acceptable values are &quot;left&quot;, &quot;right&quot;, &quot;top&quot;, &quot;bottom&quot;, &quot;vertical center&quot;, and &quot;horizontal center&quot;. "/>
					<scope name="bUseDocumentBounds" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to align to the bounds of the document. Otherwise, the method uses the bounds of the selected objects. The default is false. This parameter is optional."/>
				</scope>
				<scope name="allowScreens" ilk="function" returns="Boolean" signature="allowScreens():Boolean" doc="Use before using the document.screenOutline property. If this method returns the value true, you can safely access document.screenOutline; Flash displays an error if you access document.screenOutline in a document without screens. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e3b.html"/>
				<scope name="arrange" ilk="function" signature="arrange(arrangeMode)" doc="Arranges the selection on the Stage. This method applies only to non-shape objects. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e39.html">
					<scope name="arrangeMode" ilk="argument" citdl="value" doc="Specifies the direction in which to move the selection. Acceptable values are &quot;back&quot;, &quot;backward&quot;, &quot;forward&quot;, and &quot;front&quot;. It provides the same capabilities as these options provide on the Modify &amp;gt; Arrange menu."/>
				</scope>
				<variable name="as3AutoDeclare" citdl="Boolean" doc="A Boolean value that describes whether the instances placed on the Stage are automatically added to user-defined timeline classes. The default value is true." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd7.html"/>
				<variable name="as3Dialect" citdl="String" doc="A string that describes the ActionScript 3.0 “dialect” being used in the specified document. The default value is &quot;AS3&quot;. If you wish to allow prototype classes, as permitted in earlier ECMAScript specifications, set this value to &quot;ES&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd6.html"/>
				<variable name="as3ExportFrame" citdl="Number" doc="An integer that specifies in which frame to export ActionScript 3.0 classes. By default, classes are exported in Frame 1." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd5.html"/>
				<variable name="as3StrictMode" citdl="Boolean" doc="A Boolean value that specifies whether the ActionScript 3.0 compiler should compile with the Strict Mode option turned on (true) or off (false). Strict Mode causes warnings to be reported as errors, which means that compilation will not succeed if those errors exist. The default value is true." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd4.html"/>
				<variable name="as3WarningsMode" citdl="Boolean" doc="A Boolean value that specifies whether the ActionScript 3.0 compiler should compile with the Warnings Mode option turned on (true) or off (false). Warnings Mode causes extra warnings to be reported that are useful for discovering incompatibilities when updating ActionScript 2.0 code to ActionScript 3.0. The default value is true." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd3.html"/>
				<variable name="asVersion" citdl="Number" doc="An integer that specifies which version of ActionScript is being used in the specified document. Acceptable values are 1, 2, and 3. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd2.html"/>
				<variable name="autoLabel" citdl="Boolean" doc="A Boolean value that is equivalent to the Auto Label check box in the Accessibility panel. You can use this property to tell Flash to automatically label objects on the Stage with the text associated with them." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e5b.html"/>
				<variable name="backgroundColor" doc="The color of the background, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e5a.html"/>
				<scope name="breakApart" ilk="function" signature="breakApart()" doc="Performs a break-apart operation on the current selection. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e38.html"/>
				<scope name="canEditSymbol" ilk="function" returns="Boolean" signature="canEditSymbol():Boolean" doc="Indicates whether the Edit Symbols menu and functionality are enabled. This is not related to whether the selection can be edited. This method should not be used to test whether fl.getDocumentDOM().enterEditMode() is allowed." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e37.html"/>
				<scope name="canRevert" ilk="function" returns="Boolean" signature="canRevert():Boolean" doc="Determines whether you can use the document.revert() or fl.revertDocument() method successfully." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e36.html"/>
				<scope name="canSaveAVersion" ilk="function" returns="Boolean" signature="canSaveAVersion():Boolean" doc="Determines whether a version of the specified document can be saved to the Version Cue server. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd1.html"/>
				<scope name="canTestMovie" ilk="function" returns="Boolean" signature="canTestMovie():Boolean" doc="Determines whether you can use the document.testMovie() method successfully. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e32.html"/>
				<scope name="canTestScene" ilk="function" returns="Boolean" signature="canTestScene():Boolean" doc="Determines whether you can use the document.testScene() method successfully. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e30.html"/>
				<scope name="changeFilterOrder" ilk="function" signature="changeFilterOrder(oldIndex, newIndex)" doc="Changes the index of the filter in the Filters list. Any filters above or below newIndex are shifted up or down accordingly. For example, using the filters shown below, if you issue the command fl.getDocumentDOM().changeFilterOrder(3, 0), the filters are rearranged as follows:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e2e.html">
					<scope name="oldIndex" ilk="argument" citdl="Number" doc="An integer that represents the current zero-based index position of the filter you want to reposition in the Filters list. "/>
					<scope name="newIndex" ilk="argument" citdl="Number" doc="An integer that represents the new index position of the filter in the list."/>
				</scope>
				<scope name="clipCopy" ilk="function" signature="clipCopy()" doc="Copies the current selection from the document to the Clipboard. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e2d.html"/>
				<scope name="clipCut" ilk="function" signature="clipCut()" doc="Cuts the current selection from the document and writes it to the Clipboard." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e2c.html"/>
				<scope name="clipPaste" ilk="function" signature="clipPaste([bInPlace])" doc="Pastes the contents of the Clipboard into the document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e2b.html">
					<scope name="bInPlace" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, causes the method to perform a paste-in-place operation. The default value is false, which causes the method to perform a paste operation to the center of the document. This parameter is optional."/>
				</scope>
				<scope name="close" ilk="function" signature="close([bPromptToSaveChanges])" doc="Closes the specified document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e2a.html">
					<scope name="bPromptToSaveChanges" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, causes the method to prompt the user with a dialog box if there are unsaved changes in the document. If bPromptToSaveChanges is set to false, the user is not prompted to save any changed documents. The default value is true. This parameter is optional."/>
				</scope>
				<scope name="convertLinesToFills" ilk="function" signature="convertLinesToFills()" doc="Converts lines to fills on the selected objects. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e29.html"/>
				<scope name="convertToSymbol" ilk="function" returns="Object" signature="convertToSymbol(type, name, registrationPoint):Object" doc="Converts the selected Stage item(s) to a new symbol. For information on defining linkage and shared asset properties for a symbol, see Item object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e28.html">
					<scope name="type" ilk="argument" citdl="String" doc="A string that specifies the type of symbol to create. Acceptable values are &quot;movie clip&quot;, &quot;button&quot;, and &quot;graphic&quot;. "/>
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name for the new symbol, which must be unique. You can submit an empty string to have this method create a unique symbol name for you."/>
					<scope name="registration point" ilk="argument" citdl="Object" doc="Specifies the point that represents the 0,0 location for the symbol. Acceptable values are: &quot;top left&quot;, &quot;top center&quot;, &quot;top right&quot;, &quot;center left&quot;, &quot;center&quot;, &quot;center right&quot;, &quot;bottom left&quot;, &quot;bottom center&quot;, and &quot;bottom right&quot;."/>
				</scope>
				<scope name="crop" ilk="function" returns="Boolean" signature="crop():Boolean" doc="Uses the top selected drawing object to crop all selected drawing objects underneath it. This method returns false if there are no drawing objects selected or if any of the selected items are not drawing objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e27.html"/>
				<variable name="currentPublishProfile" citdl="String" doc="A string that specifies the name of the active publish profile for the specified document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e59.html"/>
				<variable name="currentTimeline" citdl="Number" doc="An integer that specifies the index of the active timeline. You can set the active timeline by changing the value of this property; the effect is almost equivalent to calling document.editScene(). The only difference is that you don’t get an error message if the index of the timeline is not valid; the property is simply not set, which causes silent failure." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e58.html"/>
				<scope name="deleteEnvelope" ilk="function" returns="Boolean" signature="deleteEnvelope():Boolean" doc="Deletes the envelope (bounding box that contains one or more objects) from the selected objects. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e26.html"/>
				<scope name="deletePublishProfile" ilk="function" returns="Number" signature="deletePublishProfile():Number" doc="Deletes the currently active profile, if there is more than one. There must be at least one profile left. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e25.html"/>
				<scope name="deleteScene" ilk="function" returns="Boolean" signature="deleteScene():Boolean" doc="Deletes the current scene (Timeline object) and, if the deleted scene was not the last one, sets the next scene as the current Timeline object. If the deleted scene was the last one, it sets the first object as the current Timeline object. If only one Timeline object (scene) exists, it returns the value false. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e24.html"/>
				<scope name="deleteSelection" ilk="function" signature="deleteSelection()" doc="Deletes the current selection on the Stage. Displays an error message if there is no selection." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e22.html"/>
				<variable name="description" citdl="String" doc="A string that is equivalent to the Description field in the Accessibility panel. The description is read by the screen reader. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e57.html"/>
				<scope name="disableAllFilters" ilk="function" signature="disableAllFilters()" doc="Disables all filters on the selected objects. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e21.html"/>
				<scope name="disableFilter" ilk="function" signature="disableFilter(filterIndex)" doc="Disables the specified filter in the Filters list. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e20.html">
					<scope name="filterIndex" ilk="argument" citdl="Number" doc="An integer representing the zero-based index of the filter in the Filters list."/>
				</scope>
				<scope name="disableOtherFilters" ilk="function" signature="disableOtherFilters(enabledFilterIndex)" doc="Disables all filters except the one at the specified position in the Filters list. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e1f.html">
					<scope name="enabledFilterIndex" ilk="argument" citdl="Number" doc="An integer representing the zero-based index of the filter that should remain enabled after other filters are disabled. "/>
				</scope>
				<scope name="distribute" ilk="function" signature="distribute(distributemode [, bUseDocumentBounds])" doc="Distributes the selection. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e1e.html">
					<scope name="distributemode" ilk="argument" citdl="String" doc="A string that specifies where to distribute the selected objects. Acceptable values are &quot;left edge&quot;, &quot;horizontal center&quot;, &quot;right edge&quot;, &quot;top edge&quot;, &quot;vertical center&quot;, and &quot;bottom edge&quot;."/>
					<scope name="bUseDocumentBounds" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, distributes the selected objects using the bounds of the document. Otherwise, the method uses the bounds of the selected objects. The default is false. "/>
				</scope>
				<scope name="distributeToLayers" ilk="function" signature="distributeToLayers()" doc="Performs a distribute-to-layers operation on the current selection—equivalent to selecting Distribute to Layers. This method displays an error if there is no selection." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e1d.html"/>
				<variable name="docClass" citdl="String" doc="A string that specifies the top-level ActionScript 3.0 class associated with the document. If the document isn’t configured to use ActionScript 3.0, this property is ignored. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd0.html"/>
				<scope name="documentHasData" ilk="function" returns="Boolean" signature="documentHasData(name):Boolean" doc="Checks the document for persistent data with the specified name. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e1b.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to check. "/>
				</scope>
				<scope name="duplicatePublishProfile" ilk="function" returns="Number" signature="duplicatePublishProfile([profileName]):Number" doc="Duplicates the currently active profile and gives the duplicate version focus." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e1a.html">
					<scope name="profileName" ilk="argument" citdl="String" doc="A string that specifies the unique name of the duplicated profile. If you do not specify a name, the method uses the default name. This parameter is optional."/>
				</scope>
				<scope name="duplicateScene" ilk="function" returns="Boolean" signature="duplicateScene():Boolean" doc="Makes a copy of the currently selected scene, giving the new scene a unique name and making it the current scene. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e19.html"/>
				<scope name="duplicateSelection" ilk="function" signature="duplicateSelection()" doc="Duplicates the selection on the Stage." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e18.html"/>
				<scope name="editScene" ilk="function" signature="editScene(index)" doc="Makes the specified scene the currently selected scene for editing. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e17.html">
					<scope name="index" ilk="argument" citdl="Number" doc="A zero-based integer that specifies which scene to edit. "/>
				</scope>
				<scope name="enableAllFilters" ilk="function" signature="enableAllFilters()" doc="Enables all the filters on the Filters list for the selected object(s)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e16.html"/>
				<scope name="enableFilter" ilk="function" signature="enableFilter(filterIndex)" doc="Enables the specified filter for the selected object(s)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e15.html">
					<scope name="filterIndex" ilk="argument" citdl="Number" doc="An integer specifying the zero-based index of the filter in the Filters list to enable."/>
				</scope>
				<scope name="enterEditMode" ilk="function" signature="enterEditMode([editMode])" doc="Switches the authoring tool into the editing mode specified by the parameter. If no parameter is specified, the method defaults to symbol-editing mode, which has the same result as right-clicking the symbol to invoke the context menu and selecting Edit. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e14.html">
					<scope name="editMode" ilk="argument" citdl="String" doc="A string that specifies the editing mode. Acceptable values are &quot;inPlace&quot; or &quot;newWindow&quot;. If no parameter is specified, the default is symbol-editing mode. This parameter is optional."/>
				</scope>
				<scope name="exitEditMode" ilk="function" signature="exitEditMode()" doc="Exits from symbol-editing mode and returns focus to the next level up from the editing mode. For example, if you are editing a symbol inside another symbol, this method takes you up a level from the symbol you are editing, into the parent symbol. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e13.html"/>
				<scope name="exportPNG" ilk="function" returns="Boolean" signature="exportPNG([fileURI [, bCurrentPNGSettings [, bCurrentFrame]]]):Boolean" doc="Exports the document as one or more PNG files. If fileURI is specified and the file already exists, it is overwritten without warning." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e12.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the filename for the exported file. If fileURI is an empty string or is not specified, Flash displays the Export Movie dialog box. "/>
					<scope name="bCurrentPNGSettings" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to use the current PNG publish settings (true) or to display the Export PNG dialog box (false). This parameter is optional. The default value is false."/>
					<scope name="bCurrentFrame" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to export only the current frame (true) or to export all frames, with each frame as a separate PNG file (false). This parameter is optional. The default value is false. "/>
				</scope>
				<scope name="exportPublishProfile" ilk="function" signature="exportPublishProfile(fileURI)" doc="Exports the currently active profile to an XML file. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e11.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path of the XML file to which the profile is exported. "/>
				</scope>
				<scope name="exportPublishProfileString" ilk="function" returns="String" signature="exportPublishProfileString( [profileName] ):String" doc="Method: returns a string that specifies, in XML format, the specified profile. If you don’t pass a value for profileName, the current profile is exported. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS014AAEC0-0522-4832-99CD-72FCF71AA794.html">
					<scope name="profileName" ilk="argument" citdl="String" doc="A string that specifies the name of the profile to export to an XML string. This parameter is optional."/>
				</scope>
				<scope name="exportSWF" ilk="function" signature="exportSWF([fileURI [, bCurrentSettings]])" doc="Exports the document in the Flash SWF format. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e10.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the name of the exported file. If fileURI is empty or not specified, Flash displays the Export Movie dialog box. This parameter is optional."/>
					<scope name="bCurrentSettings" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, causes Flash to use current SWF publish settings. Otherwise, Flash displays the Export Flash Player dialog box. The default is false. This parameter is optional."/>
				</scope>
				<variable name="externalLibraryPath" citdl="String" doc="A string that contains a list of items in the document’s ActionScript 3.0 External library path, which specifies the location of SWC files used as runtime shared libraries. Items in the string are delimited by semi-colons. In the authoring tool, the items are specified by choosing File &amp;gt; Publish Settings and then choosing ActionScript 3.0 Script Settings on the Flash tab." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSFE6C8B4D-A287-4f77-A002-38516C28C318.html"/>
				<variable name="forceSimple" citdl="Boolean" doc="A Boolean value that specifies whether the children of the specified object are accessible. This is equivalent to the inverse logic of the Make Child Objects Accessible setting in the Accessibility panel. That is, if forceSimple is true, it is the same as the Make Child Object Accessible option being unchecked. If forceSimple is false, it is the same as the Make Child Object Accessible option being checked. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e55.html"/>
				<variable name="frameRate" citdl="Fl" doc="A float value that specifies the number of frames displayed per second when the SWF file plays; the default is 12. Setting this property is the same as setting the default frame rate in the Document Properties dialog box (Modify &amp;gt; Document) in the FLA file. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e54.html"/>
				<scope name="getAlignToDocument" ilk="function" returns="Boolean" signature="getAlignToDocument():Boolean" doc="Identical to retrieving the value of the To Stage button in the Align panel. Gets the preference that can be used for document.align(), document.distribute(), document.match(), and document.space() methods on the document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e0f.html"/>
				<scope name="getBlendMode" ilk="function" returns="String" signature="getBlendMode():String" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e0e.html"/>
				<scope name="getCustomFill" ilk="function" returns="Fill" signature="getCustomFill([objectToFill]):Fill" doc="Retrieves the fill object of the selected shape or, if specified, of the Tools panel and Property inspector. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e0d.html">
					<scope name="objectToFill" ilk="argument" citdl="String" doc="A string that specifies the location of the fill object. The following values are valid:	"/>
				</scope>
				<scope name="getCustomStroke" ilk="function" returns="Stroke" signature="getCustomStroke([locationOfStroke]):Stroke" doc="Returns the stroke object of the selected shape or, if specified, of the Tools panel and Property inspector. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e0c.html">
					<scope name="locationOfStroke" ilk="argument" citdl="String" doc="A string that specifies the location of the stroke object. The following values are valid:	"/>
				</scope>
				<scope name="getDataFromDocument" ilk="function" signature="getDataFromDocument(name)" doc="Retrieves the value of the specified data. The type returned depends on the type of data that was stored. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e0b.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to return."/>
				</scope>
				<scope name="getElementProperty" ilk="function" returns="value" signature="getElementProperty(propertyName):value" doc="Gets the specified Element property for the current selection. For a list of acceptable values, see the Property summary table for the Element object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e0a.html">
					<scope name="propertyName" ilk="argument" citdl="String" doc=" A string that specifies the name of the Element property for which to retrieve the value."/>
				</scope>
				<scope name="getElementTextAttr" ilk="function" returns="Text" signature="getElementTextAttr(attrName [, startIndex [, endIndex]]):Text" doc="Gets a specific TextAttrs property of the selected Text objects. Selected objects that are not text fields are ignored. For a list of property names and expected values, see the Property summary table for the TextAttrs object. See also document.setElementTextAttr()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e09.html">
					<scope name="attrName" ilk="argument" citdl="String" doc="A string that specifies the name of the TextAttrs property to be returned. For a list of property names and expected values, see the Property summary table for the TextAttrs object."/>
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that specifies the index of first character, with 0 (zero) specifying the first position. This parameter is optional."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the index of last character. This parameter is optional."/>
				</scope>
				<scope name="getFilters" ilk="function" returns="Array" signature="getFilters():Array" doc="Returns an array that contains the list of filters applied to the currently selected object(s). If multiple objects are selected and they don’t have identical filters, this method returns the list of filters applied to the first selected object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e08.html"/>
				<scope name="getMetadata" ilk="function" returns="String" signature="getMetadata():String" doc="Returns a string containing the XML metadata associated with the document, or an empty string if there is no metadata." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e07.html"/>
				<scope name="getMobileSettings" ilk="function" returns="String" signature="getMobileSettings():String" doc="Returns the mobile XML settings for the document." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fcf.html"/>
				<scope name="getPlayerVersion" ilk="function" returns="String" signature="getPlayerVersion():String" doc="Returns a string that represents the targeted player version for the specified document. For a list of values that this method can return, see document.setPlayerVersion()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fce.html"/>
				<scope name="getSelectionRect" ilk="function" returns="Rectangle" signature="getSelectionRect():Rectangle" doc="Gets the bounding rectangle of the current selection. If a selection is non-rectangular, the smallest rectangle encompassing the entire selection is returned. The rectangle is based on the document space or, when in edit mode, the registration point (also origin point or zero point) of the symbol being edited." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e04.html"/>
				<scope name="getTextString" ilk="function" returns="String" signature="getTextString([startIndex [, endIndex]]):String" doc="Gets the currently selected text. If the optional parameters are not passed, the current text selection is used. If text is not currently opened for editing, the whole text string is returned. If only startIndex is passed, the string starting at that index and ending at the end of the field is returned. If startIndex and endIndex are passed, the string starting from startIndex up to, but not including, endIndex is returned. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e03.html">
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that is an index of first character to get. This parameter is optional. "/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that is an index of last character to get. This parameter is optional."/>
				</scope>
				<scope name="getTimeline" ilk="function" returns="Timeline" signature="getTimeline():Timeline" doc="Retrieves the current Timeline object in the document. The current timeline can be the current scene, the current symbol being edited, or the current screen. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e02.html"/>
				<scope name="getTransformationPoint" ilk="function" returns="Object" signature="getTransformationPoint():Object" doc="Gets the location of the transformation point of the current selection. You can use the transformation point for commutations such as rotate and skew. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e00.html"/>
				<scope name="group" ilk="function" signature="group()" doc="Converts the current selection to a group. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dff.html"/>
				<variable name="height" citdl="Number" doc="An integer that specifies the height of the document (Stage) in pixels. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e53.html"/>
				<variable name="id" citdl="Number" doc="Read-only property; a unique integer (assigned automatically) that identifies a document during a Flash session. Use this property in conjunction with fl.findDocumentDOM() to specify a particular document for an action." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fcd.html"/>
				<scope name="importFile" ilk="function" signature="importFile(fileURI [, importToLibrary])" doc="Imports a file into a document. This method performs the same operation as the Import To Library or Import To Stage menu command. To import a publish profile, use document.importPublishProfile(). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dfd.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path of the file to import."/>
					<scope name="importToLibrary" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to import the file only into the document’s library (true) or to also place a copy on the Stage (false). The default value is false."/>
				</scope>
				<scope name="importPublishProfile" ilk="function" returns="Number" signature="importPublishProfile( fileURI ):Number" doc="Imports a profile from a file." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dfe.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path of the XML file defining the profile to import. "/>
				</scope>
				<scope name="importPublishProfileString" ilk="function" returns="Boolean" signature="importPublishProfileString(xmlString):Boolean" doc="Method: imports an XML string that represents a publish profile and sets it as the current profile. To generate an XML string to import, use document.exportPublishProfileString() before using this method. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB3F3672A-EB9D-4087-9485-657EFFF8FA72.html">
					<scope name="xmlString" ilk="argument" citdl="String" doc="A string that contains the XML data to be imported as the current profile."/>
				</scope>
				<scope name="importSWF" ilk="function" signature="importSWF(fileURI)" doc="Imports a SWF file into the document. This method performs the same operation as using the Import menu command to specify a SWF file. In Flash 8 and later, you can also use document.importFile() to import a SWF file (as well as other types of files)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dfc.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the file for the SWF file to import."/>
				</scope>
				<scope name="intersect" ilk="function" returns="Boolean" signature="intersect():Boolean" doc="Creates an intersection drawing object from all selected drawing objects. This method returns false if there are no drawing objects selected, or if any of the selected items are not drawing objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dfb.html"/>
				<variable name="library" citdl="Library" doc="Read-only property; the library object for a document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f95.html"/>
				<variable name="libraryPath" citdl="String" doc="A string that contains a list of items in the document’s ActionScript 3.0 Library path, which specifies the location of SWC files or folders containing SWC files. Items in the string are delimited by semi-colons. In the authoring tool, the items are specified by choosing File &amp;gt; Publish Settings and then choosing ActionScript 3.0 Script Settings on the Flash tab." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA93A3E72-B9A1-44a5-9C99-08D88DB7BF0C.html"/>
				<variable name="livePreview" citdl="Boolean" doc="A Boolean value that specifies whether Live Preview is enabled. If set to true, components appear on the Stage as they will appear in the published Flash content, including their approximate size. If set to false, components appear only as outlines. The default value is true." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e4f.html"/>
				<scope name="match" ilk="function" signature="match(bWidth, bHeight [, bUseDocumentBounds])" doc="Makes the size of the selected objects the same. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dfa.html">
					<scope name="bWidth" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, causes the method to make the widths of the selected items the same. "/>
					<scope name="bHeight" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, causes the method to make the heights of the selected items the same. "/>
					<scope name="bUseDocumentBounds" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, causes the method to match the size of the objects to the bounds of the document. Otherwise, the method uses the bounds of the largest object. The default is false. This parameter is optional."/>
				</scope>
				<scope name="mouseClick" ilk="function" signature="mouseClick(position, bToggleSel, bShiftSel)" doc="Performs a mouse click from the Selection tool." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df9.html">
					<scope name="position" ilk="argument" citdl="Number" doc="A pair of floating-point values that specify the x and y coordinates of the click in pixels. "/>
					<scope name="bToggleSel" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies the state of the Shift key: true for pressed; false for not pressed. "/>
					<scope name="bShiftSel" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies the state of the application preference Shift select: true for on; false for off."/>
				</scope>
				<scope name="mouseDblClk" ilk="function" signature="mouseDblClk(position, bAltDown, bShiftDown, bShiftSelect)" doc="Performs a double mouse click from the Selection tool. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df8.html">
					<scope name="position" ilk="argument" citdl="Number" doc="A pair of floating-point values that specify the x and y coordinates of the click in pixels. "/>
					<scope name="bAltdown" ilk="argument" citdl="Boolean" doc="A Boolean value that records whether the Alt key is down at the time of the event: true for pressed; false for not pressed. "/>
					<scope name="bShiftDown" ilk="argument" citdl="Boolean" doc="A Boolean value that records whether the Shift key was down when the event occurred: true for pressed; false for not pressed. "/>
					<scope name="bShiftSelect" ilk="argument" citdl="Boolean" doc="A Boolean value that indicates the state of the application preference Shift select: true for on; false for off. "/>
				</scope>
				<scope name="moveSelectedBezierPointsBy" ilk="function" signature="moveSelectedBezierPointsBy(delta)" doc="If the selection contains at least one path with at least one Bézier point selected, moves all selected Bézier points on all selected paths by the specified amount. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df7.html">
					<scope name="delta" ilk="argument" citdl="Number" doc="A pair of floating-point values that specify the x and y coordinates in pixels by which the selected Bézier points are moved. For example, passing ({x:1,y:2}) specifies a location that is to the right by one pixel and down by two pixels from the current location. "/>
				</scope>
				<scope name="moveSelectionBy" ilk="function" signature="moveSelectionBy(distanceToMove)" doc="Moves selected objects by a specified distance. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df6.html">
					<scope name="distanceToMove" ilk="argument" citdl="Number" doc="A pair of floating-point values that specify the x and y coordinate values by which the method moves the selection. For example, passing ({x:1,y:2}) specifies a location one pixel to the right and two pixels down from the current location. "/>
				</scope>
				<variable name="name" citdl="String" doc="Read-only property; a string that represents the name of a document (FLA file)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f97.html"/>
				<scope name="optimizeCurves" ilk="function" signature="optimizeCurves(smoothing, bUseMultiplePasses)" doc="Optimizes smoothing for the current selection, allowing multiple passes, if specified, for optimal smoothing. This method is equivalent to selecting Modify &amp;gt; Shape &amp;gt; Optimize." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df5.html">
					<scope name="smoothing" ilk="argument" citdl="Number" doc="An integer in the range from 0 to 100, with 0 specifying no smoothing and 100 specifying maximum smoothing. "/>
					<scope name="bUseMultiplePasses" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, indicates that the method should use multiple passes, which is slower but produces a better result. This parameter has the same effect as clicking the Use Multiple Passes button in the Optimize Curves dialog box."/>
				</scope>
				<variable name="path" citdl="String" doc="Read-only property; a string that represents the path of the document in a platform-specific format. If the document has never been saved, this property is undefined. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e4d.html"/>
				<variable name="pathURI" citdl="String" doc="Read-only property; a string that represents the path of the document, expressed as a file:/// URI. If the document has never been saved, this property is undefined. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS9931616E-E7F0-41de-A90D-50B9342C8D04.html"/>
				<scope name="publish" ilk="function" signature="publish()" doc="Publishes the document according to the active publish settings (File &amp;gt; Publish Settings). This method is equivalent to selecting File &amp;gt; Publish." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df4.html"/>
				<variable name="publishProfiles" citdl="Array" doc="Read-only property; an array of the publish profile names for the document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e4c.html"/>
				<scope name="punch" ilk="function" returns="Boolean" signature="punch():Boolean" doc="Uses the top selected drawing object to punch through all selected drawing objects underneath it. This method returns false if there are no drawing objects selected or if any of the selected items are not drawing objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df3.html"/>
				<scope name="removeAllFilters" ilk="function" signature="removeAllFilters()" doc="Removes all filters from the selected object(s)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df2.html"/>
				<scope name="removeDataFromDocument" ilk="function" signature="removeDataFromDocument(name)" doc="Removes persistent data with the specified name that has been attached to the document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df1.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to remove. "/>
				</scope>
				<scope name="removeDataFromSelection" ilk="function" signature="removeDataFromSelection(name)" doc="Removes persistent data with the specified name that has been attached to the selection. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7df0.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the persistent data to remove. "/>
				</scope>
				<scope name="removeFilter" ilk="function" signature="removeFilter(filterIndex)" doc="Removes the specified filter from the Filters list of the selected object(s)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7def.html">
					<scope name="filterIndex" ilk="argument" citdl="Number" doc="An integer specifying the zero-based index of the filter to remove from the selected object(s)."/>
				</scope>
				<scope name="renamePublishProfile" ilk="function" returns="Boolean" signature="renamePublishProfile([profileNewName]):Boolean" doc="Renames the current profile." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dee.html">
					<scope name="profileNewName" ilk="argument" citdl="Parameter" doc="An optional parameter that specifies the new name for the profile. The new name must be unique. If the name is not specified, a default name is provided."/>
				</scope>
				<scope name="renameScene" ilk="function" returns="Boolean" signature="renameScene(name):Boolean" doc="Renames the currently selected scene in the Scenes panel. The new name for the selected scene must be unique. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ded.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the new name of the scene. "/>
				</scope>
				<scope name="reorderScene" ilk="function" signature="reorderScene(sceneToMove, sceneToPutItBefore)" doc="Moves the specified scene before another specified scene. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dec.html">
					<scope name="sceneToMove" ilk="argument" citdl="Number" doc="An integer that specifies which scene to move, with 0 (zero) being the first scene. "/>
					<scope name="sceneToPutItBefore" ilk="argument" citdl="Number" doc="An integer that specifies the scene before which you want to move the scene specified by sceneToMove. Specify 0 (zero) for the first scene. For example, if you specify 1 for sceneToMove and 0 for sceneToPutItBefore, the second scene is placed before the first scene. Specify -1 to move the scene to the end. "/>
				</scope>
				<scope name="resetOvalObject" ilk="function" signature="resetOvalObject()" doc="Sets all values in the Property inspector to default Oval object settings. If any Oval objects are selected, their properties are reset to default values as well." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fcc.html"/>
				<scope name="resetRectangleObject" ilk="function" signature="resetRectangleObject()" doc="Sets all values in the Property inspector to default Rectangle object settings. If any Rectangle objects are selected, their properties are reset to default values as well." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fcb.html"/>
				<scope name="resetTransformation" ilk="function" signature="resetTransformation()" doc="Resets the transformation matrix. This method is equivalent to selecting Modify &amp;gt; Transform &amp;gt; Remove Transform." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de9.html"/>
				<scope name="revert" ilk="function" signature="revert()" doc="Reverts the specified document to its previously saved version. This method is equivalent to selecting File &amp;gt; Revert. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e35.html"/>
				<scope name="revertToLastVersion" ilk="function" returns="Boolean" signature="revertToLastVersion():Boolean" doc="If the file can be reverted, displays a dialog box to let the user confirm that the file should be reverted. If the user confirms, this method reverts the file to the version stored on the Version Cue server and logs any errors to the Output panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fca.html"/>
				<scope name="rotate3DSelection" ilk="function" signature="rotate3DSelection(xyzCoordinate, bGlobalTransform)" doc="Method: applies a 3D rotation to the selection. This method is available only for movie clips." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS0866D18E-C028-4ec1-B351-59F12BAB2743.html">
					<scope name="xyzCoordinate" ilk="argument" citdl="Object" doc="An XYZ coordinate point that specifies the axes for 3D rotation. "/>
					<scope name="bGlobalTransform" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether the transformation mode should be global (true) or local (false)."/>
				</scope>
				<scope name="rotateSelection" ilk="function" signature="rotateSelection(angle [, rotationPoint])" doc="Rotates the selection by a specified number of degrees. The effect is the same as using the Free Transform tool to rotate the object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de6.html">
					<scope name="angle" ilk="argument" citdl="Number" doc="A floating-point value that specifies the angle of the rotation. "/>
					<scope name="rotationPoint" ilk="argument" citdl="String" doc="A string that specifies which side of the bounding box to rotate. Acceptable values are &quot;top right&quot;, &quot;top left&quot;, &quot;bottom right&quot;, &quot;bottom left&quot;, &quot;top center&quot;, &quot;right center&quot;, &quot;bottom center&quot;, and &quot;left center&quot;. If unspecified, the method uses the transformation point. This parameter is optional."/>
				</scope>
				<scope name="save" ilk="function" returns="Boolean" signature="save([bOkToSaveAs]):Boolean" doc="Saves the document in its default location. This method is equivalent to selecting File &amp;gt; Save." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de5.html">
					<scope name="bOkToSaveAs" ilk="argument" citdl="Parameter" doc="An optional parameter that, if true or omitted, and the file was never saved, opens the Save As dialog box. If false and the file was never saved, the file is not saved."/>
				</scope>
				<scope name="saveAndCompact" ilk="function" returns="Boolean" signature="saveAndCompact([bOkToSaveAs]):Boolean" doc="Saves and compacts the file. This method is equivalent to selecting File &amp;gt; Save and Compact. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de4.html">
					<scope name="bOkToSaveAs" ilk="argument" citdl="Parameter" doc="An optional parameter that, if true or omitted and the file was never saved, opens the Save As dialog box. If false and the file was never saved, the file is not saved. The default value is true."/>
				</scope>
				<scope name="saveAVersion" ilk="function" returns="Boolean" signature="saveAVersion():Boolean" doc="If the file can be saved to the Version Cue server, displays a dialog box to let the user enter version comments, saves a version of the specified document to the server, and logs any errors to the Output panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc9.html"/>
				<scope name="scaleSelection" ilk="function" signature="scaleSelection(xScale, yScale [, whichCorner])" doc="Scales the selection by a specified amount. This method is equivalent to using the Free Transform tool to scale the object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de2.html">
					<scope name="xScale" ilk="argument" citdl="Number" doc="A floating-point value that specifies the amount of x by which to scale. "/>
					<scope name="yScale" ilk="argument" citdl="Number" doc="A floating-point value that specifies the amount of y by which to scale. "/>
					<scope name="whichCorner" ilk="argument" citdl="String" doc="A string value that specifies the edge about which the transformation occurs. If omitted, scaling occurs about the transformation point. Acceptable values are: &quot;bottom left&quot;, &quot;bottom right&quot;, &quot;top right&quot;, &quot;top left&quot;, &quot;top center&quot;, &quot;right center&quot;, &quot;bottom center&quot;, and &quot;left center&quot;. This parameter is optional."/>
				</scope>
				<variable name="screenOutline" citdl="Screen" doc="Read-only property; the current ScreenOutline object for the document. Before accessing the object for the first time, make sure to use document.allowScreens() to determine whether the property exists. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e4b.html"/>
				<scope name="selectAll" ilk="function" signature="selectAll()" doc="Selects all items on the Stage. This method is equivalent to pressing Control+A (Windows) or Command+A (Macintosh) or selecting Edit &amp;gt; Select All. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de1.html"/>
				<variable name="selection" citdl="Array" doc="An array of the selected objects in the document. If nothing is selected, returns an array of length zero. If no document is open, returns null." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f91.html"/>
				<scope name="selectNone" ilk="function" signature="selectNone()" doc="Deselects any selected items. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7de0.html"/>
				<scope name="setAlignToDocument" ilk="function" signature="setAlignToDocument(bToStage)" doc="Sets the preferences for document.align(), document.distribute(), document.match(), and document.space() to act on the document. This method is equivalent to enabling the To Stage button in the Align panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ddf.html">
					<scope name="bToStage" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, aligns objects to the Stage. If set to false, it does not. "/>
				</scope>
				<scope name="setBlendMode" ilk="function" signature="setBlendMode(mode)" doc="Sets the blending mode for the selected objects. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dda.html">
					<scope name="mode" ilk="argument" citdl="String" doc="A string that represents the desired blending mode for the selected objects. Acceptable values are &quot;normal&quot;, &quot;layer&quot;, &quot;multiply&quot;, &quot;screen&quot;, &quot;overlay&quot;, &quot;hardlight&quot;, &quot;lighten&quot;, &quot;darken&quot;, &quot;difference&quot;, &quot;add&quot;, &quot;subtract&quot;, &quot;invert&quot;, &quot;alpha&quot;, and &quot;erase&quot;."/>
				</scope>
				<scope name="setCustomFill" ilk="function" signature="setCustomFill(fill)" doc="Sets the fill settings for the Tools panel, Property inspector, and any selected shapes. This allows a script to set the fill settings before drawing the object, rather than drawing the object, selecting it, and changing the fill settings. It also lets a script change the Tools panel and Property inspector fill settings. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd9.html">
					<scope name="fill" ilk="argument" citdl="Fill" doc="A Fill object that specifies the fill settings to be used. See Fill object."/>
				</scope>
				<scope name="setCustomStroke" ilk="function" signature="setCustomStroke(stroke)" doc="Sets the stroke settings for the Tools panel, Property inspector, and any selected shapes. This allows a script to set the stroke settings before drawing the object, rather than drawing the object, selecting it, and changing the stroke settings. It also lets a script change the Tools panel and Property inspector stroke settings. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd8.html">
					<scope name="stroke" ilk="argument" citdl="Stroke" doc="A Stroke object. "/>
				</scope>
				<scope name="setElementProperty" ilk="function" signature="setElementProperty(property, value)" doc="Sets the specified Element property on selected object(s) in the document. This method does nothing if there is no selection." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd7.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the name of the Element property to set. For a complete list of properties and values, see the Property summary table for the Element object.You can’t use this method to set values for read-only properties, such as element.elementType, element.top, or element.left. "/>
					<scope name="value" ilk="argument" citdl="Number" doc="An integer that specifies the value to set in the specified Element property."/>
				</scope>
				<scope name="setElementTextAttr" ilk="function" returns="Boolean" signature="setElementTextAttr(attrName, attrValue [, startIndex [, endIndex]]):Boolean" doc="Sets the specified textAttrs property of the selected text items to the specified value. For a list of property names and allowable values, see the Property summary table for the TextAttrs object. If the optional parameters are not passed, the method sets the style of the currently selected text range, or the whole text field if no text is selected. If only startIndex is passed, the method sets that character’s attributes. If startIndex and endIndex are passed, the method sets the attributes on the characters starting from startIndex up to, but not including, endIndex. If paragraph styles are specified, all the paragraphs that fall within the range are affected." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd6.html">
					<scope name="attrName" ilk="argument" citdl="String" doc="A string that specifies the name of the TextAttrs property to change. "/>
					<scope name="attrValue" ilk="argument" citdl="value" doc="The value to which to set the TextAttrs property. For a list of property names and expected values, see the Property summary table for the TextAttrs object."/>
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer value that specifies the index of the first character that is affected. This parameter is optional."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer value that specifies the index of the last character that is affected. This parameter is optional."/>
				</scope>
				<scope name="setFillColor" ilk="function" signature="setFillColor(color)" doc="Changes the fill color of the selection to the specified color. For information on changing the fill color in the Tools panel and Property inspector, see document.setCustomFill()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd5.html">
					<scope name="color" ilk="argument" citdl="Fill" doc="The color of the fill, in one of the following formats: "/>
				</scope>
				<scope name="setFilterProperty" ilk="function" signature="setFilterProperty(property, filterIndex, value)" doc="Sets a specified filter property for the currently selected objects (assuming that the object supports the specified filter)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd4.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string specifying the property to be set. Acceptable values are &quot;blurX&quot;, &quot;blurY&quot;, &quot;quality&quot;, angle&quot;, &quot;distance&quot;, &quot;strength&quot;, &quot;knockout&quot;, &quot;inner&quot;, &quot;bevelType&quot;, &quot;color&quot;, &quot;shadowColor&quot;, and &quot;highlightColor&quot;."/>
					<scope name="filterIndex" ilk="argument" citdl="Number" doc="An integer specifying the zero-based index of the filter in the Filters list."/>
					<scope name="value" ilk="argument" citdl="Number" doc="A number or string specifying the value to be set for the specified filter property. Acceptable values depend on the property and the filter being set. "/>
				</scope>
				<scope name="setFilters" ilk="function" signature="setFilters(filterArray)" doc="Applies filters to the selected objects. Use this method after calling document.getFilters() and making any desired changes to the filters. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd3.html">
					<scope name="filterArray" ilk="argument" citdl="Array" doc="The array of filters currently specified."/>
				</scope>
				<scope name="setInstanceAlpha" ilk="function" signature="setInstanceAlpha(opacity)" doc="Sets the opacity of the instance. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd2.html">
					<scope name="opacity" ilk="argument" citdl="Number" doc="An integer between 0 (transparent) and 100 (completely saturated) that adjusts the transparency of the instance."/>
				</scope>
				<scope name="setInstanceBrightness" ilk="function" signature="setInstanceBrightness(brightness)" doc="Sets the brightness for the instance. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd1.html">
					<scope name="brightness" ilk="argument" citdl="Number" doc="An integer that specifies brightness as a value from -100 (black) to 100 (white). "/>
				</scope>
				<scope name="setInstanceTint" ilk="function" signature="setInstanceTint( color, strength )" doc="Sets the tint for the instance. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dd0.html">
					<scope name="color" ilk="argument" doc="The color of the tint, in one of the following formats: "/>
					<scope name="strength" ilk="argument" citdl="Number" doc="An integer between 0 and 100 that specifies the opacity of the tint. "/>
				</scope>
				<scope name="setMetadata" ilk="function" returns="Boolean" signature="setMetadata(strMetadata):Boolean" doc="Sets the XML metadata for the specified document, overwriting any existing metadata. The XML passed as strMetadata is validated and may be rewritten before being stored. If it cannot be validated as legal XML or violates specific rules, then the XML metadata is not set and false is returned. (If false is returned, there is no way to get more detailed error information.) " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dcf.html">
					<scope name="strMetadata" ilk="argument" citdl="String" doc="A string containing the XML metadata to be associated with the document. For more information, see the following description. "/>
				</scope>
				<scope name="setMobileSettings" ilk="function" returns="value" signature="setMobileSettings(xmlString):value" doc="Sets the value of an XML settings string in a mobile FLA file. (Most mobile FLA files have an XML string that describes the settings within the document.) " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc8.html">
					<scope name="xmlString" ilk="argument" citdl="String" doc="A string that describes the XML settings in a mobile FLA file."/>
				</scope>
				<scope name="setOvalObjectProperty" ilk="function" signature="setOvalObjectProperty(propertyName, value)" doc="Specifies a value for a specified property of primitive Oval objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc7.html">
					<scope name="propertyName" ilk="argument" citdl="String" doc="A string that specifies the property to be set. For acceptable values, see the Property summary table for the Oval object."/>
					<scope name="value" ilk="argument" citdl="value" doc="The value to be assigned to the property. Acceptable values vary depending on the property you specify in propertyName."/>
				</scope>
				<scope name="setPlayerVersion" ilk="function" returns="value" signature="setPlayerVersion(version):value" doc="Sets the version of the Flash Player targeted by the specified document. This is the same value as that set in the Publish Settings dialog box. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc6.html">
					<scope name="version" ilk="argument" citdl="String" doc="A string that represents the version of Flash Player targeted by the specified document. Acceptable values are &quot;FlashLite&quot;, &quot;FlashLite11&quot;, &quot;FlashLite20&quot; , &quot;FlashLite30&quot;, &quot;1&quot;, &quot;2&quot;, &quot;3&quot;, &quot;4&quot;, &quot;5&quot;, &quot;6&quot;, &quot;7&quot;, &quot;8&quot;, &quot;9&quot;, &quot;FlashPlayer10&quot;, and &quot;AdobeAIR1_1&quot;. "/>
				</scope>
				<scope name="setRectangleObjectProperty" ilk="function" signature="setRectangleObjectProperty(propertyName, value)" doc="Specifies a value for a specified property of primitive Rectangle objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc5.html">
					<scope name="propertyName" ilk="argument" citdl="String" doc="A string that specifies the property to be set. For acceptable values, see the Property summary table for the Rectangle object."/>
					<scope name="value" ilk="argument" citdl="value" doc="The value to be assigned to the property. Acceptable values vary depending on the property you specify in propertyName."/>
				</scope>
				<scope name="setSelectionBounds" ilk="function" signature="setSelectionBounds(boundingRectangle [, bContactSensitiveSelection])" doc="Moves and resizes the selection in a single operation. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dca.html">
					<scope name="boundingRectangle" ilk="argument" citdl="Rectangle" doc="A rectangle that specifies the new location and size of the selection. For information on the format of boundingRectangle, see document.addNewRectangle()."/>
					<scope name="bContactSensitiveSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether the Contact Sensitive selection mode is enabled (true) or disabled (false) during object selection. The default value is false. "/>
				</scope>
				<scope name="setSelectionRect" ilk="function" signature="setSelectionRect(rect [, bReplaceCurrentSelection [, bContactSensitiveSelection]])" doc="Draws a rectangular selection marquee relative to the Stage, using the specified coordinates. This is unlike document.getSelectionRect(), in which the rectangle is relative to the object being edited. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc9.html">
					<scope name="rect" ilk="argument" citdl="Rectangle" doc="A rectangle object to set as selected. For information on the format of rect, see document.addNewRectangle()."/>
					<scope name="bReplaceCurrentSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether the method replaces the current selection (true) or adds to the current selection (false). The default value is true. "/>
					<scope name="bContactSensitiveSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether the Contact Sensitive selection mode is enabled (true) or disabled (false) during object selection. The default value is false. "/>
				</scope>
				<scope name="setStageVanishingPoint" ilk="function" signature="setStageVanishingPoint(point)" doc="Specifies the vanishing point for viewing 3D objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS98947551-DB0B-4342-A8A0-F12DA9359735.html">
					<scope name="point" ilk="argument" citdl="Object" doc="A point that specifies the x and y coordinates of the location at which to set the vanishing point for viewing 3D objects."/>
				</scope>
				<scope name="setStageViewAngle" ilk="function" signature="setStageViewAngle(angle)" doc="Specifies the perspective angle for viewing 3D objects." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB992D8DA-2CC2-4320-BF2B-39992B47A849.html">
					<scope name="angle" ilk="argument" citdl="Fl" doc="A floating point value between 0.0 and 179.0. "/>
				</scope>
				<scope name="setStroke" ilk="function" signature="setStroke(color, size, strokeType)" doc="Sets the color, width, and style of the selected stroke. For information on changing the stroke in the Tools panel and Property inspector, see document.setCustomStroke()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc8.html">
					<scope name="color" ilk="argument" citdl="Stroke" doc="The color of the stroke, in one of the following formats: "/>
					<scope name="size" ilk="argument" citdl="Number" doc="A floating-point value that specifies the new stroke size for the selection. "/>
					<scope name="strokeType" ilk="argument" citdl="String" doc="A string that specifies the new type of stroke for the selection. Acceptable values are &quot;hairline&quot;, &quot;solid&quot;, &quot;dashed&quot;, &quot;dotted&quot;, &quot;ragged&quot;, &quot;stipple&quot;, and &quot;hatched&quot;. "/>
				</scope>
				<scope name="setStrokeColor" ilk="function" signature="setStrokeColor(color)" doc="Changes the stroke color of the selection to the specified color. For information on changing the stroke in the Tools panel and Property inspector, see document.setCustomStroke()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc7.html">
					<scope name="color" ilk="argument" citdl="Stroke" doc="The color of the stroke, in one of the following formats: "/>
				</scope>
				<scope name="setStrokeSize" ilk="function" signature="setStrokeSize(size)" doc="Changes the stroke size of the selection to the specified size. For information on changing the stroke in the Tools panel and Property inspector, see document.setCustomStroke()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc6.html">
					<scope name="size" ilk="argument" citdl="Number" doc="A floating-point value from 0.25 to 10 that specifies the stroke size. The method ignores precision greater than two decimal places."/>
				</scope>
				<scope name="setStrokeStyle" ilk="function" signature="setStrokeStyle(strokeType)" doc="Changes the stroke style of the selection to the specified style. For information on changing the stroke in the Tools panel and Property inspector, see document.setCustomStroke()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc5.html">
					<scope name="strokeType" ilk="argument" citdl="String" doc="A string that specifies the stroke style for the current selection. Acceptable values are &quot;hairline&quot;, &quot;solid&quot;,&quot;dashed&quot;, &quot;dotted&quot;, &quot;ragged&quot;, &quot;stipple&quot;, and &quot;hatched&quot;. "/>
				</scope>
				<scope name="setTextRectangle" ilk="function" returns="Boolean" signature="setTextRectangle(boundingRectangle):Boolean" doc="Changes the bounding rectangle for the selected text item to the specified size. This method causes the text to reflow inside the new rectangle; the text item is not scaled or transformed. The values passed in boundingRectangle are used as follows:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc4.html">
					<scope name="boundingRectangle" ilk="argument" citdl="Rectangle" doc="A rectangle that specifies the new size within which the text item should flow. For information on the format of boundingRectangle, see document.addNewRectangle(). "/>
				</scope>
				<scope name="setTextSelection" ilk="function" returns="Boolean" signature="setTextSelection(startIndex, endIndex):Boolean" doc="Sets the text selection of the currently selected text field to the values specified by the startIndex and endIndex values. Text editing is activated, if it isn’t already. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc3.html">
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that specifies the position of the first character to select. The first character position is 0 (zero)."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the end position of the selection up to, but not including, endIndex. The first character position is 0 (zero)."/>
				</scope>
				<scope name="setTextString" ilk="function" returns="Boolean" signature="setTextString(text [, startIndex [, endIndex]]):Boolean" doc="Inserts a string of text. If the optional parameters are not passed, the existing text selection is replaced; if the Text object isn’t currently being edited, the whole text string is replaced. If only startIndex is passed, the string passed is inserted at this position. If startIndex and endIndex are passed, the string passed replaces the segment of text starting from startIndex up to, but not including, endIndex. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc2.html">
					<scope name="text" ilk="argument" citdl="String" doc="A string of the characters to insert in the text field. "/>
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that specifies the first character to replace. The first character position is 0 (zero). This parameter is optional."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the last character to replace. This parameter is optional."/>
				</scope>
				<scope name="setTransformationPoint" ilk="function" signature="setTransformationPoint( transformationPoint )" doc="Sets the position of the current selection’s transformation point. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e1c.html">
					<scope name="transformationPoint" ilk="argument" citdl="Object" doc="A point (for example, {x:10, y:20}, where x and y are floating-point numbers) that specifies values for the transformation point of each of the following elements: "/>
				</scope>
				<variable name="silent" citdl="Boolean" doc="A Boolean value that specifies whether the object is accessible. This is equivalent to the inverse logic of the Make Movie Accessible setting in the Accessibility panel. That is, if document.silent is true, it is the same as the Make Movie Accessible option being unchecked. If it is false, it is the same as the Make Movie Accessible option being checked. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e48.html"/>
				<scope name="skewSelection" ilk="function" signature="skewSelection(xSkew, ySkew [, whichEdge])" doc="Skews the selection by a specified amount. The effect is the same as using the Free Transform tool to skew the object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dc0.html">
					<scope name="xSkew" ilk="argument" citdl="Number" doc="A floating-point number that specifies the amount of x by which to skew, measured in degrees."/>
					<scope name="ySkew" ilk="argument" citdl="Number" doc="A floating-point number that specifies the amount of y by which to skew, measured in degrees."/>
					<scope name="whichEdge" ilk="argument" citdl="String" doc="A string that specifies the edge where the transformation occurs; if omitted, skew occurs at the transformation point. Acceptable values are &quot;top center&quot;, &quot;right center&quot;, &quot;bottom center&quot;, and &quot;left center&quot;. This parameter is optional."/>
				</scope>
				<scope name="smoothSelection" ilk="function" signature="smoothSelection()" doc="Smooths the curve of each selected fill outline or curved line. This method performs the same action as the Smooth button in the Tools panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7dbf.html"/>
				<variable name="sourcePath" citdl="String" doc="A string that contains a list of items in the document’s ActionScript 3.0 Source path, which specifies the location of ActionScript class files. Items in the string are delimited by semi-colons. In the authoring tool, the items are specified by choosing File &amp;gt; Publish Settings and then choosing ActionScript 3.0 Script Settings on the Flash tab." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5A00B146-30BC-4841-A27F-325968E8084B.html"/>
				<scope name="space" ilk="function" signature="space(direction [, bUseDocumentBounds])" doc="Spaces the objects in the selection evenly. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ddb.html">
					<scope name="direction" ilk="argument" citdl="String" doc="A string that specifies the direction in which to space the objects in the selection. Acceptable values are &quot;horizontal&quot; or &quot;vertical&quot;."/>
					<scope name="bUseDocumentBounds" ilk="argument" citdl="Boolean" doc="A Boolean value that, when set to true, spaces the objects to the document bounds. Otherwise, the method uses the bounds of the selected objects. The default is false. This parameter is optional."/>
				</scope>
				<scope name="straightenSelection" ilk="function" signature="straightenSelection()" doc="Straightens the currently selected strokes. This method is equivalent to using the Straighten button in the Tools panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cb4.html"/>
				<scope name="swapElement" ilk="function" signature="swapElement(name)" doc="Swaps the current selection with the specified one. The selection must contain a graphic, button, movie clip, video, or bitmap. This method displays an error message if no object is selected or the given object could not be found." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cb3.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the library item to use. "/>
				</scope>
				<scope name="swapStrokeAndFill" ilk="function" signature="swapStrokeAndFill()" doc="Swaps the Stroke and Fill colors. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cb2.html"/>
				<scope name="synchronizeWithHeadVersion" ilk="function" returns="Boolean" signature="synchronizeWithHeadVersion():Boolean" doc="Synchronizes the specified document with the most current version on the Version Cue server, and logs any errors to the Output panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc4.html"/>
				<scope name="testMovie" ilk="function" signature="testMovie()" doc="Executes a Test Movie operation on the document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e31.html"/>
				<scope name="testScene" ilk="function" signature="testScene()" doc="Executes a Test Scene operation on the current scene of the document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e2f.html"/>
				<variable name="timelines" citdl="Array" doc="Read-only property; an array of Timeline objects (see Timeline object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f90.html"/>
				<scope name="traceBitmap" ilk="function" signature="traceBitmap(threshold, minimumArea, curveFit, cornerThreshold)" doc="Performs a trace bitmap on the current selection. This method is equivalent to selecting Modify &amp;gt; Bitmap &amp;gt; Trace Bitmap." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cae.html">
					<scope name="threshold" ilk="argument" citdl="Number" doc="An integer that controls the number of colors in your traced bitmap. Acceptable values are integers between 0 and 500. "/>
					<scope name="minimumArea" ilk="argument" citdl="Number" doc="An integer that specifies the radius measured in pixels. Acceptable values are integers between 1 and 1000. "/>
					<scope name="curveFit" ilk="argument" citdl="String" doc="A string that specifies how smoothly outlines are drawn. Acceptable values are &quot;pixels&quot;, &quot;very tight&quot;, &quot;tight&quot;, &quot;normal&quot;, &quot;smooth&quot;, and &quot;very smooth&quot;."/>
					<scope name="cornerThreshold" ilk="argument" citdl="String" doc="A string that is similar to curveFit, but it pertains to the corners of the bitmap image. Acceptable values are &quot;many corners&quot;, &quot;normal&quot;, and &quot;few corners&quot;. "/>
				</scope>
				<scope name="translate3DCenter" ilk="function" signature="translate3DCenter(xyzCoordinate)" doc="Method: sets the XYZ position around which the selection is translated or rotated. This method is available only for movie clips." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS19655B33-1D64-40f7-AE53-E089C37C0F83.html">
					<scope name="xyzCoordinate" ilk="argument" citdl="Object" doc="An XYZ coordinate that specifies the center point for 3D rotation or translation. "/>
				</scope>
				<scope name="translate3DSelection" ilk="function" signature="translate3DSelection(xyzCoordinate, bGlobalTransform)" doc="Method: applies a 3D translation to the selection. This method is available only for movie clips." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS7EAC74FE-B200-4f4d-9C6B-46858F05897E.html">
					<scope name="xyzCoordinate" ilk="argument" doc="An XYZ coordinate that specifies the axes for 3D translation. "/>
					<scope name="bGlobalTransform" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether the transformation mode should be global (true) or local (false)."/>
				</scope>
				<scope name="transformSelection" ilk="function" signature="transformSelection(a, b, c, d)" doc="Performs a general transformation on the current selection by applying the matrix specified in the arguments. For more information, see the element.matrix property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cad.html">
					<scope name="a" ilk="argument" citdl="Number" doc="A floating-point number that specifies the (0,0) element of the transformation matrix. "/>
					<scope name="b" ilk="argument" citdl="Number" doc="A floating-point number that specifies the (0,1) element of the transformation matrix. "/>
					<scope name="c" ilk="argument" citdl="Number" doc="A floating-point number that specifies the (1,0) element of the transformation matrix. "/>
					<scope name="d" ilk="argument" citdl="Number" doc="A floating-point number that specifies the (1,1) element of the transformation matrix."/>
				</scope>
				<scope name="unGroup" ilk="function" signature="unGroup()" doc="Ungroups the current selection. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d3e.html"/>
				<scope name="union" ilk="function" returns="Boolean" signature="union():Boolean" doc="Combines all selected shapes into a drawing object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d97.html"/>
				<scope name="unlockAllElements" ilk="function" signature="unlockAllElements()" doc="Unlocks all locked elements on the currently selected frame. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7caa.html"/>
				<variable name="viewMatrix" citdl="Matrix" doc="Read-only property; a Matrix object. The viewMatrix is used to transform from object space to document space when the document is in edit mode. The mouse location, as a tool receives it, is relative to the object that is currently being edited. See Matrix object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cb6.html"/>
				<variable name="width" citdl="Number" doc="An integer that specifies the width of the document (Stage) in pixels. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d3d.html"/>
				<scope name="xmlPanel" ilk="function" returns="Object" signature="xmlPanel(fileURI):Object" doc="Posts an XMLUI dialog box. See fl.xmlui." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ca9.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path to the XML file defining the controls in the panel. The full path is required. "/>
				</scope>
				<variable name="zoomFactor" citdl="value" doc="Specifies the zoom percent of the Stage at authoring time. A value of 1 equals 100 percent zoom, 8 equals 800 percent, .5 equals 50 percent, and so on." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c6f.html"/>
			</scope>
			<!--

				DrawingLayer

			-->
			<scope name="DrawingLayer" ilk="class" doc="The drawingLayer object is accessible from JavaScript as a child of the flash object. The drawingLayer object is used for extensible tools when the user wants to temporarily draw while dragging—for example, when creating a selection marquee. You should call drawingLayer.beginFrame() before you call any other drawingLayer methods. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff7.html">
				<scope name="beginDraw" ilk="function" signature="beginDraw([persistentDraw])" doc="Puts Flash in drawing mode. Drawing mode is used for temporary drawing while the mouse button is pressed. You typically use this method only when creating extensible tools. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c6a.html">
					<scope name="persistentDraw" ilk="argument" citdl="Boolean" doc=" A Boolean value (optional). If set to true, it indicates that the drawing in the last frame remains on the Stage until a new beginDraw() or beginFrame() call is made. (In this context, frame refers to where you start and end drawing; it does not refer to timeline frames.) For example, when users draw a rectangle, they can preview the outline of the shape while dragging the mouse. If you want that preview shape to remain after the user releases the mouse button, set persistentDraw to true."/>
				</scope>
				<scope name="beginFrame" ilk="function" signature="beginFrame()" doc="Erases what was previously drawn using the drawingLayer and prepares for more drawing commands. Should be called after drawingLayer.beginDraw(). Everything drawn between drawingLayer.beginFrame() and an drawingLayer.endFrame() remains on the Stage until you call the next beginFrame() and endFrame(). (In this context, frame refers to where you start and end drawing; it does not refer to timeline frames.) You typically use this method only when creating extensible tools. See drawingLayer.beginDraw()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c69.html"/>
				<scope name="cubicCurveTo" ilk="function" signature="cubicCurveTo(x1Ctrl, y1Ctrl, x2Ctl, y2Ctl, xEnd, yEnd)" doc="Draws a cubic curve from the current pen location using the parameters as the coordinates of the cubic segment. You typically use this method only when creating extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c68.html">
					<scope name="x1Ctl" ilk="argument" citdl="Number" doc="A floating-point value that is the x location of the first control point. "/>
					<scope name="y1Ctl" ilk="argument" citdl="Number" doc="A floating-point value that is the y location of the first control point."/>
					<scope name="x2Ctl" ilk="argument" citdl="Number" doc="A floating-point value that is the x position of the middle control point. "/>
					<scope name="y2Ctl" ilk="argument" citdl="Number" doc="A floating-point value that is the y position of the middle control point. "/>
					<scope name="xEnd" ilk="argument" citdl="Number" doc="A floating-point value that is the x position of the end control point. "/>
					<scope name="yEnd" ilk="argument" citdl="Number" doc="A floating-point value that is the y position of the end control point. "/>
				</scope>
				<scope name="curveTo" ilk="function" signature="curveTo(xCtl, yCtl, xEnd, yEnd)" doc="Draws a quadratic curve segment starting at the current drawing position and ending at a specified point. You typically use this method only when creating extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c67.html">
					<scope name="xCtl" ilk="argument" citdl="Number" doc="A floating-point value that is the x position of the control point. "/>
					<scope name="yCtl" ilk="argument" citdl="Number" doc="A floating-point value that is the y position of the control point. "/>
					<scope name="xEnd" ilk="argument" citdl="Number" doc="A floating-point value that is the x position of the end control point. "/>
					<scope name="yEnd" ilk="argument" citdl="Number" doc="A floating-point value that is the y position of the end control point. "/>
				</scope>
				<scope name="drawPath" ilk="function" signature="drawPath(path)" doc="Draws the path specified by the path parameter. You typically use this method only when creating extensible tools. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c66.html">
					<scope name="path" ilk="argument" citdl="Path" doc="A Path object to draw. "/>
				</scope>
				<scope name="endDraw" ilk="function" signature="endDraw()" doc="Exits drawing mode. Drawing mode is used when you want to temporarily draw while the mouse button is pressed. You typically use this method only when creating extensible tools. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c65.html"/>
				<scope name="endFrame" ilk="function" signature="endFrame()" doc="Signals the end of a group of drawing commands. A group of drawing commands refers to everything drawn between drawingLayer.beginFrame() and drawingLayer.endFrame(). The next call to drawingLayer.beginFrame() will erase whatever was drawn in this group of drawing commands. You typically use this method only when creating extensible tools. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c64.html"/>
				<scope name="lineTo" ilk="function" signature="lineTo(x, y)" doc="Draws a line from the current drawing position to the point (x,y). You typically use this method only when creating extensible tools. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c62.html">
					<scope name="x" ilk="argument" citdl="Number" doc="A floating-point value that is the x coordinate of the end point of the line to draw. "/>
					<scope name="y" ilk="argument" citdl="Number" doc="A floating-point value that is the y coordinate of the end point of the line to draw. "/>
				</scope>
				<scope name="moveTo" ilk="function" signature="moveTo(x, y)" doc="Sets the current drawing position. You typically use this method only when creating extensible tools. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c61.html">
					<scope name="x" ilk="argument" citdl="Number" doc="A floating-point value that specifies the x coordinate of the position at which to start drawing."/>
					<scope name="y" ilk="argument" citdl="Number" doc="A floating-point value that specifies the y coordinate of the position at which to start drawing."/>
				</scope>
				<scope name="newPath" ilk="function" returns="Path" signature="newPath():Path" doc="Returns a new Path object. You typically use this method only when creating extensible tools. See Path object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c5d.html"/>
				<scope name="setColor" ilk="function" signature="setColor(color)" doc="Sets the color of subsequently drawn data. Applies only to persistent data. To use this method, the parameter passed to drawingLayer.beginDraw() must be set to true. You typically use this method only when creating extensible tools. See drawingLayer.beginDraw()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c5b.html">
					<scope name="color" ilk="argument" doc="The color of subsequently drawn data, in one of the following formats: "/>
				</scope>
				<scope name="setFill" ilk="function" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSEA0D1613-8917-4787-8ECF-E5A76CC22628.html"/>
				<scope name="setStroke" ilk="function" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA508088C-50E3-489d-9EAE-B32734E7C1C0.html"/>
			</scope>
			<!--

				Edge

			-->
			<scope name="Edge" ilk="class" doc="The Edge object represents an edge of a shape on the Stage." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f4f.html">
				<variable name="cubicSegmentIndex" citdl="Number" doc="Read-only property; an integer that specifies the index value of a cubic segment of the edge (see shape.getCubicSegmentPoints()). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS582A639A-301E-4e72-BB0C-215DFCB1351D.html"/>
				<scope name="getControl" ilk="function" returns="Object" signature="getControl(i):Object" doc="Gets a point object set to the location of the specified control point of the edge. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c57.html">
					<scope name="i" ilk="argument" citdl="Number" doc="An integer that specifies which control point of the edge to return. Specify 0 for the first control point, 1 for the middle control point, or 2 for the end control point. If the edge.isLine property is true, the middle control point is set to the midpoint of the segment joining the beginning and ending control points. "/>
				</scope>
				<scope name="getHalfEdge" ilk="function" returns="HalfEdge" signature="getHalfEdge(index):HalfEdge" doc="Returns a HalfEdge object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c56.html">
					<scope name="index" ilk="argument" citdl="Number" doc="An integer that specifies which half edge to return. The value of index must be either 0 for the first half edge or 1 for the second half edge."/>
				</scope>
				<variable name="id" citdl="Number" doc="Read-only property; an integer that represents a unique identifier for the edge." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c4e.html"/>
				<variable name="isLine" citdl="Number" doc="Read-only property; an integer with a value of 0 or 1. A value of 1 indicates that the edge is a straight line. In that case, the middle control point bisects the line joining the two end points. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c50.html"/>
				<scope name="setControl" ilk="function" signature="setControl(index, x, y)" doc="Sets the position of the control point of the edge. You must call shape.beginEdit() before using this method. See shape.beginEdit()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c55.html">
					<scope name="index " ilk="argument" citdl="Number" doc="An integer that specifies which control point to set. Use values 0, 1, or 2 to specify the beginning, middle, and end control points, respectively. "/>
					<scope name="x" ilk="argument" citdl="Number" doc="A floating-point value that specifies the horizontal location of the control point. If the Stage is in edit or edit-in-place mode, the point coordinate is relative to the edited object. Otherwise, the point coordinate is relative to the Stage."/>
					<scope name="y" ilk="argument" citdl="Number" doc="A floating-point value that specifies the vertical location of the control point. If the Stage is in edit or edit-in-place mode, the point coordinate is relative to the edited object. Otherwise, the point coordinate is relative to the Stage."/>
				</scope>
				<scope name="splitEdge" ilk="function" signature="splitEdge(t)" doc="Splits the edge into two pieces. You must call shape.beginEdit() before using this method." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c54.html">
					<scope name="t" ilk="argument" citdl="Number" doc="A floating-point value between 0 and 1 that specifies where to split the edge. A value of 0 represents one end point and a value of 1represents the other. For example, passing a value of 0.5 splits the edge in the middle, which, for a line is exactly in the center. If the edge represents a curve, 0.5 represents the parametric middle of the curve. "/>
				</scope>
				<variable name="stroke" citdl="Stroke" doc="A Stroke object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSFF3A5469-A8EB-494d-9D7A-2492A0971425.html"/>
			</scope>
			<!--

				Element

			-->
			<scope name="Element" ilk="class" doc="Everything that appears on the Stage is of the type Element. The following code example lets you select an element:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f6c.html">
				<variable name="depth" citdl="Number" doc="Read-only property; an integer that has a value greater than 0 for the depth of the object in the view. The drawing order of objects on the Stage specifies which one is on top of the others. Object order can also be managed with the Modify &amp;gt; Arrange menu item." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c06.html"/>
				<variable name="elementType" citdl="String" doc="Read-only property; a string that represents the type of the specified element. The value is one of the following: &quot;shape&quot;, &quot;text&quot;, &quot;instance&quot;, or &quot;shapeObj&quot;. A &quot;shapeObj&quot; is created with an extensible tool." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cbc.html"/>
				<scope name="getPersistentData" ilk="function" returns="Parameter" signature="getPersistentData(name):Parameter" doc="Retrieves the value of the data specified by the name parameter. The type of data depends on the type of the data that was stored (see element.setPersistentData()). Only symbols and bitmaps support persistent data. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c43.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that identifies the data to be returned. "/>
				</scope>
				<scope name="getTransformationPoint" ilk="function" returns="Object" signature="getTransformationPoint():Object" doc="Gets the value of the specified element’s transformation point." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc3.html"/>
				<scope name="hasPersistentData" ilk="function" returns="Boolean" signature="hasPersistentData(name):Boolean" doc="Determines whether the specified data has been attached to the specified element. Only symbols and bitmaps support persistent data." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c42.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data item to test. "/>
				</scope>
				<variable name="height" citdl="Fl" doc="A float value that specifies the height of the element in pixels. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c08.html"/>
				<variable name="layer" citdl="Layer" doc="Read-only property; represents the Layer object on which the element is located. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c0a.html"/>
				<variable name="left" citdl="Fl" doc="Read-only property; a float value that represents the left side of the element. The value of element.left is relative to the upper left of the Stage for elements that are in a scene and is relative to the symbol’s registration point (also origin point or zero point) if the element is stored within a symbol. Use document.setSelectionBounds() or document.moveSelectionBy() to set this property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c8a.html"/>
				<variable name="locked" citdl="Boolean" doc="A Boolean value: true if the element is locked; false otherwise. If the value of element.elementType is &quot;shape&quot;, this property is ignored. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cc0.html"/>
				<variable name="matrix" citdl="Matrix" doc="A Matrix object. A matrix has properties a, b, c, d, tx, and ty. The a, b, c, and d properties are floating-point values; the tx and ty properties are coordinates. See Matrix object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cc7.html"/>
				<variable name="name" citdl="String" doc="A string that specifies the name of the element, normally referred to as the Instance name. If the value of element.elementType is &quot;shape&quot;, this property is ignored. See element.elementType." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c0f.html"/>
				<scope name="removePersistentData" ilk="function" signature="removePersistentData(name)" doc="Removes any persistent data with the specified name that has been attached to the object. Only symbols and bitmaps support persistent data." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c41.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to remove. "/>
				</scope>
				<variable name="rotation" citdl="Number" doc="An integer or float value between ‑180 and 180 that specifies the object’s clockwise rotation, in degrees." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc2.html"/>
				<variable name="scaleX" citdl="Fl" doc="A float value that specifies the x scale value of symbols, drawing objects, and primitive rectangles and ovals. A value of 1 indicates 100 percent scale." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc1.html"/>
				<variable name="scaleY" citdl="Fl" doc="A float value that specifies the y scale value of symbols, drawing objects, and primitive rectangles and ovals. A value of 1 indicates 100 percent scale." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fc0.html"/>
				<variable name="selected" citdl="Boolean" doc="A Boolean value that specifies whether the element is selected (true) or not (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c02.html"/>
				<scope name="setPersistentData" ilk="function" signature="setPersistentData(name, type, value)" doc="Stores data with an element. The data is available when the FLA file containing the element is reopened. Only symbols and bitmaps support persistent data." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c40.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name to associate with the data. This name is used to retrieve the data. "/>
					<scope name="type" ilk="argument" citdl="String" doc="A string that defines the type of the data. The allowable values are &quot;integer&quot;, &quot;integerArray&quot;, &quot;double&quot;, &quot;doubleArray&quot;, &quot;string&quot;, and &quot;byteArray&quot;."/>
					<scope name="value" ilk="argument" citdl="value" doc="Specifies the value to associate with the object. The data type of value depends on the value of the type parameter. The specified value should be appropriate to the data type specified by the type parameter. "/>
				</scope>
				<scope name="setTransformationPoint" ilk="function" signature="setTransformationPoint(transformationPoint)" doc="Sets the position of the element’s transformation point." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fbf.html">
					<scope name="transformationPoint" ilk="argument" citdl="Object" doc="A point (for example, {x:10, y:20}, where x and y are floating-point numbers) that specifies values for an element’s or group’s transformation point. "/>
				</scope>
				<variable name="skewX" citdl="Fl" doc="A float value between ‑180 and 180 that specifies the x skew value of symbols, drawing objects, and primitive rectangles and ovals." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fbe.html"/>
				<variable name="skewY" citdl="Fl" doc="A float value between ‑180 and 180 that specifies the y skew value of symbols, drawing objects, and primitive rectangles and ovals." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fbd.html"/>
				<variable name="top" citdl="Element" doc="Read-only property; top side of the element. The value of element.top is relative to the upper left of the Stage for elements that are in a scene and is relative to the symbol’s registration point if the element is stored within a symbol. Use document.setSelectionBounds() or document.moveSelectionBy() to set this property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c8e.html"/>
				<variable name="transformX" citdl="Number" doc="A floating-point number that specifies the x value of the selected element’s transformation point, within the coordinate system of the element's parent. Setting this property to a new value moves the element. By contrast, the element.setTransformationPoint() method moves the transformation point but does not move the element." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fbc.html"/>
				<variable name="transformY" citdl="Number" doc="A floating-point number that specifies the y value of the selected element’s transformation point, within the coordinate system of the element’s parent. Setting this property to a new value moves the element. By contrast, the element.setTransformationPoint() method moves the transformation point but does not move the element." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fbb.html"/>
				<variable name="width" citdl="Fl" doc="A float value that specifies the width of the element in pixels." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bfa.html"/>
				<variable name="x" citdl="Fl" doc="A float value that specifies the x value of the selected element’s registration point." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fba.html"/>
				<variable name="y" citdl="Fl" doc="A float value that specifies the y value of the selected element’s registration point." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb9.html"/>
			</scope>
			<!--

				Fill

			-->
			<scope name="Fill" ilk="class" doc="This object contains all the properties of the Fill color setting of the Tools panel or of a selected shape. To retrieve a Fill object, use document.getCustomFill()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f83.html">
				<variable name="bitmapIsClipped" citdl="Boolean" doc="A Boolean value that specifies whether the bitmap fill for a shape that is larger than the bitmap is clipped (true) or repeated (false). This property is available only if the value of the fill.style property is &quot;bitmap&quot;. If the shape is smaller than the bitmap, this value is false." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS3854FE36-98DF-479c-976A-801C34C6981D.html"/>
				<variable name="bitmapPath" citdl="String" doc="A string that specifies the path and name of the bitmap fill in the Library. This property is available only if the value of the fill.style property is &quot;bitmap&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSFC4C9644-1C54-4fd4-B3AE-DBF871FB0926.html"/>
				<variable name="color" citdl="Fill" doc="The color of the fill, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bf2.html"/>
				<variable name="colorArray" citdl="Array" doc="An array of colors in the gradient, expressed as integers. This property is available only if the value of the fill.style property is either &quot;radialGradient&quot; or &quot;linearGradient&quot;. See fill.style" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bf5.html"/>
				<variable name="focalPoint" citdl="Number" doc="An integer that specifies the gradient focal point horizontal offset from the transformation point. A value of 10, for example, would place the focal point at 10/255 of the distance from the transformation point to the edge of the gradient. A value of ‑255 would place the focal point at the left boundary of the gradient. The default value is 0. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bef.html"/>
				<variable name="linearRGB" citdl="Boolean" doc="A Boolean value that specifies whether to render the fill as a linear or radial RGB gradient. Set this property to true to specify a linear interpolation of a gradient; set it to false to specify a radial interpolation of a gradient. The default value is false." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bf0.html"/>
				<variable name="matrix" citdl="Matrix" doc="A Matrix object that defines the placement, orientation, and scales for gradient fills." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bee.html"/>
				<variable name="overflow" citdl="String" doc="A string that specifies the behavior of a gradient’s overflow. Acceptable values are &quot;extend&quot;, &quot;repeat&quot;, and &quot;reflect&quot;; the strings are not case-sensitive. The default value is &quot;extend&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bec.html"/>
				<variable name="posArray" citdl="Array" doc="An array of integers, each in the range of zero to 255, indicating the position of the corresponding color. This property is available only if the value of the fill.style property is either &quot;radialGradient&quot; or &quot;linearGradient&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bf4.html"/>
				<variable name="style" citdl="String" doc="A string that specifies the fill style. Acceptable values are &quot;bitmap&quot;, &quot;solid&quot;, &quot;linearGradient&quot;, &quot;radialGradient&quot;, and &quot;noFill&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bf8.html"/>
			</scope>
			<!--

				Filter

			-->
			<scope name="Filter" ilk="class" doc="This object contains all the properties for all filters. The filter.name property specifies the type of filter, and determines which properties are applicable to each filter. See filter.name." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f85.html">
				<variable name="angle" citdl="Fl" doc="A float value that specifies the angle of the shadow or highlight color, in degrees. Acceptable values are between 0 and 360. This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;dropShadowFilter&quot;, &quot;gradientBevelFilter&quot;, or &quot;gradientGlowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7be4.html"/>
				<variable name="blurX" citdl="Fl" doc="A float value that specifies the amount to blur in the x direction, in pixels. Acceptable values are between 0 and 255. This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;blurFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientBevelFilter&quot;, or &quot;gradientGlowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7be3.html"/>
				<variable name="blurY" citdl="Fl" doc="A float value that specifies the amount to blur in the y direction, in pixels. Acceptable values are between 0 and 255. This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;blurFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientBevelFilter&quot;, or &quot;gradientGlowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7be2.html"/>
				<variable name="brightness" citdl="Fl" doc="A float value that specifies the brightness of the filter. Acceptable values are between ‑100 and 100. This property is defined for Filter objects with a value of &quot;adjustColorFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc8.html"/>
				<variable name="color" citdl="Filter" doc="The color of the filter, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7be1.html"/>
				<variable name="contrast" citdl="Fl" doc="A float value that specifies the contrast value of the filter. Acceptable values are between ‑100 and 100. This property is defined for Filter objects with a value of &quot;adjustColorFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc7.html"/>
				<variable name="distance" citdl="Fl" doc="A float value that specifies the distance between the filter’s effect and an object, in pixels. Acceptable values are from ‑255 to 255. This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;dropShadowFilter&quot;, &quot;gradientBevelFilter&quot;, or &quot;gradientGlowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7be0.html"/>
				<variable name="enabled" citdl="Boolean" doc="A Boolean value that specifies whether the specified filter is enabled (true) or disabled (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb8.html"/>
				<variable name="hideObject" citdl="Boolean" doc="A Boolean value that specifies whether the source image is hidden (true) or displayed (false). This property is defined for Filter objects with a value of &quot;dropShadowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc6.html"/>
				<variable name="highlightColor" doc="The color of the highlight, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc5.html"/>
				<variable name="hue" citdl="Fl" doc="A float value that specifies the hue of the filter. Acceptable values are between ‑180 and 180. This property is defined for Filter objects with a value of &quot;adjustColorFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc4.html"/>
				<variable name="inner" citdl="Boolean" doc="A Boolean value that specifies whether the shadow is an inner shadow (true) or not (false). This property is defined for Filter objects with a value of &quot;dropShadowFilter&quot; or &quot;glowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bdf.html"/>
				<variable name="knockout" citdl="Boolean" doc="A Boolean value that specifies whether the filter is a knockout filter (true) or not (false). This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientBevelFilter&quot;, or &quot;gradientGlowFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bde.html"/>
				<variable name="name" citdl="String" doc="Read-only property; a string that specifies the type of filter. The value of this property determines which other properties of the Filter object are available. The value is one of the following: &quot;adjustColorFilter&quot;, &quot;bevelFilter&quot;, &quot;blurFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientBevelFilter&quot;, or &quot;gradientGlowFilter&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7be8.html"/>
				<variable name="quality" citdl="String" doc="A string that specifies the blur quality. Acceptable values are &quot;low&quot;, &quot;medium&quot;, and &quot;high&quot; (&quot;high&quot; is similar to a Gaussian blur). This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;blurFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientGlowFilter&quot;, or &quot;gradientBevelFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bdc.html"/>
				<variable name="saturation" citdl="Fl" doc="A float value that specifies the saturation value of the filter. Acceptable values are from ‑100 to 100. This property is defined for Filter objects with a value of &quot;adjustColorFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc3.html"/>
				<variable name="shadowColor" doc="The color of the shadow, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc2.html"/>
				<variable name="strength" citdl="Number" doc="An integer that specifies the percentage strength of the filter. Acceptable values are between 0 and 25,500. This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;dropShadowFilter&quot;, &quot;glowFilter&quot;, &quot;gradientGlowFilter&quot;, or &quot;gradientBevelFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc1.html"/>
				<variable name="type" citdl="String" doc="A string that specifies the type of bevel or glow. Acceptable values are &quot;inner&quot;, &quot;outer&quot;, and &quot;full&quot;. This property is defined for Filter objects with a value of &quot;bevelFilter&quot;, &quot;gradientGlowFilter&quot;, or &quot;gradientBevelFilter&quot; for the filter.name property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bc0.html"/>
			</scope>
			<!--

				fl

			-->
			<scope name="fl" ilk="class" doc="The flash object represents the Flash application. You can use flash or fl to refer to this object. This documentation uses fl in code samples throughout. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa0.html">
				<variable name="actionsPanel" citdl="ActionsPanel" doc="Read-only property; an actionsPanel object, which represents the currently displayed Actions panel. For information on using this property, see actionsPanel object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fff.html"/>
				<scope name="addEventListener" ilk="function" signature="addEventListener(eventType, callbackFunction)" doc="Registers a function to be called when a specific event occurs. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb6.html">
					<scope name="eventType" ilk="argument" citdl="String" doc="A string that specifies the event type to pass to this callback function. Acceptable values are &quot;documentNew&quot;, &quot;documentOpened&quot;, &quot;documentClosed&quot;, &quot;mouseMove&quot;, &quot;documentChanged&quot;, &quot;layerChanged&quot;, and &quot;frameChanged&quot;. The documentChanged value doesn’t mean that the content of a document has changed; it means that a different document is now in the foreground. That is, fl.getDocumentDOM() will return a different value than it did before this event occurred. "/>
					<scope name="callbackFunction" ilk="argument" doc="The name of the function you want to execute every time the event occurs."/>
				</scope>
				<variable name="as3PackagePaths" citdl="String" doc="A string that corresponds to the global Classpath setting in the ActionScript 3.0 Settings dialog box. Items in the string are delimited by semi-colons. To view or change ActionScript 2.0 Classpath settings, use fl.packagePaths." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f1e.html"/>
				<scope name="browseForFileURL" ilk="function" signature="browseForFileURL(browseType [, title [, previewArea]])" doc="Opens a File Open or File Save system dialog box and lets the user specify a file to be opened or saved. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d3a.html">
					<scope name="browseType" ilk="argument" citdl="String" doc="A string that specifies the type of file browse operation. Acceptable values are &quot;open&quot;, &quot;select&quot; or &quot;save&quot;. The values &quot;open&quot; and &quot;select&quot; open the system File Open dialog box. Each value is provided for compatibility with Dreamweaver. The value &quot;save&quot; opens a system File Save dialog box. "/>
					<scope name="title" ilk="argument" citdl="String" doc="A string that specifies the title for the File Open or File Save dialog box. If this parameter is omitted, a default value is used. This parameter is optional."/>
					<scope name="previewArea" ilk="argument" citdl="Parameter" doc="An optional parameter that is ignored by Flash and Fireworks and is present only for compatibility with Dreamweaver."/>
				</scope>
				<scope name="browseForFolderURL" ilk="function" signature="browseForFolderURL([description])" doc="Displays a Browse for Folder dialog box and lets the user select a folder. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bb0.html">
					<scope name="description" ilk="argument" citdl="String" doc="An optional string that specifies the description of the Browse For Folder dialog box. If this parameter is omitted, nothing is shown in the description area."/>
				</scope>
				<scope name="clipCopyString" ilk="function" signature="clipCopyString(string)" doc="Copies the specified string to the Clipboard." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb3.html">
					<scope name="string" ilk="argument" citdl="String" doc="A string to be copied to the Clipboard."/>
				</scope>
				<scope name="closeAll" ilk="function" signature="closeAll([bPromptToSave])" doc="Closes all open files (FLA files, SWF files, JSFL files, and so on). If you want to close all open files without saving changes to any of them, pass false for bPromptToSave. This method does not terminate the application. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f1d.html">
					<scope name="bPromptToSave" ilk="argument" citdl="Boolean" doc="An optional Boolean value that specifies whether to display the Save dialog box for any files that have been changed since they were previously saved, or the Save As dialog box for files that have never been saved. The default value is true."/>
				</scope>
				<scope name="closeAllPlayerDocuments" ilk="function" returns="Boolean" signature="closeAllPlayerDocuments():Boolean" doc="Closes all the SWF files that were opened with Control &amp;gt; Test Movie." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb5.html"/>
				<scope name="closeDocument" ilk="function" signature="closeDocument(documentObject [, bPromptToSaveChanges])" doc="Closes the specified document." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bae.html">
					<scope name="documentObject" ilk="argument" citdl="Document" doc="A Document object. If documentObject refers to the active document, the Document window might not close until the script that calls this method finishes executing. "/>
					<scope name="bPromptToSaveChanges" ilk="argument" citdl="Boolean" doc="A Boolean value. When bPromptToSaveChanges is false, the user is not prompted if the document contains unsaved changes; that is, the file is closed and the changes are discarded. If bPromptToSaveChanges is true, and if the document contains unsaved changes, the user is prompted with the standard yes-or-no dialog box. The default value is true. This parameter is optional."/>
				</scope>
				<variable name="compilerErrors" citdl="CompilerErrors" doc="Read-only property; a compilerErrors object, which represents the Errors panel. For information on using this property, see compilerErrors object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ffd.html"/>
				<variable name="componentsPanel" citdl="ComponentsPanel" doc="Read-only property; a componentsPanel object, which represents the Components panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ffb.html"/>
				<variable name="configDirectory" citdl="String" doc="Read-only property; a string that specifies the full path for the local user’s Configuration directory in a platform-specific format. To specify this path as a file:/// URI, which is not platform-specific, use fl.configURI." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fe2.html"/>
				<variable name="configURI" citdl="String" doc="Read-only property; a string that specifies the full path for the local user’s Configuration directory as a file:/// URI. See also fl.configDirectory." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fe1.html"/>
				<variable name="contactSensitiveSelection" citdl="Boolean" doc="A Boolean value that specifies whether Contact Sensitive selection mode is enabled (true) or not (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cdf.html"/>
				<scope name="createDocument" ilk="function" returns="Document" signature="createDocument([docType]):Document" doc="Opens a new document and selects it. Values for size, resolution, and color are the same as the current defaults. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7bab.html">
					<scope name="docType" ilk="argument" citdl="String" doc="A string that specifies the type of document to create. Acceptable values are &quot;timeline&quot;, &quot;presentation&quot;, and &quot;application&quot;. The default value is &quot;timeline&quot;, which has the same effect as choosing File &amp;gt; New &amp;gt; Flash File (ActionScript 3.0). This parameter is optional. "/>
				</scope>
				<variable name="createNewDocList" citdl="Array" doc="Read-only property; an array of strings that represent the various types of documents that can be created." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b7b.html"/>
				<variable name="createNewDocListType" citdl="Array" doc="Read-only property; an array of strings that represent the file extensions of the types of documents that can be created. The entries in the array correspond directly (by index) to the entries in the fl.createNewDocList array." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b7a.html"/>
				<variable name="createNewTemplateList" citdl="Array" doc="Read-only property; an array of strings that represent the various types of templates that can be created. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b79.html"/>
				<variable name="documents" citdl="Array" doc="Read-only property; an array of Document objects (see Document object) that represent the documents (FLA files) that are currently open for editing. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff9.html"/>
				<scope name="downloadLatestVersion" ilk="function" returns="Boolean" signature="downloadLatestVersion(fileURI):Boolean" doc="Downloads from the Version Cue server the latest version of a file that is not currently open. To download the latest version of an open file, use document.synchronizeWithHeadVersion()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb2.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the local path of the file to be downloaded from the Version Cue server. Only files that are not already opened can be downloaded. If the file specified by fileURI is already open, this method has no effect."/>
				</scope>
				<variable name="drawingLayer" citdl="DrawingLayer" doc="Read-only property; the drawingLayer object that an extensible tool should use when the user wants to temporarily draw while dragging (for example, when creating a selection marquee)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff6.html"/>
				<variable name="externalLibraryPath" citdl="String" doc="A string that contains a list of items in the global ActionScript 3.0 External library path, which specifies the location of SWC files used as runtime shared libraries. Items in the string are delimited by semi-colons. In the authoring tool, the items are specified by choosing Edit &amp;gt; Preferences &amp;gt; ActionScript &amp;gt; ActionScript 3.0 Settings." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSEE65964E-CF35-477a-AD6C-58E70307301F.html"/>
				<scope name="fileExists" ilk="function" returns="Boolean" signature="fileExists(fileURI):Boolean" doc="Checks whether a file already exists on disk." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ba3.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that contains the path to the file. "/>
				</scope>
				<scope name="findDocumentDOM" ilk="function" returns="Document" signature="findDocumentDOM(id):Document" doc="Lets you target a specific file by using its unique identifier (instead of its index value, for example). Use this method in conjunction with document.id." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb1.html">
					<scope name="id" ilk="argument" citdl="Number" doc="An integer that represents a unique identifier for a document."/>
				</scope>
				<scope name="findDocumentIndex" ilk="function" returns="Array" signature="findDocumentIndex(name):Array" doc="Returns an array of integers that represent the position of the document name in the fl.documents array. More than one document with the same name can be open (if the documents are located in different folders)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f9a.html">
					<scope name="name" ilk="argument" citdl="Document" doc="The document name for which you want to find the index. The document must be open."/>
				</scope>
				<scope name="findObjectInDocByName" ilk="function" returns="Array" signature="findObjectInDocByName(instanceName, document):Array" doc="Exposes elements in a document with instance names that match the specified text. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb0.html">
					<scope name="instanceName" ilk="argument" citdl="String" doc="A string that specifies the instance name of an item in the specified document."/>
					<scope name="document" ilk="argument" citdl="Document" doc="The Document object in which to search for the specified item."/>
				</scope>
				<scope name="findObjectInDocByType" ilk="function" returns="Array" signature="findObjectInDocByType(elementType, document):Array" doc="Exposes elements of a specified element type in a document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7faf.html">
					<scope name="elementType" ilk="argument" citdl="String" doc="A string that represents the type of element to search for. For acceptable values, see element.elementType."/>
					<scope name="document" ilk="argument" citdl="Document" doc="The Document object in which to search for the specified item."/>
				</scope>
				<variable name="flexSDKPath" citdl="String" doc="A string that specifies the path to the Flex SDK folder, which contains bin, frameworks, lib, and other folders. In the authoring tool, the items are specified by choosing Edit &amp;gt; Preferences &amp;gt; ActionScript &amp;gt; ActionScript 3.0 Settings." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSD44C30AA-1DC9-4632-8419-F3715EDAC52B.html"/>
				<scope name="getAppMemoryInfo" ilk="function" returns="Number" signature="getAppMemoryInfo(memType):Number" doc="Method (Windows only); returns an integer that represents the number of bytes being used in a specified area of Flash.exe memory. Use the following table to determine which value you want to pass as memType:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b9f.html">
					<scope name="memType" ilk="argument" citdl="Number" doc="An integer that specifies the memory utilization area to be queried. For a list of acceptable values, see the following description."/>
				</scope>
				<scope name="getDocumentDOM" ilk="function" returns="Document" signature="getDocumentDOM():Document" doc="Retrieves the DOM (Document object) of the currently active document (FLA file). If one or more documents are open but a document does not currently have focus (for example, if a JSFL file has focus), retrieves the DOM of the most recently active document." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff8.html"/>
				<scope name="isFontInstalled" ilk="function" returns="Boolean" signature="isFontInstalled(fontName):Boolean" doc="Determines whether a specified font is installed. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS38D862B6-91B2-4b45-A30F-0FF28B881CEC.html">
					<scope name="fontName" ilk="argument" citdl="String" doc="A string that specifies the name of a device font. "/>
				</scope>
				<variable name="libraryPath" citdl="String" doc="A string that contains a list of items in the global ActionScript 3.0 Library path, which specifies the location of SWC files or folders containing SWC files. Items in the string are delimited by semi-colons. In the authoring tool, the items are specified by choosing Edit &amp;gt; Preferences &amp;gt; ActionScript &amp;gt; ActionScript 3.0 Settings." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS04A9687B-06F4-4a94-B377-AB0DA07147B0.html"/>
				<scope name="mapPlayerURL" ilk="function" returns="String" signature="mapPlayerURL(URI [, returnMBCS]):String" doc="Maps an escaped Unicode URL to a UTF-8 or MBCS URL. Use this method when the string will be used in ActionScript to access an external resource. You must use this method if you need to handle multibyte characters. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b98.html">
					<scope name="URI" ilk="argument" citdl="String" doc="A string that contains the escaped Unicode URL to map. "/>
					<scope name="returnMBCS" ilk="argument" citdl="Boolean" doc="A Boolean value that you must set to true if you want an escaped MBCS path returned. Otherwise, the method returns UTF-8. The default value is false. This parameter is optional."/>
				</scope>
				<variable name="Math" citdl="Math" doc="Read-only property; the Math object provides methods for matrix and point operations." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff1.html"/>
				<variable name="mruRecentFileList" citdl="Array" doc="Read-only property; an array of the complete filenames in the Most Recently Used (MRU) list that the Flash authoring tool manages." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b70.html"/>
				<variable name="mruRecentFileListType" citdl="Array" doc="Read-only property; an array of the file types in the MRU list that the Flash authoring tool manages. This array corresponds to the array in the fl.mruRecentFileList property." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b6f.html"/>
				<variable name="objectDrawingMode" citdl="Boolean" doc="A Boolean value that specifies whether the object drawing mode is enabled (true) or the merge drawing mode is enabled (false). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b6e.html"/>
				<scope name="openDocument" ilk="function" returns="Document" signature="openDocument(fileURI):Document" doc="Opens a Flash document (FLA file) for editing in a new Flash Document window and gives it focus. For a user, the effect is the same as selecting File &amp;gt; Open and then selecting a file. If the specified file is already open, the window that contains the document comes to the front. The window that contains the specified file becomes the currently selected document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b97.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the name of the file to be opened. "/>
				</scope>
				<scope name="openScript" ilk="function" signature="openScript(fileURI)" doc="Opens a script (JSFL, AS, ASC) or other file (XML, TXT) in the Flash text editor." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b95.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path of the JSFL, AS, ASC, XML, TXT, or other file that should be loaded into the Flash text editor. "/>
				</scope>
				<variable name="outputPanel" citdl="OutputPanel" doc="Read-only property; reference to the outputPanel object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fef.html"/>
				<variable name="packagePaths" citdl="String" doc="A string that corresponds to the global Classpath setting in the ActionScript 2.0 Settings dialog box. Class paths within the string are delimited with semi-colons (;). To view or change ActionScript 3.0 Classpath settings, use fl.as3PackagePaths." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fb7.html"/>
				<variable name="presetPanel" citdl="PresetPanel" doc="Read-only property: a presetPanel object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSE0BA9AEC-7F66-47aa-87F5-E3851964A9ED.html"/>
				<scope name="quit" ilk="function" signature="quit([bPromptIfNeeded])" doc="Quits Flash, prompting the user to save any changed documents. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b94.html">
					<scope name="bPromptIfNeeded" ilk="argument" citdl="Boolean" doc="A Boolean value that is true (default) if you want the user to be prompted to save any modified documents. Set this parameter to false if you do not want the user to be prompted to save modified documents. In the latter case, any modifications in open documents will be discarded and the application will exit immediately. Although it is useful for batch processing, use this method with caution. This parameter is optional."/>
				</scope>
				<scope name="reloadEffects" ilk="function" signature="reloadEffects()" doc="Reloads all effects descriptors defined in the user’s Configuration Effects folder. This permits you to rapidly change the scripts during development, and it provides a mechanism to improve the effects without relaunching the application. This method works best if used in a command placed in the Commands folder." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fe0.html"/>
				<scope name="reloadTools" ilk="function" signature="reloadTools()" doc="Rebuilds the Tools panel from the toolconfig.xml file. This method is used only when creating extensible tools. Use this method when you need to reload the Tools panel, for example, after modifying the JSFL file that defines a tool that is already present in the panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fdf.html"/>
				<scope name="removeEventListener" ilk="function" returns="Boolean" signature="removeEventListener(eventType):Boolean" doc="Unregisters a function that was registered using fl.addEventListener()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fae.html">
					<scope name="eventType" ilk="argument" citdl="String" doc="A string that specifies the event type to remove from this callback function. Acceptable values are &quot;documentNew&quot;, &quot;documentOpened&quot;, &quot;documentClosed&quot;, &quot;mouseMove&quot;, &quot;documentChanged&quot;, &quot;layerChanged&quot;, and &quot;frameChanged&quot;."/>
				</scope>
				<scope name="resetAS3PackagePaths" ilk="function" returns="Path" signature="resetAS3PackagePaths():Path" doc="The following example resets the ActionScript 3.0 Classpath setting to its default value." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f1f.html"/>
				<scope name="resetPackagePaths" ilk="function" returns="Path" signature="resetPackagePaths():Path" doc="The following example resets the ActionScript 2.0 Classpath setting to its default value." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f20.html"/>
				<scope name="revertDocument" ilk="function" returns="Boolean" signature="revertDocument(documentObject):Boolean" doc="Reverts the specified FLA document to its last saved version. Unlike the File &amp;gt; Revert menu option, this method does not display a warning window that asks the user to confirm the operation. See also document.revert() and document.canRevert()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e34.html">
					<scope name="documentObject" ilk="argument" citdl="Document" doc="A Document object. If documentObject refers to the active document, the Document window might not revert until the script that calls this method finishes executing. "/>
				</scope>
				<scope name="revertDocumentToLastVersion" ilk="function" returns="Boolean" signature="revertDocumentToLastVersion(documentObject):Boolean" doc="If the file can be reverted, reverts the specified document to the version on the Version Cue server and logs any errors to the Output panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fad.html">
					<scope name="documentObject" ilk="argument" citdl="Document" doc="A Document object."/>
				</scope>
				<scope name="runScript" ilk="function" returns="String" signature="runScript(fileURI [, funcName [, arg1, arg2, ...]]):String" doc="Executes a JavaScript file. If a function is specified as one of the arguments, it runs the function and also any code in the script that is not within the function. The rest of the code in the script runs before the function is run." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fde.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the name of the script file to execute. "/>
					<scope name="funcName" ilk="argument" citdl="String" doc="A string that identifies a function to execute in the JSFL file that is specified in fileURI. This parameter is optional."/>
					<scope name="arg" ilk="argument" citdl="Parameter" doc="An optional parameter that specifies one or more arguments to be passed to funcname."/>
				</scope>
				<scope name="saveAll" ilk="function" signature="saveAll()" doc="Saves all open documents. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d10.html"/>
				<scope name="saveAVersionOfDocument" ilk="function" returns="Boolean" signature="saveAVersionOfDocument(document):Boolean" doc="If the file can be saved to the Version Cue server, displays a dialog box to let the user enter version comments, saves a version of the specified document to the server, and logs any errors to the Output panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fac.html">
					<scope name="document" ilk="argument" citdl="Document" doc="A Document object."/>
				</scope>
				<scope name="saveDocument" ilk="function" returns="Boolean" signature="saveDocument(document [, fileURI]):Boolean" doc="Saves the specified document as a FLA document. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d12.html">
					<scope name="document" ilk="argument" citdl="Document" doc="A Document object that specifies the document to be saved. If document is null, the active document is saved."/>
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the name of the saved document. If the fileURI parameter is null or omitted, the document is saved with its current name. This parameter is optional."/>
				</scope>
				<scope name="saveDocumentAs" ilk="function" returns="Boolean" signature="saveDocumentAs(document):Boolean" doc="Displays the Save As dialog box for the specified document." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d11.html">
					<scope name="document" ilk="argument" citdl="Document" doc="A Document object that specifies the document to save. If document is null, the active document is saved."/>
				</scope>
				<variable name="scriptURI" citdl="String" doc="Read-only property; a string that represents the path of the currently running JSFL script, expressed as a file:/// URI. If the script was called from fl.runScript(), this property represents the path of the immediate parent script. That is, it doesn’t traverse multiple calls to fl.runScript() to find the path of the original calling script." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fab.html"/>
				<scope name="selectElement" ilk="function" returns="Boolean" signature="selectElement(elementObject, editMode):Boolean" doc="Enables selection or editing of an element. Generally, you will use this method on objects returned by fl.findObjectInDocByName() or fl.findObjectInDocByType(). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7faa.html">
					<scope name="elementObject" ilk="argument" citdl="Element" doc="The Element object you want to select."/>
					<scope name="editMode" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether you want to edit the element (true) or want only to select it (false)."/>
				</scope>
				<scope name="selectTool" ilk="function" returns="Tools" signature="selectTool(toolName):Tools" doc="The following example selects the Pen tool." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f22.html">
					<scope name="toolName" ilk="argument" citdl="String" doc="A string that specifies the name of the tool to select. See “Description” below for information on acceptable values for this parameter."/>
				</scope>
				<scope name="setActiveWindow" ilk="function" signature="setActiveWindow(document [, bActivateFrame])" doc="Sets the active window to be the specified document. This method is also supported by Dreamweaver and Fireworks. If the document has multiple views (created by Window &amp;gt; Duplicate Window), the most recently active view is selected. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b8a.html">
					<scope name="document" ilk="argument" citdl="Document" doc="A Document object that specifies the document to select as the active window."/>
					<scope name="bActivateFrame" ilk="argument" citdl="Parameter" doc="An optional parameter that is ignored by Flash and Fireworks and is present only for compatibility with Dreamweaver. "/>
				</scope>
				<scope name="showIdleMessage" ilk="function" signature="showIdleMessage(show)" doc="Lets you disable the warning about a script running too long (pass false for show). You might want to do this when processing batch operations that take a long time to complete. To re-enable the alert, issue the command again, this time passing true for show." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b87.html">
					<scope name="show" ilk="argument" citdl="Boolean" doc="A Boolean value specifying whether to enable or disable the warning about a script running too long."/>
				</scope>
				<variable name="sourcePath" citdl="String" doc="A string that contains a list of items in the global ActionScript 3.0 Source path, which specifies the location of ActionScript class files. Items in the string are delimited by semi-colons. In the authoring tool, the items are specified by choosing Edit &amp;gt; Preferences &amp;gt; ActionScript &amp;gt; ActionScript 3.0 Settings." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSE9379F20-A29F-4e9d-80F6-01945A82DFE1.html"/>
				<variable name="swfPanels" citdl="Array" doc="Read-only property; an array of registered swfPanel objects (see swfPanel object). A swfPanel object is registered if it has been opened at least once. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSAA9E5A2A-9254-48fb-B0FF-671673D3C93E.html"/>
				<scope name="synchronizeDocumentWithHeadVersion" ilk="function" returns="Boolean" signature="synchronizeDocumentWithHeadVersion(documentObject):Boolean" doc="Synchronizes the specified document with the most current version on the Version Cue server and logs any errors to the Output panel. This method is identical to document.synchronizeWithHeadVersion(). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa9.html">
					<scope name="documentObject" ilk="argument" citdl="Document" doc="A Document object."/>
				</scope>
				<variable name="tools" citdl="Array" doc="Read-only property; an array of Tools objects (see Tools object). This property is used only when creating extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7feb.html"/>
				<scope name="trace" ilk="function" signature="trace(message)" doc="Sends a text string to the Output panel, terminated by a new line, and displays the Output panel if it is not already visible. This method is identical to outputPanel.trace() and works in the same way as the trace() statement in ActionScript. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b85.html">
					<scope name="message" ilk="argument" citdl="String" doc="A string that appears in the Output panel. "/>
				</scope>
				<variable name="version" citdl="String" doc="Read-only property; the long string version of the Flash authoring tool, including platform. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b6a.html"/>
				<variable name="xmlui" citdl="Object" doc="Read-only property; an XMLUI object. This property lets you get and set XMLUI properties in a XMLUI dialog box and lets you accept or cancel the dialog box programmatically." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fe9.html"/>
			</scope>
			<!--

				FLfile

			-->
			<scope name="FLfile" ilk="class" doc="The FLfile object lets you write Flash extensions that can access, modify, and remove files and folders on the local file system. The FLfile API is provided in the form of an extension to the JavaScript API. This extension is called a shared library and is located in the following folder: " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa1.html">
				<scope name="copy" ilk="function" returns="Boolean" signature="copy(fileURI, copyURI):Boolean" doc="Copies a file from one location to another. This method returns false if copyURI already exists. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae9.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the file you want to copy."/>
					<scope name="copyURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the location and name of the copied file."/>
				</scope>
				<scope name="createFolder" ilk="function" returns="Boolean" signature="createFolder(folderURI):Boolean" doc="Creates one or more folders at the specified location. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae8.html">
					<scope name="folderURI" ilk="argument" doc="A folder URI that specifies the folder structure you want to create."/>
				</scope>
				<scope name="exists" ilk="function" returns="Boolean" signature="exists(fileURI):Boolean" doc="Determines whether a specified file exists. If you specify a folder and a filename, the folder must already exist. To create folders, see FLfile.createFolder()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae7.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the file you want to verify."/>
				</scope>
				<scope name="getAttributes" ilk="function" returns="String" signature="getAttributes(fileOrFolderURI):String" doc="Returns a string representing the attributes of the specified file or folder, or an empty string if the file has no specific attributes (that it, it is not read-only, not hidden, and so on). You should always use FLfile.exists() to test for the existence of a file or folder before using this method." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae6.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file or folder whose attributes you want to retrieve."/>
				</scope>
				<scope name="getCreationDate" ilk="function" returns="String" signature="getCreationDate(fileOrFolderURI):String" doc="Specifies how many seconds have passed between January 1, 1970 and the time the file or folder was created. This method is used primarily to compare the creation or modification dates of files or folders." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae5.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file or folder whose creation date and time you want to retrieve as a hexadecimal string."/>
				</scope>
				<scope name="getCreationDateObj" ilk="function" returns="Date" signature="getCreationDateObj(fileOrFolderURI):Date" doc="Returns a JavaScript Date object that represents the date and time when the specified file or folder was created." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae4.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file or folder whose creation date and time you want to retrieve as a JavaScript Date object."/>
				</scope>
				<scope name="getModificationDate" ilk="function" returns="String" signature="getModificationDate(fileOrFolderURI):String" doc="Specifies how many seconds have passed between January 1, 1970 and the time the file or folder was last modified. This method is used primarily to compare the creation or modification dates of files or folders." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae3.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file whose modification date and time you want to retrieve as a hexadecimal string."/>
				</scope>
				<scope name="getModificationDateObj" ilk="function" returns="Date" signature="getModificationDateObj(fileOrFolderURI):Date" doc="Returns a JavaScript Date object that represents the date and time when the specified file or folder was last modified." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae2.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file or folder whose modification date and time you want to retrieve as a JavaScript Date object."/>
				</scope>
				<scope name="getSize" ilk="function" returns="Number" signature="getSize(fileURI):Number" doc="Returns an integer that represents the size of the specified file, in bytes, or 0 if the file doesn’t exist. If the return value is 0, you can use FLfile.exists() to determine whether the file is a zero-byte file or the file doesn’t exist." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae1.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file whose size you want to retrieve."/>
				</scope>
				<scope name="listFolder" ilk="function" returns="Array" signature="listFolder(folderURI [, filesOrDirectories]):Array" doc="Returns an array of strings representing the contents of the folder." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ae0.html">
					<scope name="folderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the folder whose contents you want to retrieve. You can include a wildcard mask as part of folderURI. Valid wildcards are * (matches one or more characters) and ? (matches a single character)."/>
					<scope name="filesOrDirectories" ilk="argument" citdl="String" doc="An optional string that specifies whether to return only filenames or only folder (directory) names. If omitted, both filenames and folder names are returned. Acceptable values are &quot;files&quot; and &quot;directories&quot;."/>
				</scope>
				<scope name="platformPathToURI" ilk="function" returns="String" signature="platformPathToURI(fileName):String" doc="Converts a filename in a platform-specific format to a file:/// URI." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB3C34277-77BE-4f0e-AAD5-15AD5744D193.html">
					<scope name="fileName" ilk="argument" citdl="String" doc="A string, expressed in a platform-specific format, specifying the filename you want to convert."/>
				</scope>
				<scope name="read" ilk="function" returns="String" signature="read():String" doc="Returns the contents of the specified file as a string, or null if the read fails." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7adf.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file or folder whose attributes you want to retrieve."/>
				</scope>
				<scope name="remove" ilk="function" returns="Boolean" signature="remove(fileOrFolderURI):Boolean" doc="Deletes the specified file or folder. If the folder contains files, those files will be deleted as well. Files with the R (read-only) attribute cannot be removed. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ade.html">
					<scope name="fileOrFolderURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file or folder you want to remove (delete)."/>
				</scope>
				<scope name="setAttributes" ilk="function" returns="Boolean" signature="setAttributes(fileURI, strAttrs):Boolean" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7add.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file whose attributes you want to set."/>
					<scope name="strAttrs" ilk="argument" citdl="String" doc="A string specifying values for the attribute(s) you want to set. For acceptable values for strAttrs, see the “Description” section below."/>
				</scope>
				<scope name="uriToPlatformPath" ilk="function" returns="String" signature="uriToPlatformPath(fileURI):String" doc="Converts a filename expressed as a file:/// URI to a platform-specific format." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS67F11A16-77DB-48b2-A7B4-9531FBE4D332.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the filename you want to convert."/>
				</scope>
				<scope name="write" ilk="function" returns="Boolean" signature="write(fileURI, textToWrite, [ , strAppendMode]):Boolean" doc="Writes the specified string to the specified file (as UTF-8). If the specified file does not exist, it is created. However, the folder in which you are placing the file must exist before you use this method. To create folders, use FLfile.createFolder()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7adc.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, specifying the file to which you want to write."/>
					<scope name="textToWrite" ilk="argument" citdl="String" doc="A string representing the text you want to place in the file."/>
					<scope name="strAppendMode" ilk="argument" citdl="String" doc="An optional string with the value &quot;append&quot;, which specifies that you want to append textToWrite to the existing file. If omitted, fileURI is overwritten with textToWrite."/>
				</scope>
			</scope>
			<!--

				FolderItem

			-->
			<scope name="FolderItem" ilk="class" classrefs="Item" doc="The folderItem object is a subclass of the Item object. There are no unique methods or properties for folderItem. See Item object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f7c.html"/>
			<!--

				FontItem

			-->
			<scope name="FontItem" ilk="class" classrefs="Item" doc="The fontItem object is a subclass of the Item object (see Item object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f7a.html">
				<variable name="bitmap" citdl="Boolean" doc="A Boolean value that specifies whether the Font item is bitmapped (true) or not (false). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA942804D-61EC-4b5f-A01D-CAB27B79EB5D.html"/>
				<variable name="bold" citdl="Boolean" doc="A Boolean value that specifies whether the Font item is bold (true) or not (false). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS4160538F-321F-4e9d-A867-577878A95480.html"/>
				<variable name="embedVariantGlyphs" citdl="Boolean" doc="A Boolean value that specifies whether variant glyphs should be output in the font when publishing a SWF file (true) or not (false). Setting this value to true increases the size of your SWF file. The default value is false." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS13F81583-8A69-4960-B689-881C7F3A9A9A.html"/>
				<variable name="font" doc="" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA3A1E62F-E406-4c24-ADEC-BB2FB0C4FC7A.html"/>
				<variable name="isDefineFont4Symbol" citdl="Boolean" doc="A Boolean value that specifies the format of the font that is output when publishing a SWF file. If this value is true, Flash outputs a font that can be used with the flash.text.engine (FTE) APIs. If this value is false, the font can be used with the flash.text APIs, including text fields. The default value is false." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS4B420807-6786-46e9-94AE-B39155227EDC.html"/>
				<variable name="italic" citdl="Boolean" doc="A Boolean value that specifies whether the Font item is italic (true) or not (false). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSC36B1134-2A5F-4223-A40E-F9409C73976F.html"/>
				<variable name="size" citdl="Number" doc="An integer that represents the size of the Font item, in points." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB2F4ADED-5E3B-49fb-9231-F531962884DC.html"/>
			</scope>
			<!--

				Frame

			-->
			<scope name="Frame" ilk="class" doc="The Frame object represents frames in the layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f6e.html">
				<variable name="actionScript" citdl="String" doc="A string that represents ActionScript code. To insert a new line character, use &quot;\n&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a95.html"/>
				<variable name="duration" citdl="Number" doc="Read-only property; an integer that represents the number of frames in a frame sequence." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7abf.html"/>
				<variable name="elements" citdl="Array" doc="Read-only property; an array of Element objects (see Element object). The order of elements is the order in which they are stored in the FLA file. If there are multiple shapes on the Stage, and each is ungrouped, Flash treats them as one element. If each shape is grouped, so there are multiple groups on the Stage, Flash sees them as separate elements. In other words, Flash treats raw, ungrouped shapes as a single element, regardless of how many separate shapes are on the Stage. If a frame contains three raw, ungrouped shapes, for example, then elements.length in that frame returns a value of 1. To work around this issue, select each shape individually and group it ." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f6b.html"/>
				<scope name="getCustomEase" ilk="function" returns="Array" signature="getCustomEase([property]):Array" doc="Returns an array of objects that represent the control points for the cubic Bézier curve that defines the ease curve." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a96.html">
					<scope name="property" ilk="argument" citdl="String" doc="An optional string that specifies the property for which you want to return the custom ease value. Acceptable values are &quot;all&quot;, &quot;position&quot;, &quot;rotation&quot;, &quot;scale&quot;, &quot;color&quot;, and &quot;filters&quot;. The default value is &quot;all&quot;."/>
				</scope>
				<variable name="hasCustomEase" citdl="Boolean" doc="A Boolean value. If true, the frame gets its ease information from the custom ease curve. If false, the frame gets its ease information from the ease value." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a92.html"/>
				<variable name="labelType" citdl="String" doc="A string that specifies the type of Frame name. Acceptable values are &quot;none&quot;, &quot;name&quot;, &quot;comment&quot;, and &quot;anchor&quot;. Setting a label to &quot;none&quot; clears the frame.name property. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7abd.html"/>
				<variable name="motionTweenOrientToPath" citdl="Boolean" doc="A Boolean value that specifies whether the tweened element rotates the element as it moves along a path to maintain its angle with respect to each point on the path (true) or whether it does not rotate (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7abb.html"/>
				<variable name="motionTweenRotate" citdl="String" doc="A string that specifies how the tweened element rotates. Acceptable values are &quot;none&quot;, &quot;auto&quot;, &quot;clockwise&quot;, and &quot;counter-clockwise&quot;. A value of &quot;auto&quot; means the object will rotate in the direction requiring the least motion to match the rotation of the object in the following keyframe. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aba.html"/>
				<variable name="motionTweenRotateTimes" citdl="Number" doc="An integer that specifies the number of times the tweened element rotates between the starting keyframe and the next keyframe." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab9.html"/>
				<variable name="motionTweenScale" citdl="Boolean" doc="A Boolean value that specifies whether the tweened element scales to the size of the object in the following keyframe, increasing its size with each frame in the tween (true), or doesn’t scale (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab8.html"/>
				<variable name="motionTweenSnap" citdl="Boolean" doc="A Boolean value that specifies whether the tweened element automatically snaps to the nearest point on the motion guide layer associated with this frame’s layer (true) or not (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab7.html"/>
				<variable name="motionTweenSync" citdl="Boolean" doc="A Boolean value that if set to true, synchronizes the animation of the tweened object with the main timeline." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab6.html"/>
				<variable name="name" citdl="String" doc="A string that specifies the name of the frame. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7abc.html"/>
				<scope name="setCustomEase" ilk="function" signature="setCustomEase(property, easeCurve)" doc="Specifies an array of control point and tangent endpoint coordinates that describe a cubic Bézier curve to be used as a custom ease curve. This array is constructed by the horizontal (ordinal: left to right) position of the control points and tangent endpoints. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a93.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the property the ease curve should be used for. Acceptable values are &quot;all&quot;, &quot;position&quot;, &quot;rotation&quot;, &quot;scale&quot;, &quot;color&quot;, and &quot;filters&quot;."/>
					<scope name="easeCurve" ilk="argument" citdl="Array" doc="An array of objects that defines the ease curve. Each array element must be a JavaScript object with x and y properties."/>
				</scope>
				<variable name="shapeTweenBlend" citdl="String" doc="A string that specifies how a shape tween is blended between the shape in the keyframe at the start of the tween and the shape in the following keyframe. Acceptable values are &quot;distributive&quot; and &quot;angular&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab4.html"/>
				<variable name="soundEffect" citdl="String" doc="A string that specifies effects for a sound that is attached directly to a frame (frame.soundLibraryItem). Acceptable values are &quot;none&quot;, &quot;left channel&quot;, &quot;right channel&quot;, &quot;fade left to right&quot;, &quot;fade right to left&quot;, &quot;fade in&quot;, &quot;fade out&quot;, and &quot;custom&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab3.html"/>
				<variable name="soundLibraryItem" citdl="Item" doc="A library item (see SoundItem object) used to create a sound. The sound is attached directly to the frame." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab2.html"/>
				<variable name="soundLoop" citdl="Number" doc="An integer value that specifies the number of times a sound that is attached directly to a frame (frame.soundLibraryItem) plays. If you want to specify a value for this property, set frame.soundLoopMode to &quot;repeat&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab1.html"/>
				<variable name="soundLoopMode" citdl="String" doc="A string that specifies whether a sound that is attached directly to a frame (frame.soundLibraryItem) should play a specific number of times or loop indefinitely. Acceptable values are &quot;repeat&quot; and &quot;loop&quot;. To specify the number of times the sound should play, set a value for frame.soundLoop." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ab0.html"/>
				<variable name="soundName" citdl="String" doc="A string that specifies the name of a sound that is attached directly to a frame (frame.soundLibraryItem), as stored in the library." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aaf.html"/>
				<variable name="soundSync" citdl="String" doc="A string that specifies the sync behavior of a sound that is attached directly to a frame (frame.soundLibraryItem). Acceptable values are &quot;event&quot;, &quot;stop&quot;, &quot;start&quot;, and &quot;stream&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aae.html"/>
				<variable name="startFrame" citdl="Frame" doc="Read-only property; the index of the first frame in a sequence." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aad.html"/>
				<variable name="tweenEasing" citdl="Number" doc="An integer that specifies the amount of easing that should be applied to the tweened object. Acceptable values are ‑100 to 100. To begin the motion tween slowly and accelerate the tween toward the end of the animation, use a value between -1 and ‑100. To begin the motion tween rapidly and decelerate the tween toward the end of the animation, use a positive value between 1 and 100." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aac.html"/>
				<variable name="tweenType" citdl="String" doc="A string that specifies the type of tween; acceptable values are &quot;motion&quot;, &quot;shape&quot;, or &quot;none&quot;. The value &quot;none&quot; removes the motion tween. Use the timeline.createMotionTween() method to create a motion tween. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aab.html"/>
				<variable name="useSingleEaseCurve" citdl="Boolean" doc="A Boolean value. If true, a single custom ease curve is used for easing information for all properties. If false, each property has its own ease curve. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a91.html"/>
			</scope>
			<!--

				HalfEdge

			-->
			<scope name="HalfEdge" ilk="class" doc="The HalfEdge object is the directed side of the edge of a Shape object. An edge has two half edges. You can transverse the contours of a shape by “walking around” these half edges. For example, starting from a half edge, you can trace all the half edges around a contour of a shape, and return to the original half edge. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f51.html">
				<scope name="getEdge" ilk="function" returns="Edge" signature="getEdge():Edge" doc="Gets the Edge object for the HalfEdge object. See Edge object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a84.html"/>
				<scope name="getNext" ilk="function" returns="HalfEdge" signature="getNext():HalfEdge" doc="Gets the next half edge on the current contour. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a83.html"/>
				<scope name="getOppositeHalfEdge" ilk="function" returns="HalfEdge" signature="getOppositeHalfEdge():HalfEdge" doc="Gets the HalfEdge object on the other side of the edge. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a82.html"/>
				<scope name="getPrev" ilk="function" returns="HalfEdge" signature="getPrev():HalfEdge" doc="Gets the preceding HalfEdge object on the current contour. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a81.html"/>
				<scope name="getVertex" ilk="function" returns="Vertex" signature="getVertex():Vertex" doc="Gets the Vertex object at the head of the HalfEdge object. See Vertex object" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a80.html"/>
				<variable name="id" citdl="Number" doc="Read-only property; a unique integer identifier for the HalfEdge object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a78.html"/>
				<variable name="index" citdl="Number" doc="Read-only property; an integer with a value of 0 or 1 that specifies the index for this HalfEdge object in the parent edge." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a7d.html"/>
			</scope>
			<!--

				Instance

			-->
			<scope name="Instance" ilk="class" classrefs="Element" doc="Instance is a subclass of the Element object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f69.html">
				<variable name="instanceType" citdl="String" doc="Read-only property; a string that represents the type of instance. Possible values are &quot;symbol&quot;, &quot;bitmap&quot;, &quot;embedded video&quot;, &quot;linked video&quot;, &quot;video&quot;, and &quot;compiled clip&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a71.html"/>
				<variable name="libraryItem" citdl="Item" doc="A library item used to instantiate this instance. You can change this property only to another library item of the same type (that is, you cannot set a symbol instance to refer to a bitmap). See library object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a76.html"/>
			</scope>
			<!--

				Item

			-->
			<scope name="Item" ilk="class" doc="The Item object is an abstract base class. Anything in the library derives from Item. See also library object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f92.html">
				<scope name="addData" ilk="function" signature="addData(name, type, data)" doc="Adds specified data to a library item." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a65.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data."/>
					<scope name="type" ilk="argument" citdl="String" doc="A string that specifies the type of data. Valid types are &quot;integer&quot;, &quot;integerArray&quot;, &quot;double&quot;, &quot;doubleArray&quot;, &quot;string&quot;, and &quot;byteArray&quot;."/>
					<scope name="data" ilk="argument" citdl="Item" doc="The data to add to the specified library item. The type of data depends on the value of the type parameter. For example, if type is &quot;integer&quot;, the value of data must be an integer, and so on."/>
				</scope>
				<scope name="getData" ilk="function" returns="Parameter" signature="getData(name):Parameter" doc="Retrieves the value of the specified data. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a64.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to retrieve. "/>
				</scope>
				<scope name="hasData" ilk="function" returns="Boolean" signature="hasData(name):Boolean" doc="Determines whether the library item has the named data. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a63.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the data to check for in the library item. "/>
				</scope>
				<variable name="itemType" citdl="String" doc="Read-only property; a string that specifies the type of element. The value is one of the following: &quot;undefined&quot;, &quot;component&quot;, &quot;movie clip&quot;, &quot;graphic&quot;, &quot;button&quot;, &quot;folder&quot;, &quot;font&quot;, &quot;sound&quot;, &quot;bitmap&quot;, &quot;compiled clip&quot;, &quot;screen&quot;, or &quot;video&quot;. If this property is &quot;video&quot;, you can determine the type of video; see videoItem.videoType." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a6f.html"/>
				<variable name="linkageBaseClass" citdl="String" doc="A string that specifies the ActionScript 3.0 class that will be associated with the symbol. The value specified here appears in the Linkage dialog box in the authoring environment, and in other dialog boxes that include the Linkage dialog box controls, such as the Symbol Properties dialog box. (To specify this value for an ActionScript 2.0 class, use item.linkageClassName.)" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f1a.html"/>
				<variable name="linkageClassName" citdl="String" doc="A string that specifies the ActionScript 2.0 class that will be associated with the symbol. (To specify this value for an ActionScript 3.0 class, use item.linkageBaseClass.)" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a6d.html"/>
				<variable name="linkageExportForAS" citdl="Boolean" doc="A Boolean value. If this property is true, the item is exported for ActionScript. You can also set the item.linkageExportForRS and item.linkageExportInFirstFrame properties to true. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a6c.html"/>
				<variable name="linkageExportForRS" citdl="Boolean" doc="A Boolean value. If this property is true, the item is exported for run-time sharing. You can also set the item.linkageExportForAS and item.linkageExportInFirstFrame properties to true. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a6b.html"/>
				<variable name="linkageExportInFirstFrame" citdl="Boolean" doc="A Boolean value. If true, the item is exported in the first frame; if false, the item is exported in the frame of the first instance. If the item does not appear on the Stage, it isn’t exported." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a6a.html"/>
				<variable name="linkageIdentifier" citdl="String" doc="A string that specifies the name Flash will use to identify the asset when linking to the destination SWF file. Flash ignores this property if item.linkageImportForRS, item.linkageExportForAS, and item.linkageExportForRS are set to false. Conversely, this property must be set when any of those properties are set to true. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a69.html"/>
				<variable name="linkageImportForRS" citdl="Boolean" doc="A Boolean value: if true, the item is imported for run-time sharing. If this property is set to true, both item.linkageExportForAS and item.linkageExportForRS must be set to false. Also, you must specify an identifier (item.linkageIdentifier) and a URL (item.linkageURL)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a68.html"/>
				<variable name="linkageURL" citdl="String" doc="A string that specifies the URL where the SWF file containing the shared asset is located. Flash ignores this property if item.linkageImportForRS, item.linkageExportForAS, and item.linkageExportForRS are set to false. Conversely, this property must be set when any of those properties are set to true. You can specify a web URL or a filename in platform-dependent format (that is, forward slashes [/] or backward slashes [\], depending on the platform)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a67.html"/>
				<variable name="name" citdl="String" doc="A string that specifies the name of the library item, which includes the folder structure. For example, if Symbol_1 is inside a folder called Folder_1, the name property of Symbol_1 is &quot;Folder_1/Symbol_1&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a66.html"/>
				<scope name="removeData" ilk="function" signature="removeData(name)" doc="Removes persistent data from the library item." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a62.html">
					<scope name="name" ilk="argument" citdl="Item" doc="Specifies the name of the data to remove from the library item."/>
				</scope>
			</scope>
			<!--

				Layer

			-->
			<scope name="Layer" ilk="class" doc="The Layer object represents a layer in the timeline. The timeline.layers property contains an array of Layer objects, which can be accessed by fl.getDocumentDOM().getTimeline().layers. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f70.html">
				<variable name="color" citdl="Layer" doc="The color assigned to outline the layer, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a41.html"/>
				<variable name="frameCount" citdl="Number" doc="Read-only property; an integer that specifies the number of frames in the layer." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a40.html"/>
				<variable name="frames" citdl="Array" doc="Read-only property; an array of Frame objects (see Frame object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f6d.html"/>
				<variable name="height" citdl="Number" doc="An integer that specifies the percentage layer height; equivalent to the Layer height value in the Layer Properties dialog box. Acceptable values represent percentages of the default height: 100, 200, or 300. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a3e.html"/>
				<variable name="layerType" citdl="String" doc="A string that specifies the current use of the layer; equivalent to the Type setting in the Layer Properties dialog box. Acceptable values are &quot;normal&quot;, &quot;guide&quot;, &quot;guided&quot;, &quot;mask&quot;, &quot;masked&quot;, and &quot;folder&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a3d.html"/>
				<variable name="locked" citdl="Boolean" doc="A Boolean value that specifies the locked status of the layer. If set to true, the layer is locked. The default value is false. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a3c.html"/>
				<variable name="name" citdl="String" doc="A string that specifies the name of the layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a3b.html"/>
				<variable name="outline" citdl="Boolean" doc="A Boolean value that specifies the status of outlines for all objects in the layer. If set to true, all objects in the layer appear only with outlines. If false, objects appear as they were created." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a3a.html"/>
				<variable name="parentLayer" citdl="Layer" doc="A Layer object that represents the layer’s containing folder, guiding, or masking layer. The parent layer must be a folder, guide, or mask layer that precedes the layer, or the parentLayer of the preceding or following layer. Setting the layer’s parentLayer does not move the layer’s position in the list; trying to set a layer’s parentLayer to a layer that would require moving it has no effect. Uses null for a top-level layer." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a39.html"/>
				<variable name="visible" citdl="Boolean" doc="A Boolean value that specifies whether the layer’s objects on the Stage are shown or hidden. If set to true, all objects in the layer are visible; if false, they are hidden. The default value is true." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a38.html"/>
			</scope>
			<!--

				Library

			-->
			<scope name="Library" ilk="class" doc="The library object represents the Library panel. It is a property of the Document object (see document.library) and can be accessed by fl.getDocumentDOM().library." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f94.html">
				<scope name="addItemToDocument" ilk="function" returns="Boolean" signature="addItemToDocument(position [, namePath]):Boolean" doc="Adds the current or specified item to the Stage at the specified position." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a34.html">
					<scope name="position" ilk="argument" citdl="Object" doc="A point that specifies the x,y position of the center of the item on the Stage. "/>
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, you can specify its name and path using slash notation. If namePath is not specified, the current library selection is used. This parameter is optional."/>
				</scope>
				<scope name="addNewItem" ilk="function" returns="Boolean" signature="addNewItem(type [, namePath]):Boolean" doc="Creates a new item of the specified type in the Library panel and sets the new item to the currently selected item. For more information on importing items into the library, including items such as sounds, see document.importFile()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a70.html">
					<scope name="type" ilk="argument" citdl="String" doc="A string that specifies the type of item to create. The only acceptable values for type are &quot;video&quot;, &quot;movie clip&quot;, &quot;button&quot;, &quot;graphic&quot;, &quot;bitmap&quot;, &quot;screen&quot;, and &quot;folder&quot; (so, for example, you cannot add a sound to the library with this method). Specifying a folder path is the same as using library.newFolder()library.newFolder() before calling this method."/>
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item to be added. If the item is in a folder, specify its name and path using slash notation. This parameter is optional."/>
				</scope>
				<scope name="deleteItem" ilk="function" returns="Boolean" signature="deleteItem([namePath]):Boolean" doc="Deletes the current items or a specified item from the Library panel. This method can affect multiple items if several are selected." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a32.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item to be deleted. If the item is in a folder, you can specify its name and path using slash notation. If you pass a folder name, the folder and all its items are deleted. If no name is specified, Flash deletes the currently selected item or items. To delete all the items in the Library panel, select all items before using this method. This parameter is optional."/>
				</scope>
				<scope name="duplicateItem" ilk="function" returns="Boolean" signature="duplicateItem( [ namePath ] ):Boolean" doc="Makes a copy of the currently selected or specified item. The new item has a default name (such as item copy) and is set as the currently selected item. If more than one item is selected, the command fails." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a31.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item to duplicate. If the item is in a folder, you can specify its name and path using slash notation. This parameter is optional."/>
				</scope>
				<scope name="editItem" ilk="function" returns="Boolean" signature="editItem([namePath]):Boolean" doc="Opens the currently selected or specified item in Edit mode. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a30.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, you can specify its name and path using slash notation. If namePath is not specified, the single selected library item opens in Edit mode. If none or more than one item in the library is currently selected, the first scene in the main timeline appears for editing. This parameter is optional."/>
				</scope>
				<scope name="expandFolder" ilk="function" returns="Boolean" signature="expandFolder(bExpand [, bRecurseNestedParents [, namePath]]):Boolean" doc="Expands or collapses the currently selected or specified folder in the library. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a2f.html">
					<scope name="bExpand" ilk="argument" citdl="Boolean" doc="A Boolean value: if true, the folder is expanded; if false (the default), the folder is collapsed."/>
					<scope name="bRecurseNestedParents" ilk="argument" citdl="Boolean" doc="A Boolean value: if true, all the folders within the specified folder are expanded or collapsed, based on the value of bExpand. The default value is false. This parameter is optional."/>
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name and, optionally, the path of the folder to expand or collapse. If this parameter is not specified, the method applies to the currently selected folder. This parameter is optional."/>
				</scope>
				<scope name="findItemIndex" ilk="function" returns="Number" signature="findItemIndex(namePath):Number" doc="Returns the library item’s index value (zero-based). The library index is flat, so folders are considered part of the main index. Folder paths can be used to specify a nested item." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a2e.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, you can specify its name and path using slash notation."/>
				</scope>
				<scope name="getItemProperty" ilk="function" returns="String" signature="getItemProperty(property):String" doc="Gets the property for the selected item. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a2d.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string. For a list of values that you can use as a property parameter, see the Property summary table for the Item object, along with property summaries for its subclasses. "/>
				</scope>
				<scope name="getItemType" ilk="function" returns="String" signature="getItemType([namePath]):String" doc="Gets the type of object currently selected or specified by a library path. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a2c.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, specify its name and path using slash notation. If namePath is not specified, Flash provides the type of the current selection. If more than one item is currently selected and no namePath is provided, Flash ignores the command. This parameter is optional."/>
				</scope>
				<scope name="getSelectedItems" ilk="function" returns="Array" doc="The following example stores the array of currently selected library items (in this case, several audio files) in the selItems variable and then changes the sampleRate property of the first audio file in the array to 11 kHz: " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a2b.html"/>
				<scope name="importEmbeddedSWF" ilk="function" signature="importEmbeddedSWF(linkageName, swfData [, libName])" doc="Imports a SWF file into the library as a compiled clip. Unlike File &amp;gt; Import &amp;gt; SWF, this method lets you embed a compiled SWF file inside the library. There is no corresponding user interface functionality, and this method must be used with an external library or DLL (see C-Level Extensibility). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a2a.html">
					<scope name="linkageName" ilk="argument" citdl="String" doc="A string that provides the name of the SWF linkage of the root movie clip. "/>
					<scope name="swfData" ilk="argument" citdl="Array" doc="An array of binary SWF data, which comes from an external library or DLL. "/>
					<scope name="libName" ilk="argument" citdl="String" doc="A string that specifies the library name for the created item. If the name is already used, the method creates an alternate name. This parameter is optional."/>
				</scope>
				<scope name="itemExists" ilk="function" returns="Boolean" signature="itemExists(namePath):Boolean" doc="Checks to see if a specified item exists in the library. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a29.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, specify its name and path using slash notation."/>
				</scope>
				<variable name="items" citdl="Array" doc="An array of item objects in the library." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f93.html"/>
				<scope name="moveToFolder" ilk="function" returns="Boolean" signature="moveToFolder(folderPath [, itemToMove [, bReplace]]):Boolean" doc="Moves the currently selected or specified library item to a specified folder. If the folderPath parameter is empty, the items move to the top level. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a28.html">
					<scope name="folderPath" ilk="argument" citdl="String" doc="A string that specifies the path to the folder in the form &quot;FolderName&quot; or &quot;FolderName/FolderName&quot;. To move an item to the top level, specify an empty string (&quot;&quot;) for folderPath. "/>
					<scope name="itemToMove" ilk="argument" citdl="String" doc="A string that specifies the name of the item to move. If itemToMove is not specified, the currently selected items move. This parameter is optional."/>
					<scope name="bReplace" ilk="argument" citdl="Boolean" doc="A Boolean value. If an item with the same name already exists, specifying true for the bReplace parameter replaces the existing item with the item being moved. If false, the name of the dropped item changes to a unique name. The default value is false. This parameter is optional."/>
				</scope>
				<scope name="newFolder" ilk="function" returns="Boolean" signature="newFolder([folderPath]):Boolean" doc="Creates a new folder with the specified name, or a default name (&quot;untitled folder #&quot;) if no folderName parameter is provided, in the currently selected folder." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a27.html">
					<scope name="folderPath" ilk="argument" citdl="String" doc="A string that specifies the name of the folder to be created. If it is specified as a path, and the path doesn’t exist, the path is created. This parameter is optional."/>
				</scope>
				<scope name="renameItem" ilk="function" returns="Boolean" signature="renameItem(name):Boolean" doc="Renames the currently selected library item in the Library panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a26.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies a new name for the library item. "/>
				</scope>
				<scope name="selectAll" ilk="function" signature="selectAll([bSelectAll])" doc="Selects or deselects all items in the library. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a25.html">
					<scope name="bSelectAll" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to select or deselect all items in the library. Omit this parameter or use the default value of true to select all the items in the library; false deselects all library items. This parameter is optional."/>
				</scope>
				<scope name="selectItem" ilk="function" returns="Boolean" signature="selectItem(namePath [, bReplaceCurrentSelection [, bSelect]]):Boolean" doc="Selects a specified library item. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a24.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, you can specify its name and path using slash notation. "/>
					<scope name="bReplaceCurrentSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to replace the current selection or add the item to the current selection. The default value is true (replace current selection). This parameter is optional."/>
					<scope name="bSelect" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to select or deselect an item. The default value is true (select). This parameter is optional."/>
				</scope>
				<scope name="selectNone" ilk="function" signature="selectNone()" doc="Deselects all the library items." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a23.html"/>
				<scope name="setItemProperty" ilk="function" signature="setItemProperty(property, value)" doc="Sets the property for all selected library items (ignoring folders)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a22.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that is the name of the property to set. For a list of properties, see the Property summary table for the Item object and property summaries for its subclasses. To see which objects are subclasses of the Item object, see Summary of the DOM structure."/>
					<scope name="value" ilk="argument" citdl="value" doc="The value to assign to the specified property."/>
				</scope>
				<scope name="updateItem" ilk="function" returns="Boolean" signature="updateItem([namePath]):Boolean" doc="Updates the specified item. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a21.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the name of the item. If the item is in a folder, specify its name and path using slash notation. This is the same as right-clicking on an item and selecting Update from the menu in the user interface. If no name is provided, the current selection is updated. This parameter is optional."/>
				</scope>
			</scope>
			<!--

				Math

			-->
			<scope name="Math" ilk="class" doc="The Math object is available as a read-only property of the flash object; see fl.Math. This object provides methods that perform common mathematical operations." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff2.html">
				<scope name="concatMatrix" ilk="function" returns="Object" signature="concatMatrix(mat1, mat2):Object" doc="Performs a matrix concatenation and returns the result. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a16.html">
					<scope name="mat1, mat2" ilk="argument" citdl="Matrix" doc="Specify the Matrix objects to be concatenated (see Matrix object). Each parameter must be an object with fields a, b, c, d, tx, and ty."/>
				</scope>
				<scope name="invertMatrix" ilk="function" returns="Matrix" signature="invertMatrix(mat):Matrix" doc="Returns the inverse of the specified matrix. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a15.html">
					<scope name="mat" ilk="argument" citdl="Matrix" doc="Indicates the Matrix object to invert (see Matrix object). It must have the following fields: a, b, c, d, tx, and ty. "/>
				</scope>
				<scope name="pointDistance" ilk="function" returns="Number" signature="pointDistance(pt1, pt2):Number" doc="Computes the distance between two points. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a14.html">
					<scope name="pt1, pt2" ilk="argument" citdl="Object" doc="Specify the points between which distance is measured."/>
				</scope>
			</scope>
			<!--

				Matrix

			-->
			<scope name="Matrix" ilk="class" doc="The Matrix object represents a transformation matrix. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f84.html">
				<variable name="a" citdl="Number" doc="A floating-point value that specifies the (0,0) element in the transformation matrix. This value represents the scale factor of the object’s x-axis. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a0b.html"/>
				<variable name="b" citdl="Number" doc="A floating-point value that specifies the (0,1) element in the matrix. This value represents the vertical skew of a shape; it causes Flash to move the shape’s right edge along the vertical axis. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a10.html"/>
				<variable name="c" citdl="Number" doc="A floating-point value that specifies the (1,0) element in the matrix. This value causes Flash to skew the object by moving its bottom edge along a horizontal axis. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a0f.html"/>
				<variable name="d" citdl="Number" doc="A floating-point value that specifies the (1,1) element in the matrix. This value represents the scale factor of the object’s y-axis. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a0e.html"/>
				<variable name="tx" citdl="Number" doc="A floating-point value that specifies the x-axis location of a symbol’s registration point (also origin point or zero point) or the center of a shape. It defines the x translation of the transformation. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a0d.html"/>
				<variable name="ty" citdl="Number" doc="A floating-point value that specifies the y-axis location of a symbol’s registration point or the center of a shape. It defines the y translation of the transformation. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a0c.html"/>
			</scope>
			<!--

				OutputPanel

			-->
			<scope name="OutputPanel" ilk="class" doc="This object represents the Output panel, which displays troubleshooting information such as syntax errors. To access this object, use fl.outputPanel (or flash.outputPanel). See fl.outputPanel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7ff0.html">
				<scope name="clear" ilk="function" signature="clear()" doc="Clears the contents of the Output panel. You can use this method in a batch processing application to clear a list of errors, or to save them incrementally by using this method with outputPanel.save()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a04.html"/>
				<scope name="save" ilk="function" signature="save(fileURI [, bAppendToFile [ , bUseSystemEncoding]])" doc="Saves the contents of the Output panel to a local text file, either by overwriting the file or by appending to the file. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a03.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the local file to contain the contents of the Output panel."/>
					<scope name="bAppendToFile" ilk="argument" citdl="Boolean" doc="An optional Boolean value. If true, it appends the Output panel’s contents to the output file, and if false, the method overwrites the output file if it already exists. The default value is false. "/>
					<scope name="bUseSystemEncoding" ilk="argument" citdl="Boolean" doc="An optional Boolean value. If true, it saves the Output panel text using the system encoding; if false, it saves the Output panel text using UTF-8 encoding, with Byte Order Mark characters at the beginning of the text. The default value is false. "/>
				</scope>
				<scope name="trace" ilk="function" signature="trace(message)" doc="Sends a text string to the Output panel, terminated by a new line, and displays the Output panel if it is not already visible. This method is identical to fl.trace(), and works in the same way as the trace() statement in ActionScript." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7b27.html">
					<scope name="message" ilk="argument" citdl="String" doc="A string that contains the text to add to the Output panel. "/>
				</scope>
			</scope>
			<!--

				Oval

			-->
			<scope name="Oval" ilk="class" doc="The Oval object is a shape that is drawn using the Oval Primitive tool. To determine if an item is an Oval object, use shape.isOvalObject." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd9.html">
				<variable name="closePath" citdl="Boolean" doc="Read-only property; a Boolean value that specifies whether the Close Path check box in the Property inspector is selected. If the start angle and end angle values for the object are the same, setting this property has no effect until the values change." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79fb.html"/>
				<variable name="endAngle" citdl="Fl" doc="Read-only property; a float value that specifies the end angle of the Oval object. Acceptable values are from 0 to 360." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79fa.html"/>
				<variable name="innerRadius" citdl="Fl" doc="Read-only property; a float value that specifies the inner radius of the Oval object as a percentage. Acceptable values are from 0 to 99." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79f9.html"/>
				<variable name="startAngle" citdl="Fl" doc="Read-only property; a float value that specifies the start angle of the Oval object. Acceptable values are from 0 to 360." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79f8.html"/>
			</scope>
			<!--

				Parameter

			-->
			<scope name="Parameter" ilk="class" doc="The Parameter object type is accessed from the screen.parameters array (which corresponds to the screen Property inspector in the Flash authoring tool) or by the componentInstance.parameters array (which corresponds to the component Property inspector in the authoring tool). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f61.html">
				<variable name="category" citdl="String" doc="A string that specifies the category property for the screen parameter or componentInstance parameter. This property provides an alternative way of presenting a list of parameters. This functionality is not available through the Flash user interface. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79de.html"/>
				<scope name="insertItem" ilk="function" signature="insertItem(index, name, value, type)" doc="Inserts an item in a list, object, or array. If a parameter is a list, object, or array, the value property is an array. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e7.html">
					<scope name="index" ilk="argument" citdl="Number" doc="A zero-based integer index that indicates where the item will be inserted in the list, object, or array. If the index is 0, the item is inserted at the beginning of the list. If the index is greater than the list size, the new item is inserted at the end of the array. "/>
					<scope name="name" ilk="argument" citdl="String" doc=" A string that specifies the name of the item to insert. This is a required parameter for object parameters."/>
					<scope name="value" ilk="argument" citdl="String" doc=" A string that specifies the value of the item to insert."/>
					<scope name="type" ilk="argument" citdl="String" doc=" A string that specifies the type of item to insert. "/>
				</scope>
				<variable name="listIndex" citdl="value" doc="The value of the selected list item. This property is valid only if parameter.valueType is &quot;List&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e5.html"/>
				<variable name="name" citdl="String" doc="Read-only property; a string that specifies the name of the parameter. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e4.html"/>
				<scope name="removeItem" ilk="function" signature="removeItem(index)" doc="Removes an element of the list, object, or array type of a screen or component parameter. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e6.html">
					<scope name="index" ilk="argument" citdl="Number" doc=" The zero-based integer index of the item to be removed from the screen or component property."/>
				</scope>
				<variable name="value" citdl="Value" doc="Corresponds to the Value field in the Parameters tab of the Component inspector, the Parameters tab of the Property inspector, or the screen Property inspector. The type of the value property is determined by the valueType property for the parameter (see parameter.valueType). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e3.html"/>
				<variable name="valueType" citdl="String" doc="Read-only property; a string that indicates the type of the screen or component parameter. The type can be one of the following values: &quot;Default&quot;, &quot;Array&quot;, &quot;Object&quot;, &quot;List&quot;, &quot;String&quot;, &quot;Number&quot;, &quot;Boolean&quot;, &quot;Font Name&quot;, &quot;Color&quot;, &quot;Collection&quot;, &quot;Web Service URL&quot;, or &quot;Web Service Operation&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e2.html"/>
				<variable name="verbose" citdl="Parameter" doc="Specifies where the parameter is displayed. If the value of this property is 0 (nonverbose), the parameter is displayed only in the Component inspector. If it is 1 (verbose), the parameter is displayed in the Component inspector and in the Parameters tab of the Property inspector." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79e1.html"/>
			</scope>
			<!--

				Path

			-->
			<scope name="Path" ilk="class" doc="The Path object defines a sequence of line segments (straight, curved, or both), which you typically use when creating extensible tools. The following example shows an instance of a Path object being returned from the flash object:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f3e.html">
				<scope name="addCubicCurve" ilk="function" signature="addCubicCurve(xAnchor, yAnchor, x2, y2, x3, y3, x4, y4)" doc="Appends a cubic Bézier curve segment to the path." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79db.html">
					<scope name="xAnchor" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the first control point. "/>
					<scope name="yAnchor" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the first control point. "/>
					<scope name="x2" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the second control point. "/>
					<scope name="y2" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the second control point. "/>
					<scope name="x3" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the third control point. "/>
					<scope name="y3" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the third control point. "/>
					<scope name="x4" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the fourth control point. "/>
					<scope name="y4" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the fourth control point. "/>
				</scope>
				<scope name="addCurve" ilk="function" signature="addCurve(xAnchor, yAnchor, x2, y2, x3, y3)" doc="Appends a quadratic Bézier segment to the path. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79da.html">
					<scope name="xAnchor" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the first control point. "/>
					<scope name="yAnchor" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the first control point. "/>
					<scope name="x2" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the second control point. "/>
					<scope name="y2" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the second control point. "/>
					<scope name="x3" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the third control point. "/>
					<scope name="y3" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the third control point. "/>
				</scope>
				<scope name="addPoint" ilk="function" signature="addPoint(x, y)" doc="Adds a point to the path." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79d9.html">
					<scope name="x" ilk="argument" citdl="Number" doc="A floating-point number that specifies the x position of the point."/>
					<scope name="y" ilk="argument" citdl="Number" doc="A floating-point number that specifies the y position of the point. "/>
				</scope>
				<scope name="clear" ilk="function" signature="clear()" doc="Removes all points from the path. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79d8.html"/>
				<scope name="close" ilk="function" signature="close()" doc="Appends a point at the location of the first point of the path and extends the path to that point, which closes the path. If the path has no points, no points are added. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79d7.html"/>
				<scope name="makeShape" ilk="function" signature="makeShape([bSupressFill [, bSupressStroke]])" doc="Creates a shape on the Stage by using the current stroke and fill settings. The path is cleared after the shape is created. This method has two optional parameters for suppressing the fill and stroke of the resulting shape object. If you omit these parameters or set them to false, the current values for fill and stroke are used. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79d6.html">
					<scope name="bSuppressFill" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, suppresses the fill that would be applied to the shape. The default value is false. This parameter is optional."/>
					<scope name="bSupressStroke" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, suppresses the stroke that would be applied to the shape. The default value is false. This parameter is optional."/>
				</scope>
				<scope name="newContour" ilk="function" signature="newContour()" doc="Starts a new contour in the path. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79d5.html"/>
				<variable name="nPts" citdl="Number" doc="Read-only property; an integer representing the number of points in the path. A new path has 0 points." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-79d3.html"/>
			</scope>
			<!--

				PresetItem

			-->
			<scope name="PresetItem" ilk="class" doc="The presetItem object represents an item (preset or folder) in the Motion Presets panel (Window &amp;gt; Motion Presets). The array of presetItem objects is a property of the presetPanel object (presetPanel.items)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSE919B2D5-F0A1-4b9e-81C4-DBA5E7C2059F.html">
				<variable name="isDefault" citdl="Boolean" doc="Read-only property: a Boolean value that specifies whether the item is installed along with Flash (true) or is a custom item that you or someone else has created (false). If this value is true, you can consider it a “read-only” item; it can’t be moved, deleted, or have any similar operations applied to it." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSFB9CD311-E78F-4f80-A761-7C57F8249456.html"/>
				<variable name="isFolder" citdl="Boolean" doc="Read-only property: a Boolean value that specifies whether the item in the Motion Presets panel is a folder (true) or a preset (false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSDFAC3D34-E01C-4170-BFE2-B3E7919CF7E0.html"/>
				<variable name="level" citdl="Number" doc="Read-only property: an integer that specifies the level of the item in the folder structure of the Motion Presets panel. The Default Folder and Custom Presets folder are level 0." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS06C174B6-2FB7-4cb2-9D58-BB6A42C1EE1F.html"/>
				<variable name="name" citdl="String" doc="Read-only property: a string that represents the name of the preset or folder, without path information." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS9F5FB6CB-7068-4948-A83F-D1D9DD22EDF5.html"/>
				<variable name="open" doc="Read-only property: specifies whether a folder in the Motion Presets panel is currently expanded (true) or not (false). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS615B2B71-D839-4701-B759-E7C36385CD18.html"/>
				<variable name="path" citdl="String" doc="Read-only property: a string that represents the path to the item in the Motion Presets panel folder tree, and the item name. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS74E3A9E0-BC58-4f41-B88E-73F4E0648B7E.html"/>
			</scope>
			<!--

				PresetPanel

			-->
			<scope name="PresetPanel" ilk="class" doc="The presetPanel object represents the Motion Presets panel (Window &amp;gt; Motion Presets). It is a property of the flash object (fl.presetPanel)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS6F01C4D8-31E3-4d68-BB08-2E4C116DA70E.html">
				<scope name="addNewItem" ilk="function" returns="Boolean" signature="presetPanel.addNewItem( [namePath] );:Boolean" doc="If a single motion tween is currently selected on the Stage, adds that motion to the Motion Presets panel in the specified folder with the specified name. The path specified in namePath must exist in the panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS7740F0D7-4155-4fa3-A020-A2DB7EF84F7E.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the path and name of the item to add to the Motion Presets panel. This parameter is optional."/>
				</scope>
				<scope name="applyPreset" ilk="function" returns="Boolean" signature="applyPreset( [presetPath] ):Boolean" doc="Applies the specified or currently selected preset to the currently selected item on the Stage. The item must be a motion tween, a symbol, or an item that can be converted to a symbol. If the item is a motion tween, its current motion is replaced with the selected preset without requesting user confirmation. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS095450F0-1021-44c1-B6BA-1992DA125DB5.html">
					<scope name="presetPath" ilk="argument" citdl="String" doc="A string that specifies the full path and name of the preset to be applied, as it appears in the Motion Presets panel. This parameter is optional; if you don’t pass a value, the currently selected preset is applied. "/>
				</scope>
				<scope name="deleteFolder" ilk="function" returns="Boolean" signature="deleteFolder( [folderPath]):Boolean" doc="Deletes the specified folder and any of its subfolders from the folder tree of the Motion Presets panel. Any presets in the folders are also deleted. You can’t delete folders from the Default Presets folder." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSBE79787D-8288-40fe-8747-7EC50D621AA7.html">
					<scope name="folderPath" ilk="argument" citdl="String" doc="A string that specifies the folder to delete from the Motion Presets panel. This parameter is optional. "/>
				</scope>
				<scope name="deleteItem" ilk="function" returns="Boolean" signature="deleteItem( [namePath] ):Boolean" doc="Deletes the specified preset from the Motion Presets panel. If you don’t pass a value for namePath, any presets that are currently selected are deleted. You can’t delete items from the Default Presets folder." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS6AA1398C-A748-4a52-A17F-8008D8625B89.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the path and name of the item to delete from the Motion Presets panel. This parameter is optional."/>
				</scope>
				<scope name="expandFolder" ilk="function" returns="Boolean" signature="expandFolder( [bExpand [, bRecurse [, folderPath] ] ] ):Boolean" doc="Expands or collapses the currently selected folder or folders in the Motion Presets panel. To expand or collapse folders other than the folders that are currently selected, pass a value for folderPath." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS7145B563-37F0-4012-9DFA-D544CDD43539.html">
					<scope name="bExpand" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to expand the folder (true) or collapse it (false). This parameter is optional; the default value is true."/>
					<scope name="bRecurse" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to expand or collapse the folder’s subfolders (true) or not false). This parameter is optional; the default value is false."/>
					<scope name="folderPath" ilk="argument" citdl="String" doc="A string that specifies the path to the folder to expand or collapse. This parameter is optional."/>
				</scope>
				<scope name="exportItem" ilk="function" returns="Boolean" signature="exportItem(fileURI [, namePath] ):Boolean" doc="Exports the currently selected or the specified preset to an XML file. Only presets can be exported; the method fails if you try to export a folder. This method also fails if you try to overwrite a file on disk." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSDF7D0D65-3162-4136-B499-A5CE4BD355CE.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path and optionally a filename for the exported file. See “Description,” below, for more information."/>
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the path and name of the item to select from the Motion Presets panel. This parameter is optional."/>
				</scope>
				<scope name="findItemIndex" ilk="function" signature="findItemIndex([presetName])" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS4E589979-75E5-442d-BD38-E1EF95475F8C.html">
					<scope name="presetName" ilk="argument" citdl="String" doc="A string that specifies the name of the preset for which the index value is returned. This parameter is optional."/>
				</scope>
				<scope name="getSelectedItems" ilk="function" returns="Array" signature="getSelectedItems():Array" doc="Returns an array of presetItem objects corresponding to the currently selected items in the Motion Presets panel (see presetItem object). Each item in the array represents either a folder or a preset." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5BD78EB6-D4C9-4cda-8C2D-31A07B6E29F2.html"/>
				<scope name="importItem" ilk="function" returns="Boolean" signature="importItem(fileURI [,namePath ]):Boolean" doc="Adds a preset to the Motion Presets panel from a specified XML file. The path specified in namePath must exist in the panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS0C2032CE-ACD4-47d5-B489-E3221AF9A7BF.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the XML file to be imported as a preset in the Motion Presets panel. "/>
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies in which folder to place the imported file and what to name it. This parameter is optional."/>
				</scope>
				<variable name="items" citdl="Array" doc="An array of presetItem objects in the Motion Presets panel (see presetItem object). Each item in the array represents either a folder or a preset." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS2A326863-69B8-4f78-939B-48239518CA2A.html"/>
				<scope name="moveToFolder" ilk="function" returns="Boolean" signature="moveToFolder(folderPath [, namePath] ):Boolean" doc="Moves the specified item to the specified folder. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS223301A7-2E62-4af3-A3AD-671C577C199C.html">
					<scope name="folderPath" ilk="argument" citdl="String" doc="A string that specifies the path to the folder in the Motion Presets panel to which the item or items are moved. "/>
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the path and name of the item to move. This parameter is optional."/>
				</scope>
				<scope name="newFolder" ilk="function" returns="Boolean" signature="newFolder( [folderPath] ):Boolean" doc="Creates a folder in the folder tree of the Motion Presets panel. You can create only one new folder level with this method. That is, if you pass “Custom Presets/My First Folder/My Second Folder&quot; for folderPath, “Custom Presets/My First Folder“ must exist in the folder tree." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS11E427B4-7DDF-4589-9F07-A83A5B161285.html">
					<scope name="folderPath" ilk="argument" citdl="String" doc="A string that specifies where to add a new folder in the Motion Presets panel, and the name of the new folder. This parameter is optional. "/>
				</scope>
				<scope name="renameItem" ilk="function" returns="Boolean" signature="renameItem(newName):Boolean" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5D744FFA-76B2-4bc1-9A5A-0546B9425692.html">
					<scope name="newName" ilk="argument" citdl="String" doc="A string that specifies the new name for the preset or folder."/>
				</scope>
				<scope name="selectItem" ilk="function" returns="Boolean" signature="selectItem(namePath [, bReplaceCurrentSelection [, bSelect] ]):Boolean" doc="Selects or deselects an item in the Motion Presets panel, optionally replacing any items currently selected." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS42D98F58-0C4A-4343-A8AE-2B3D1A2CCE97.html">
					<scope name="namePath" ilk="argument" citdl="String" doc="A string that specifies the path and name of the item to select from the Motion Presets panel. "/>
					<scope name="bReplaceCurrentSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether the specified item replaces any current selection (true) or is added to the current selection (false). This parameter is optional; the default value is true."/>
					<scope name="bSelect" ilk="argument" citdl="Boolean" doc="A Boolean value that specifies whether to select the item (true) or deselect the item (false). This parameter is optional; the default value is true. If you pass false for bSelect, the value of bReplaceCurrentSelection is ignored."/>
				</scope>
			</scope>
			<!--

				Rectangle

			-->
			<scope name="Rectangle" ilk="class" doc="The Rectangle object is a shape that is drawn using the Rectangle Primitive tool. To determine if an item is a Rectangle object, use shape.isRectangleObject." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fd8.html">
				<variable name="bottomLeftRadius" citdl="Fl" doc="Read-only property; a float value that sets the radius of the bottom-left corner of the Rectangle object. If RectangleObject.lockFlag is true, trying to set this value has no effect. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7975.html"/>
				<variable name="bottomRightRadius" citdl="Fl" doc="Read-only property; a float value that sets the radius of the bottom-right corner of the Rectangle object. If RectangleObject.lockFlag is true, trying to set this value has no effect." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7974.html"/>
				<variable name="lockFlag" citdl="Boolean" doc="Read-only property; a Boolean value that determines whether different corners of the rectangle can have different radius values. If this value is true, all corners have the value assigned to RectangleObject.topLeftRadius. If it is false, each corner radius can be set independently." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7977.html"/>
				<variable name="topLeftRadius" citdl="Fl" doc="Read-only property; a float value that sets the radius of all corners of the rectangle (if RectangleObject.lockFlag is true) or that sets only the radius of the top-left corner (if RectangleObject.lockFlag is false)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7972.html"/>
				<variable name="topRightRadius" citdl="Fl" doc="Read-only property; a float value that sets the radius of the top-right corner of the Rectangle object. If RectangleObject.lockFlag is true, trying to set this value has no effect." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-796f.html"/>
			</scope>
			<!--

				Screen

			-->
			<scope name="Screen" ilk="class" doc="The Screen object represents a single screen in a slide or form document. This object contains properties related to the slide or form. For access to the array of all Screen objects in the document, use the following code: " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f43.html">
				<variable name="accName" citdl="String" doc="A string that is equivalent to the Name field in the Accessibility panel. Screen readers identify objects by reading the name aloud. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-793c.html"/>
				<variable name="childScreens" citdl="Array" doc="Read-only property; the array of child screens for this screen. The array is empty if there are no child screens. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-794c.html"/>
				<variable name="description" citdl="String" doc="A string that is equivalent to the Description field in the Accessibility panel. The description is read by the screen reader. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-794b.html"/>
				<variable name="forceSimple" citdl="Boolean" doc="A Boolean value that enables or disables accessibility for the object’s children. This is equivalent to the inverse logic of the Make Child Objects Accessible setting in the Accessibility panel. That is, if forceSimple is true, it is the same as the Make Child Object Accessible option being deselected. If forceSimple is false, it is the same as the Make Child Object Accessible option being selected. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-794a.html"/>
				<variable name="hidden" citdl="Boolean" doc="A Boolean value that specifies whether the screen is visible. A screen with the hidden property set to true is not visible in any other screen. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7949.html"/>
				<variable name="instanceName" citdl="String" doc="Read-only property; a string that represents the instance name used to access the object from ActionScript. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7948.html"/>
				<variable name="name" citdl="String" doc="Read-only property; a string that represents the name of the screen. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7947.html"/>
				<variable name="nextScreen" citdl="Object" doc="Read-only property; an object that represents the next peer screen in the parent’s childScreens array. That is, screen.nextScreen is found by moving down an array of child screens to the next screen in the array. See screen.prevScreen." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7946.html"/>
				<variable name="parameters" citdl="Array" doc="Read-only property; an array of ActionScript 2.0 properties that are accessible from the screen Property inspector." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f40.html"/>
				<variable name="parentScreen" citdl="Object" doc="Read-only property; an object that represents the parent screen. If parentScreen is null, the screen is a top-level screen. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7944.html"/>
				<variable name="prevScreen" citdl="Object" doc="Read-only property; an object that represents the previous peer screen in the parent’s childScreens array. If there isn’t a peer screen, the value is null. See also screen.nextScreen." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7943.html"/>
				<variable name="silent" citdl="Boolean" doc="A Boolean value that specifies whether the object is accessible. This is equivalent to the inverse logic of the Make Object Accessible setting in the Accessibility panel. That is, if silent is true, it is the same as having the Make Object Accessible option deselected in the Accessibility panel. If silent is false, it is the same as having the Make Object Accessible option selected in the Accessibility panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7940.html"/>
				<variable name="tabIndex" citdl="value" doc="Equivalent to the Tab Index field in the Accessibility panel. This value lets you determine the order in which objects are accessed when the user presses the Tab key. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-793f.html"/>
				<variable name="timeline" citdl="Timeline" doc="Read-only property; the Timeline object for the screen. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-793e.html"/>
			</scope>
			<!--

				ScreenOutline

			-->
			<scope name="ScreenOutline" ilk="class" doc="The ScreenOutline object represents the group of screens in a slide or form document. The object is accessed by using fl.getDocumentDOM().screenOutline." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f44.html">
				<scope name="copyScreenFromFile" ilk="function" signature="copyScreenFromFile(fileURI [, screenName])" doc="Inserts all the screens, or a named screen and its children, from a specified document under the currently selected screen. If more than one screen is selected, the screen(s) are inserted under the last selected screen, as its sibling. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7934.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies a filename for the authoring file that contains the screens to copy into the document."/>
					<scope name="screenName" ilk="argument" citdl="Screen" doc="The name of the screen to copy. If the screenName parameter is present, Flash copies that screen and its children. If the screenName is not specified, Flash copies the whole document. This parameter is optional."/>
				</scope>
				<variable name="currentScreen" citdl="Screen" doc="A Screen object, the currently selected screen (see Screen object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7939.html"/>
				<scope name="deleteScreen" ilk="function" signature="deleteScreen([screenName])" doc="Deletes the currently selected screen(s), or a specified screen, and the children of the screen(s). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7933.html">
					<scope name="screenName" ilk="argument" citdl="String" doc="A string that specifies the name of the screen to be deleted. If you don’t pass a value for screenName, the currently selected screen(s) and their children are deleted. This parameter is optional."/>
				</scope>
				<scope name="duplicateScreen" ilk="function" returns="Boolean" signature="duplicateScreen([screenName]):Boolean" doc="Duplicates the currently selected screen(s) or a specified screen. The duplicate screens are given a default name by appending _copy to the original name, such as Screen_copy, Screen_copy2, and so on. If you duplicate multiple screens, the duplicates are placed directly below the selected screen that is lowest in the screen outline hierarchy." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7932.html">
					<scope name="screenName" ilk="argument" citdl="String" doc="A string value that specifies the screen name to duplicate. If you don’t pass a value for screenName, the currently selected screen(s) are duplicated. This parameter is optional."/>
				</scope>
				<scope name="getSelectedScreens" ilk="function" returns="Array" signature="getSelectedScreens():Array" doc="Returns an array of Screen objects that are currently selected in the screen outline. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7931.html"/>
				<scope name="insertNestedScreen" ilk="function" returns="Screen" signature="insertNestedScreen([name [, referenceScreen [, screenTypeName]]]):Screen" doc="Inserts a nested screen of a specific type into a particular location in the screen outline." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7930.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string indicating the name of the new screen to insert. An empty name will insert a screen with a default screen name, such as Slide n or Form n (where n is the first available unique number). This parameter is optional."/>
					<scope name="referenceScreen" ilk="argument" citdl="String" doc="A string indicating the name of the screen into which the new screen is inserted as a child. If this parameter is omitted, the new screen is inserted as a child of the currently selected screen. This parameter is optional."/>
					<scope name="screenTypeName" ilk="argument" citdl="String" doc="A string that specifies the screen type to attach to the new nested screen. The screen type and class name are set for this screen. Acceptable values are &quot;Form&quot; and &quot;Slide&quot;. This parameter is optional. If this parameter is omitted, the type is inherited from the parent screen."/>
				</scope>
				<scope name="insertScreen" ilk="function" returns="Screen" signature="insertScreen([name [, referenceScreen [, screenTypeName]]]):Screen" doc="Inserts a new blank screen of a specified type into the document at a specified location. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-792f.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string indicating the name of the new screen to insert. If this parameter is omitted, the method inserts a screen with a default screen name, such as Slide n or Form n (where n is the first available unique number). This parameter is optional."/>
					<scope name="referenceScreen" ilk="argument" citdl="String" doc="A string indicating the name of the screen before the new screen. If this parameter is omitted, the new screen is inserted after the currently selected screen. If the referenceScreen parameter identifies a child screen, the new screen will be a peer of the child screen, and a child screen of the same parent. This parameter is optional."/>
					<scope name="screenTypeName" ilk="argument" citdl="String" doc="A string that specifies the screen type to attach to the new screen. The screen type and class name are set for this screen. Acceptable values are &quot;Form&quot; and &quot;Slide&quot;. This parameter is optional."/>
				</scope>
				<scope name="moveScreen" ilk="function" returns="Boolean" signature="moveScreen(screenToMove, referenceScreen, position):Boolean" doc="Moves the specified screen in relation to the value of the referenceScreen parameter; either before, after, as the first child, or as the last child. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-792e.html">
					<scope name="screenToMove" ilk="argument" citdl="String" doc="A string that is the screen name to move. "/>
					<scope name="referenceScreen" ilk="argument" citdl="String" doc="A string that specifies the screen near which screenToMove will be placed."/>
					<scope name="position" ilk="argument" citdl="String" doc="A string that specifies where to move the screen in relation to referenceScreen. Acceptable values are &quot;before&quot;, &quot;after&quot;, &quot;firstChild&quot;, and &quot;lastChild&quot;. "/>
				</scope>
				<scope name="renameScreen" ilk="function" returns="Boolean" signature="renameScreen(newScreenName [, oldScreenName[, bDisplayError]]):Boolean" doc="Changes the screen with a specified name to a new name. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-792d.html">
					<scope name="newScreenName" ilk="argument" citdl="String" doc="A string that specifies the new name of the screen."/>
					<scope name="oldScreenName" ilk="argument" citdl="String" doc="A string that specifies the name of the existing screen to change. If not specified, the name of the currently selected screen changes. This parameter is optional."/>
					<scope name="bDisplayError" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, shows an error message if an error occurs—for example, if a screen with the same name as the value passed to newScreenName already exists. The default value is false. "/>
				</scope>
				<variable name="rootScreen" citdl="Screen" doc="Read-only property; the first screen in the screen outline. You can use screenOutline.rootScreen as a shortcut for screenOutline.screens[0]. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7937.html"/>
				<variable name="screens" citdl="Array" doc="Read-only property; the array of top-level Screen objects contained in the document (see Screen object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f42.html"/>
				<scope name="setCurrentScreen" ilk="function" signature="setCurrentScreen(name)" doc="Sets the current selection in the screen outline to the specified screen." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-792c.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the screen that should become the currently selected screen. If the screen is a child of another screen, you do not need to indicate a path or hierarchy."/>
				</scope>
				<scope name="setScreenProperty" ilk="function" signature="setScreenProperty(property, value)" doc="Sets the specified property with the specified value for the selected screens. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-792b.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the property to set."/>
					<scope name="value" ilk="argument" citdl="value" doc="The new value for the property. The type of value depends on the property being set.Available properties are screenOutline.currentScreen, screenOutline.rootScreen, and screenOutline.screens. "/>
				</scope>
				<scope name="setSelectedScreens" ilk="function" signature="setSelectedScreens(selection [, bReplaceCurrentSelection])" doc="Selects the specified screens in the screen outline. If multiple screens are specified, the screen with the last index value of the selection array is focused on the Stage. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-792a.html">
					<scope name="selection" ilk="argument" citdl="Array" doc="An array of screen names to be selected in the screen outline."/>
					<scope name="bReplaceCurrentSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that, if true, lets you deselect the current selection. The default value is true. If false, Flash extends the current selection to include the specified screens. This parameter is optional."/>
				</scope>
			</scope>
			<!--

				Shape

			-->
			<scope name="Shape" ilk="class" classrefs="Element" doc="The Shape object is a subclass of the Element object. The Shape object provides more precise control than the drawing APIs when manipulating or creating geometry on the Stage. This control is necessary so that scripts can create useful effects and other drawing commands (see Element object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f57.html">
				<scope name="beginEdit" ilk="function" signature="beginEdit()" doc="Defines the start of an edit session. You must use this method before issuing any commands that change the Shape object or any of its subordinate parts. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7c53.html"/>
				<variable name="contours" citdl="Array" doc="Read-only property; an array of Contour objects for the shape (see Contour object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f52.html"/>
				<scope name="deleteEdge" ilk="function" signature="deleteEdge(index)" doc="Deletes the specified edge. You must call shape.beginEdit() before using this method. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7920.html">
					<scope name="index" ilk="argument" citdl="Number" doc="A zero-based index that specifies the edge to delete from the shape.edges array. This method changes the length of the shape.edges array. "/>
				</scope>
				<variable name="edges" citdl="Array" doc="Read-only property; an array of Edge objects (see Edge object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f4d.html"/>
				<scope name="endEdit" ilk="function" signature="endEdit()" doc="Defines the end of an edit session for the shape. All changes made to the Shape object or any of its subordinate parts will be applied to the shape. You must use this method after issuing any commands that change the Shape object or any of its subordinate parts." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-791f.html"/>
				<scope name="getCubicSegmentPoints" ilk="function" returns="Array" signature="getCubicSegmentPoints(cubicSegmentIndex):Array" doc="Returns an array of points that define a cubic curve." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS90EC1073-2937-4383-8D63-573716135062.html">
					<scope name="cubicSegmentIndex" ilk="argument" citdl="Number" doc="An integer that specifies the cubic segment for which points are returned."/>
				</scope>
				<variable name="isDrawingObject" citdl="Shape" doc="Read-only property; if true, the shape is a drawing object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d96.html"/>
				<variable name="isGroup" citdl="Shape" doc="Read-only property; if true, the shape is a group. A group can contain different types of elements, such as text elements and symbols. However, the group itself is considered a shape, and you can use the shape.isGroup property no matter what types of elements the group contains." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7917.html"/>
				<variable name="isOvalObject" citdl="Shape" doc="Read-only property; if true, the shape is a primitive Oval object (was created using the Oval Primitive tool). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa8.html"/>
				<variable name="isRectangleObject" citdl="Shape" doc="Read-only property; if true, the shape is a primitive Rectangle object (was created using the Rectangle Primitive tool). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa7.html"/>
				<variable name="members" citdl="Array" doc="Read-only property; an array of objects in the currently selected group. This property is available only if the value of shape.isGroup is true). Raw shapes in the group are not included in the shape.members array. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSFA6E92A4-EE4B-4f35-846F-AF697D217B0A.html"/>
				<variable name="numCubicSegments" citdl="Number" doc="Read-only property; the number of cubic segments in the shape." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS59D9DFE7-9D98-4892-A5FC-7EB4336D08F2.html"/>
				<variable name="vertices" citdl="Array" doc="Read-only property; an array of Vertex objects (see Vertex object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f48.html"/>
			</scope>
			<!--

				SoundItem

			-->
			<scope name="SoundItem" ilk="class" classrefs="Item" doc="The SoundItem object is a subclass of the Item object. It represents a library item used to create a sound. See also frame.soundLibraryItem and Item object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f78.html">
				<variable name="bitRate" citdl="String" doc="A string that specifies the bit rate of a sound in the library. This property is available only for the MP3 compression type. Acceptable values are &quot;8 kbps&quot;, &quot;16 kbps&quot;, &quot;20 kbps&quot;, &quot;24 kbps&quot;, &quot;32 kbps&quot;, &quot;48 kbps&quot;, &quot;56 kbps&quot;, &quot;64 kbps&quot;, &quot;80 kbps&quot;, &quot;112 kbps&quot;, &quot;128 kbps&quot;, and &quot;160 kbps&quot;. Stereo sounds exported at 8 Kbps or 16 Kbps are converted to mono. The property is undefined for other compression types." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78fe.html"/>
				<variable name="bits" citdl="String" doc="A string that specifies the bits value for a sound in the library that has ADPCM compression. Acceptable values are &quot;2 bit&quot;, &quot;3 bit&quot;, &quot;4 bit&quot;, and &quot;5 bit&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78fa.html"/>
				<variable name="compressionType" citdl="String" doc="A string that specifies that compression type for a sound in the library. Acceptable values are &quot;Default&quot;, &quot;ADPCM&quot;, &quot;MP3&quot;, &quot;Raw&quot;, and &quot;Speech&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78f9.html"/>
				<variable name="convertStereoToMono" citdl="Boolean" doc="A Boolean value available only for MP3 and Raw compression types. Setting this value to true converts a stereo sound to mono; false leaves it as stereo. For the MP3 compression type, if soundItem.bitRate is less than 20 Kbps, this property is ignored and forced to true (see soundItem.bitRate)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78f8.html"/>
				<scope name="exportToFile" ilk="function" returns="Boolean" signature="exportToFile(fileURI):Boolean" doc="Exports the specified item to a QuickTime file on the Macintosh, or to a WAV or QT file on Windows. The exported QuickTime or QT files contain only audio; video is not exported. Export settings are based on the item being exported." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSDB63C432-5816-4c62-94B9-5B3DE6DF137A.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path and name of the exported file."/>
				</scope>
				<variable name="fileLastModifiedDate" citdl="String" doc="Read-only property: a string containing a hexadecimal number that represents the number of seconds that have elapsed between January 1, 1970, and the modification date of the original file (on disk) at the time the file was imported to the library. If the file no longer exists, this value is &quot;00000000&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS26AC5ABC-5FF2-4406-827E-D5B3E9DC5B09.html"/>
				<variable name="originalCompressionType" citdl="String" doc="Read-only property: a string that specifies whether the specified item was imported as an mp3 file. Possible values for this property are “RAW” and “MP3”." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS439722E8-D974-42ca-B9B3-79B259E88494.html"/>
				<variable name="quality" citdl="String" doc="A string that specifies the playback quality of a sound in the library. This property is available only for the MP3 compression type. Acceptable values are &quot;Fast&quot;, &quot;Medium&quot;, and &quot;Best&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78f7.html"/>
				<variable name="sampleRate" citdl="String" doc="A string that specifies the sample rate for the audio clip. This property is available only for the ADPCM, Raw, and Speech compression types. Acceptable values are &quot;5 kHz&quot;, &quot;11 kHz&quot;, &quot;22 kHz&quot;, and &quot;44 kHz&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78f6.html"/>
				<variable name="sourceFileExists" citdl="Boolean" doc="Read-only property: a Boolean value of true if the file that was imported to the Library still exists in the location from where it was imported; false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS13380127-A3D3-46bb-BFAD-17C6552F95F2.html"/>
				<variable name="sourceFileIsCurrent" citdl="Boolean" doc="Read-only property: a Boolean value of true if the file modification date of the Library item is the same as the modification date on disk of the file that was imported; false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS042E2C05-E896-49db-BC6B-27EB1655D3F5.html"/>
				<variable name="sourceFilePath" citdl="String" doc="Read-only property: a string, expressed as a file:/// URI, that represents the path and name of the file that was imported into the Library. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSD34A13E3-EE45-4361-9E36-B307AC561C94.html"/>
				<variable name="useImportedMP3Quality" citdl="Boolean" doc="A Boolean value. If true, all other properties are ignored, and the imported MP3 quality is used. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78f3.html"/>
			</scope>
			<!--

				Stroke

			-->
			<scope name="Stroke" ilk="class" doc="The Stroke object contains all the settings for a stroke, including the custom settings. This object represents the information contained in the Property inspector. Using the Stroke object together with the document.setCustomStroke() method, you can change the stroke settings for the Tools panel, the Property inspector, and the current selection. You can also get the stroke settings of the Tools panel and Property inspector, or of the current selection, by using the document.getCustomStroke() method." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f82.html">
				<variable name="breakAtCorners" citdl="Boolean" doc="A Boolean value. This property is the same as the Sharp Corners setting in the custom Stroke Style dialog box. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f1c.html"/>
				<variable name="capType" citdl="String" doc="A string that specifies the type of cap for the stroke. Acceptable values are &quot;none&quot;, &quot;round&quot;, and &quot;square&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d3.html"/>
				<variable name="color" citdl="Stroke" doc="The color of the stroke, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d4.html"/>
				<variable name="curve" citdl="String" doc="A string that specifies the type of hatching for the stroke. This property can be set only if the stroke.style property is set to &quot;hatched&quot; (see stroke.style). Acceptable values are &quot;straight&quot;, &quot;slight curve&quot;, &quot;medium curve&quot;, and &quot;very curved&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d5.html"/>
				<variable name="dash1" citdl="Number" doc="An integer that specifies the lengths of the solid parts of a dashed line. This property is available only if the stroke.style property is set to dashed(see stroke.style)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d6.html"/>
				<variable name="dash2" citdl="Number" doc="An integer that specifies the lengths of the blank parts of a dashed line. This property is available only if the stroke.style property is set to dashed (see stroke.style)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d7.html"/>
				<variable name="density" citdl="String" doc="A string that specifies the density of a stippled line. This property is available only if the stroke.style property is set to stipple (see stroke.style). Acceptable values are &quot;very dense&quot;, &quot;dense&quot;, &quot;sparse&quot;, and &quot;very sparse&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d8.html"/>
				<variable name="dotSize" citdl="String" doc="A string that specifies the dot size of a stippled line. This property is available only if the stroke.style property is set to stipple (see stroke.style). Acceptable values are &quot;tiny&quot;, &quot;small&quot;, &quot;medium&quot;, and &quot;large&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d9.html"/>
				<variable name="dotSpace" citdl="Number" doc="An integer that specifies the spacing between dots in a dotted line. This property is available only if the stroke.style property is set to dotted. See stroke.style." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78da.html"/>
				<variable name="hatchThickness" citdl="String" doc="A string that specifies the thickness of a hatch line. This property is available only if the stroke.style property is set to hatched (see stroke.style). Acceptable values are &quot;hairline&quot;, &quot;thin&quot;, &quot;medium&quot;, and &quot;thick&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78db.html"/>
				<variable name="jiggle" citdl="String" doc="A string that specifies the jiggle property of a hatched line. This property is available only if the stroke.style property is set to hatched (see stroke.style). Acceptable values are &quot;none&quot;, &quot;bounce&quot;, &quot;loose&quot;, and &quot;wild&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78dc.html"/>
				<variable name="joinType" citdl="String" doc="A string that specifies the type of join for the stroke. Acceptable values are &quot;miter&quot;, &quot;round&quot;, and &quot;bevel&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78dd.html"/>
				<variable name="length" citdl="String" doc="A string that specifies the length of a hatch line. This property is available only if the stroke.style property is set to hatched (see stroke.style). Acceptable values are &quot;equal&quot;, &quot;slight variation&quot;, &quot;medium variation&quot;, and &quot;random&quot; (The value &quot;random&quot; actually maps to &quot;medium variation&quot;.)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78de.html"/>
				<variable name="miterLimit" citdl="Fl" doc="A float value that specifies the angle above which the tip of the miter will be truncated by a segment. That means the miter is truncated only if the miter angle is greater than the value of miterLimit." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78df.html"/>
				<variable name="pattern" citdl="String" doc="A string that specifies the pattern of a ragged line. This property is available only if the stroke.style property is set to ragged (see stroke.style). Acceptable values are &quot;solid&quot;, &quot;simple&quot;, &quot;random&quot;, &quot;dotted&quot;, &quot;random dotted&quot;, &quot;triple dotted&quot;, and &quot;random triple dotted&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78e0.html"/>
				<variable name="rotate" citdl="String" doc="A string that specifies the rotation of a hatch line. This property is available only if the stroke.style property is set to hatched (see stroke.style). Acceptable values are &quot;none&quot;, &quot;slight&quot;, &quot;medium&quot;, and &quot;free&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78e2.html"/>
				<variable name="scaleType" citdl="String" doc="A string that specifies the type of scale to be applied to the stroke. Acceptable values are &quot;normal&quot;, &quot;horizontal&quot;, &quot;vertical&quot;, and &quot;none&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78c1.html"/>
				<variable name="shapeFill" citdl="Fill" doc="A Fill object that represents the fill settings of the stroke." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78c2.html"/>
				<variable name="space" citdl="String" doc="A string that specifies the spacing of a hatched line. This property is available only if the stroke.style property is set to hatched (see stroke.style). Acceptable values are &quot;very close&quot;, &quot;close&quot;, &quot;distant&quot;, and &quot;very distant&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78b5.html"/>
				<variable name="strokeHinting" citdl="Boolean" doc="A Boolean value that specifies whether stroke hinting is set on the stroke." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78e3.html"/>
				<variable name="style" citdl="String" doc="A string that describes the stroke style. Acceptable values are &quot;noStroke&quot;,&quot;solid&quot;, &quot;dashed&quot;, &quot;dotted&quot;, &quot;ragged&quot;, &quot;stipple&quot;, and &quot;hatched&quot;. Some of these values require additional properties of the Stroke object to be set, as described in the following list:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78d1.html"/>
				<variable name="thickness" citdl="Number" doc="An integer that specifies the stroke size. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78bc.html"/>
				<variable name="variation" citdl="String" doc="A string that specifies the variation of a stippled line. This property is available only if the stroke.style property is set to stipple (see stroke.style). Acceptable values are &quot;one size&quot;, &quot;small variation&quot;, &quot;varied sizes&quot;, and &quot;random sizes&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78bd.html"/>
				<variable name="waveHeight" citdl="String" doc="A string that specifies the wave height of a ragged line. This property is available only if the stroke.style property is set to ragged (see stroke.style). Acceptable values are &quot;flat&quot;, &quot;wavy&quot;, &quot;very wavy&quot;, and &quot;wild&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78be.html"/>
				<variable name="waveLength" citdl="String" doc="A string that specifies the wavelength of a ragged line. This property is available only if the stroke.style property is set to ragged (see stroke.style). Acceptable values are &quot;very short&quot;, &quot;short&quot;, &quot;medium&quot;, and &quot;long&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78c0.html"/>
			</scope>
			<!--

				SwfPanel

			-->
			<scope name="SwfPanel" ilk="class" doc="The swfPanel object represents a Window SWF panel. Window SWF panels are SWF files that implement applications you can run from the Flash authoring environment; they are available from the Window &amp;gt; Other Panels menu. By default, Window SWF panels are stored in a subfolder of the Configuration folder (see Saving JSFL files). For example, on Windows XP, the folder is in boot drive\Documents and Settings\user\Local Settings\Application Data\Adobe\Flash CS4\language\Configuration\WindowSWF. A sample Window SWF panel is available; see Sample Trace Bitmap panel. The array of registered Window SWF panels is stored in the fl.swfPanels property. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS3B7A5142-3884-4224-96EC-255095DE860A.html">
				<scope name="call" ilk="function" returns="String" signature="call(request):String" doc="Works in conjunction with the ActionScript ExternalInterface.addCallback() and MMExecute() methods to communicate with the SWF panel from the authoring environment. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS4B030CF7-0681-47e3-9E2C-9A6D799980AA.html">
					<scope name="request" ilk="argument" citdl="Parameter" doc="Parameters to pass to the function (see “Description” and “Example” below)."/>
				</scope>
				<variable name="name" citdl="String" doc="Read-only property: a string that represents the name of the specified Window SWF panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSD69860D2-35A7-48ee-B7F7-8E8177A3D86F.html"/>
				<variable name="path" citdl="String" doc="Read-only property: a string that represents the path to the SWF file used in the specified Window SWF panel. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB20DFC15-D3A4-4120-AD70-EB036AC084B0.html"/>
			</scope>
			<!--

				SymbolInstance

			-->
			<scope name="SymbolInstance" ilk="class" classrefs="Instance" doc="SymbolInstance is a subclass of the Instance object and represents a symbol in a frame (see Instance object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f62.html">
				<variable name="accName" citdl="String" doc="A string that is equivalent to the Name field in the Accessibility panel. Screen readers identify objects by reading the name aloud. This property is not available for graphic symbols. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-789b.html"/>
				<variable name="actionScript" citdl="String" doc="A string that specifies the actions assigned to the symbol. This applies only to movie clip and button instances. For a graphic symbol instance, the value returns undefined. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7e79.html"/>
				<variable name="blendMode" citdl="String" doc="A string that specifies the blending mode to be applied to a movie clip symbol. Acceptable values are &quot;normal&quot;, &quot;layer&quot;, &quot;multiply&quot;, &quot;screen&quot;, &quot;overlay&quot;, &quot;hardlight&quot;, &quot;lighten&quot;, &quot;darken&quot;, &quot;difference&quot;, &quot;add&quot;, &quot;subtract&quot;, &quot;invert&quot;, &quot;alpha&quot;, and &quot;erase&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cf4.html"/>
				<variable name="buttonTracking" citdl="String" doc="A string that, for button symbols only, sets the same property as the pop-up menu for Track As Button or Track As Menu Item in the Property inspector. For other types of symbols, this property is ignored. Acceptable values are &quot;button&quot; or &quot;menu&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78b0.html"/>
				<variable name="cacheAsBitmap" citdl="Boolean" doc="A Boolean value that specifies whether run-time bitmap caching is enabled." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7896.html"/>
				<variable name="colorAlphaAmount" citdl="Number" doc="An integer that is part of the color transformation for the instance, specifying the Advanced Effect Alpha settings. This property is equivalent to using the Color &amp;gt; Advanced setting in the Property inspector and adjusting the controls on the right of the dialog box. This value either reduces or increases the tint and alpha values by a constant amount. This value is added to the current value. This property is most useful if used with symbolInstance.colorAlphaPercent. Allowable values are from ‑255 to 255." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78b2.html"/>
				<variable name="colorAlphaPercent" citdl="Number" doc="An integer that specifies part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector (the percentage controls on the left of the dialog box). This value changes the tint and alpha values to a specified percentage. Allowable values are from ‑100 to 100. See also symbolInstance.colorAlphaAmount." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78ae.html"/>
				<variable name="colorBlueAmount" citdl="Number" doc="An integer that is part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector. Allowable values are from ‑255 to 255. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78ad.html"/>
				<variable name="colorBluePercent" citdl="Number" doc="An integer that is part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector (the percentage controls on the left of the dialog box). This value sets the blue values to a specified percentage. Allowable values are from ‑100 to 100." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78ac.html"/>
				<variable name="colorGreenAmount" citdl="Number" doc="An integer that is part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector. Allowable values are from ‑255 to 255. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78ab.html"/>
				<variable name="colorGreenPercent" citdl="Instance" doc="Part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector (the percentage controls on the left of the dialog box). This value sets the green values by a specified percentage. Allowable values are from ‑100 to 100." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78aa.html"/>
				<variable name="colorMode" citdl="String" doc="A string that specifies the color mode as identified in the symbol Property inspector Color pop-up menu. Acceptable values are &quot;none&quot;, &quot;brightness&quot;, &quot;tint&quot;, &quot;alpha&quot;, and &quot;advanced&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a9.html"/>
				<variable name="colorRedAmount" citdl="Number" doc="An integer that is part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector. Allowable values are from ‑255 to 255. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a8.html"/>
				<variable name="colorRedPercent" citdl="Instance" doc="Part of the color transformation for the instance. This property is equivalent to using the Color &amp;gt; Advanced setting in the instance Property inspector (the percentage controls on the left of the dialog box). This value sets the red values to a specified percentage. Allowable values are from ‑100 to 100." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a7.html"/>
				<variable name="description" citdl="String" doc="A string that is equivalent to the Description field in the Accessibility panel. The description is read by the screen reader. This property is not available for graphic symbols. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a6.html"/>
				<variable name="filters" citdl="Array" doc="An array of Filter objects (see Filter object). To modify filter properties, you don’t write to this array directly. Instead, retrieve the array, set the individual properties, and then set the array to reflect the new properties. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7897.html"/>
				<variable name="firstFrame" citdl="Number" doc="A zero-based integer that specifies the first frame to appear in the timeline of the graphic. This property applies only to graphic symbols and sets the same property as the First field in the Property inspector. For other types of symbols, this property is undefined." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a5.html"/>
				<variable name="forceSimple" citdl="Boolean" doc="A Boolean value that enables and disables the accessibility of the object’s children. This property is equivalent to the inverse logic of the Make Child Objects Accessible setting in the Accessibility panel. For example, if forceSimple is true, it is the same as the Make Child Object Accessible option being unchecked. If forceSimple is false, it is the same as the Make Child Object Accessible option being checked. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a4.html"/>
				<variable name="loop" citdl="String" doc="A string that, for graphic symbols, sets the same property as the Loop pop-up menu in the Property inspector. For other types of symbols, this property is undefined. Acceptable values are &quot;loop&quot;, &quot;play once&quot;, and &quot;single frame&quot; to set the graphic’s animation accordingly. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a3.html"/>
				<variable name="shortcut" citdl="String" doc="A string that is equivalent to the shortcut key associated with the symbol. This property is equivalent to the Shortcut field in the Accessibility panel. This key is read by the screen readers. This property is not available for graphic symbols." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a2.html"/>
				<variable name="silent" citdl="Boolean" doc="A Boolean value that enables or disables the accessibility of the object. This property is equivalent to the inverse logic of the Make Object Accessible setting in the Accessibility panel. For example, if silent is true, it is the same as the Make Object Accessible option being unchecked. If silent is false, it is the same as the Make Object Accessible option being checked. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a1.html"/>
				<variable name="symbolType" citdl="String" doc="A string that specifies the type of symbol. This property is equivalent to the value for Behavior in the Create New Symbol and Convert To Symbol dialog boxes. Acceptable values are &quot;button&quot;, &quot;movie clip&quot;, and &quot;graphic&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-78a0.html"/>
				<variable name="tabIndex" citdl="Number" doc="An integer that is equivalent to the Tab index field in the Accessibility panel. Creates a tab order in which objects are accessed when the user presses the Tab key. This property is not available for graphic symbols. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-789f.html"/>
			</scope>
			<!--

				SymbolItem

			-->
			<scope name="SymbolItem" ilk="class" classrefs="Item" doc="The SymbolItem object is a subclass of the Item object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f76.html">
				<scope name="convertToCompiledClip" ilk="function" signature="convertToCompiledClip()" doc="Converts a symbol item in the library to a compiled movie clip." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-788c.html"/>
				<scope name="exportSWC" ilk="function" signature="exportSWC(outputURI)" doc="Exports the symbol item to a SWC file." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-788b.html">
					<scope name="outputURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the SWC file to which the method will export the symbol. The outputURI must reference a local file. Flash does not create a folder if outputURI does not exist."/>
				</scope>
				<scope name="exportSWF" ilk="function" signature="exportSWF(outputURI)" doc="Exports the symbol item to a SWF file." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-788a.html">
					<scope name="outputURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the SWF file to which the method will export the symbol. The outputURI must reference a local file. Flash does not create a folder if outputURI doesn’t exist. "/>
				</scope>
				<variable name="scalingGrid" citdl="Boolean" doc="A Boolean value that specifies whether 9‑slice scaling is enabled for the item. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7893.html"/>
				<variable name="scalingGridRect" citdl="Rectangle" doc="A Rectangle object that specifies the locations of the four 9‑slice guides. For information on the format of the rectangle, see document.addNewRectangle(). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7883.html"/>
				<variable name="sourceAutoUpdate" citdl="Boolean" doc="A Boolean value that specifies whether the item is updated when the FLA file is published. The default value is false. Used for shared library symbols." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7892.html"/>
				<variable name="sourceFilePath" citdl="String" doc="A string that specifies the path for the source FLA file as a file:/// URI. The path must be an absolute path, not a relative path. This property is used for shared library symbols. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7891.html"/>
				<variable name="sourceLibraryName" citdl="String" doc="A string that specifies the name of the item in the source file library. It is used for shared library symbols. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7890.html"/>
				<variable name="symbolType" citdl="String" doc="A string that specifies the type of symbol. Acceptable values are &quot;movie clip&quot;, &quot;button&quot;, and &quot;graphic&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-788f.html"/>
				<variable name="timeline" citdl="Timeline" doc="Read-only property; a Timeline object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d40.html"/>
			</scope>
			<!--

				Text

			-->
			<scope name="Text" ilk="class" doc="The Text object represents a single text item in a document. All properties of the text pertain to the entire text block. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f5d.html">
				<variable name="accName" citdl="String" doc="A string that is equivalent to the Name field in the Accessibility panel. Screen readers identify objects by reading the name aloud. This property cannot be used with dynamic text." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7874.html"/>
				<variable name="antiAliasSharpness" citdl="Fl" doc="A float value that specifies the anti-aliasing sharpness of the text. This property controls how crisply the text is drawn; higher values specify sharper (or crisper) text. A value of 0 specifies normal sharpness. This property is available only if text.fontRenderingMode is set to customThicknessSharpness." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7873.html"/>
				<variable name="antiAliasThickness" citdl="Fl" doc="A float value that specifies the anti-aliasing thickness of the text. This property controls how thickly the text is drawn, with higher values specifying thicker text. A value of 0 specifies normal thickness. This property is available only if text.fontRenderingMode is set to customThicknessSharpness." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7872.html"/>
				<variable name="autoExpand" citdl="Boolean" doc="A Boolean value. For static text fields, a value of true causes the bounding width to expand to show all text. For dynamic or input text fields, a value of true causes the bounding width and height to expand to show all text. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7871.html"/>
				<variable name="border" citdl="Boolean" doc="A Boolean value. A value of true causes Flash to show a border around text." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7870.html"/>
				<variable name="description" citdl="String" doc="A string that is equivalent to the Description field in the Accessibility panel. The description is read by the screen reader. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-786f.html"/>
				<variable name="embeddedCharacters" citdl="String" doc="A string that specifies characters to embed. This is equivalent to entering text in the Character Embedding dialog box. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-786e.html"/>
				<variable name="embedRanges" citdl="String" doc="A string that consists of delimited integers that correspond to the items that can be selected in the Character Embedding dialog box. This property works only with dynamic or input text; it is ignored if used with static text. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-786d.html"/>
				<variable name="embedVariantGlyphs" citdl="Boolean" doc="A Boolean value that specifies whether to enable the embedding of variant glyphs (true) or not (false). This property works only with dynamic or input text; it is ignored if used with static text. The default value is false." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS34C828F3-4D44-44eb-A73E-5A0F17C25962.html"/>
				<variable name="fontRenderingMode" citdl="String" doc="A string that specifies the rendering mode for the text. This property affects how the text is displayed both on the Stage and in Flash Player. Acceptable values are described in the following table:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-786c.html"/>
				<scope name="getTextAttr" ilk="function" returns="value" signature="getTextAttr(attrName [, startIndex [, endIndex]]):value" doc="Retrieves the attribute specified by the attrName parameter for the text identified by the optional startIndex and endIndex parameters. If the attribute is not consistent for the specified range, Flash returns undefined. If you omit the optional parameters startIndex and endIndex, the method uses the entire text range. If you specify only startIndex, the range used is a single character at that position. If you specify both startIndex and endIndex, the range starts from startIndex and goes up to, but does not include, endIndex. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-787e.html">
					<scope name="attrName" ilk="argument" citdl="String" doc="A string that specifies the name of the TextAttrs object property to be returned. For a list of possible values for attrName, see the Property summary for the TextAttrs object."/>
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that is the index of first character. This parameter is optional."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the end of the range of text, which starts with startIndex and goes up to, but does not include, endIndex. This parameter is optional. "/>
				</scope>
				<scope name="getTextString" ilk="function" returns="String" signature="getTextString([startIndex [, endIndex]]):String" doc="Retrieves the specified range of text. If you omit the optional parameters startIndex and endIndex, the whole text string is returned. If you specify only startIndex, the method returns the string starting at the index location and ending at the end of the field. If you specify both startIndex and endIndex, the method returns the string starting from startIndex and goes up to, but does not include, endIndex. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-787d.html">
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that specifies the index (zero-based) of the first character. This parameter is optional. "/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the end of the range of text, which starts from startIndex and goes up to, but does not include, endIndex. This parameter is optional."/>
				</scope>
				<variable name="length" citdl="Number" doc="Read-only property; an integer that represents the number of characters in the Text object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-786b.html"/>
				<variable name="lineType" citdl="String" doc="A string that sets the line type. Acceptable values are &quot;single line&quot;, &quot;multiline&quot;, &quot;multiline no wrap&quot;, and &quot;password&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-786a.html"/>
				<variable name="maxCharacters" citdl="Number" doc="An integer that specifies the maximum number of characters the user can enter in this Text object. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7869.html"/>
				<variable name="orientation" citdl="String" doc="A string that specifies the orientation of the text field. Acceptable values are &quot;horizontal&quot;, &quot;vertical left to right&quot;, and &quot;vertical right to left&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7868.html"/>
				<variable name="renderAsHTML" citdl="Boolean" doc="A Boolean value. If the value is true, Flash draws the text as HTML and interprets embedded HTML tags. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7867.html"/>
				<variable name="scrollable" citdl="Boolean" doc="A Boolean value. If the value is true, the text can be scrolled. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7866.html"/>
				<variable name="selectable" citdl="Boolean" doc="A Boolean value. If the value is true, the text can be selected. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7865.html"/>
				<variable name="selectionEnd" citdl="Number" doc="A zero-based integer that specifies the end of a text subselection. For more information, see text.selectionStart. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7864.html"/>
				<variable name="selectionStart" citdl="Number" doc="A zero-based integer that specifies the beginning of a text subselection. You can use this property with text.selectionEnd to select a range of characters. Characters up to, but not including, text.selectionEnd are selected. See text.selectionEnd." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7863.html"/>
				<scope name="setTextAttr" ilk="function" signature="setTextAttr(attrName, attrValue [, startIndex [, endIndex]])" doc="Sets the attribute specified by the attrName parameter associated with the text identified by startIndex and endIndex to the value specified by attrValue. This method can be used to change attributes of text that might span TextRun elements (see TextRun object), or that are portions of existing TextRun elements. Using it may change the position and number of TextRun elements within this object’s text.textRuns array (see text.textRuns). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-787c.html">
					<scope name="attrName" ilk="argument" citdl="String" doc="A string that specifies the name of the TextAttrs object property to change. "/>
					<scope name="attrValue" ilk="argument" citdl="value" doc="The value for the TextAttrs object property. For a list of possible values for attrName and attrValue, see the Property summary for the TextAttrs object. "/>
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that is the index (zero-based) of the first character in the array. This parameter is optional."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the index of the end point in the selected text string, which starts at startIndex and goes up to, but does not include, endIndex. This parameter is optional."/>
				</scope>
				<scope name="setTextString" ilk="function" signature="setTextString(text [, startIndex [, endIndex]])" doc="Changes the text string within this Text object. If you omit the optional parameters, the whole Text object is replaced. If you specify only startIndex, the specified string is inserted at the startIndex position. If you specify both startIndex and endIndex, the specified string replaces the segment of text starting from startIndex up to, but not including, endIndex. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-787b.html">
					<scope name="text" ilk="argument" citdl="String" doc="A string that consists of the characters to be inserted into this Text object. "/>
					<scope name="startIndex" ilk="argument" citdl="Number" doc="An integer that specifies the index (zero-based) of the character in the string where the text will be inserted. This parameter is optional."/>
					<scope name="endIndex" ilk="argument" citdl="Number" doc="An integer that specifies the index of the end point in the selected text string. The new text overwrites the text from startIndex up to, but not including, endIndex. This parameter is optional."/>
				</scope>
				<variable name="shortcut" citdl="String" doc="A string that is equivalent to the Shortcut field in the Accessibility panel. The shortcut is read by the screen reader. This property cannot be used with dynamic text." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7862.html"/>
				<variable name="silent" citdl="Boolean" doc="A Boolean value that specifies whether the object is accessible. This is equivalent to the inverse logic of the Make Object Accessible setting in the Accessibility panel. That is, if silent is true, Make Object Accessible is deselected. If it is false, Make Object Accessible is selected." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7861.html"/>
				<variable name="tabIndex" citdl="Number" doc="An integer that is equivalent to the Tab Index field in the Accessibility panel. This value lets you determine the order in which objects are accessed when the user presses the Tab key." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7860.html"/>
				<variable name="textRuns" citdl="Array" doc="Read-only property; an array of TextRun objects (see TextRun object). " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f5a.html"/>
				<variable name="textType" citdl="String" doc="A string that specifies the type of text field. Acceptable values are &quot;static&quot;, &quot;dynamic&quot;, and &quot;input&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7854.html"/>
				<variable name="useDeviceFonts" citdl="Boolean" doc="A Boolean value. A value of true causes Flash to draw text using device fonts. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7853.html"/>
				<variable name="variableName" citdl="String" doc="A string that contains the name of the variable associated with the Text object. This property works only with dynamic or input text; it generates a warning if used with other text types. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7852.html"/>
			</scope>
			<!--

				TextAttrs

			-->
			<scope name="TextAttrs" ilk="class" doc="The TextAttrs object contains all the properties of text that can be applied to a subselection. This object is a property of the TextRun object (textRun.textAttrs)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f59.html">
				<variable name="aliasText" citdl="Boolean" doc="A Boolean value that specifies that Flash should draw the text using a method optimized for increasing the legibility of small text." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-783b.html"/>
				<variable name="alignment" citdl="String" doc="A string that specifies paragraph justification. Acceptable values are &quot;left&quot;, &quot;center&quot;, &quot;right&quot;, and &quot;justify&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-784e.html"/>
				<variable name="autoKern" citdl="Boolean" doc="A Boolean value that determines whether Flash uses (true) or ignores (false) pair kerning information in the font(s) when it kerns the text. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-784d.html"/>
				<variable name="bold" citdl="Boolean" doc="A Boolean value. A value of true causes text to appear with the bold version of the font." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-784c.html"/>
				<variable name="characterPosition" citdl="String" doc="A string that determines the baseline for the text. Acceptable values are &quot;normal&quot;, &quot;subscript&quot;, and &quot;superscript&quot;. This property applies only to static text. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-784b.html"/>
				<variable name="characterSpacing" citdl="Number" doc="An integer that represents the space between characters. Acceptable values are -60 through 60. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-784a.html"/>
				<variable name="face" citdl="String" doc="A string that represents the name of the font, such as &quot;Arial&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7849.html"/>
				<variable name="fillColor" citdl="Fill" doc="The color of the fill, in one of the following formats:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7848.html"/>
				<variable name="indent" citdl="Number" doc="An integer that specifies paragraph indentation. Acceptable values are -720 through 720." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7847.html"/>
				<variable name="italic" citdl="Boolean" doc="A Boolean value. A value of true causes text to appear with the italic version of the font." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7846.html"/>
				<variable name="leftMargin" citdl="Number" doc="An integer that specifies the paragraph’s left margin. Acceptable values are 0 through 720." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7845.html"/>
				<variable name="letterSpacing" citdl="Number" doc="An integer that represents the space between characters. Acceptable values are -60 through 60. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-783d.html"/>
				<variable name="lineSpacing" citdl="Number" doc="An integer that specifies the line spacing (leading) of the paragraph. Acceptable values are -360 through 720." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7844.html"/>
				<variable name="rightMargin" citdl="Number" doc="An integer that specifies the paragraph’s right margin. Acceptable values are 0 through 720." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7843.html"/>
				<variable name="rotation" citdl="Boolean" doc="A Boolean value. A value of true causes Flash to rotate the characters of the text 90º. The default value is false. This property applies only to static text with a vertical orientation; it generates a warning if used with other text types. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7842.html"/>
				<variable name="size" citdl="Number" doc="An integer that specifies the size of the font. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7841.html"/>
				<variable name="target" citdl="String" doc="A string that represents the target property of the text field. This property works only with static text." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7840.html"/>
				<variable name="url" citdl="String" doc="A string that represents the URL property of the text field. This property works only with static text. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-783f.html"/>
			</scope>
			<!--

				TextRun

			-->
			<scope name="TextRun" ilk="class" doc="The TextRun object represents a run of characters that have attributes that match all of the properties in the TextAttrs object. This object is a property of the Text object (text.textRuns)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f5b.html">
				<variable name="textAttrs" citdl="Text" doc="The TextAttrs object containing the attributes of the run of text." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f58.html"/>
				<variable name="characters" citdl="Text" doc="The text contained in the TextRun object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7838.html"/>
			</scope>
			<!--

				Timeline

			-->
			<scope name="Timeline" ilk="class" doc="The Timeline object represents the Flash timeline, which can be accessed for the current document by using fl.getDocumentDOM().getTimeline(). This method returns the timeline of the current scene or symbol that is being edited." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f8f.html">
				<scope name="addMotionGuide" ilk="function" returns="Number" signature="addMotionGuide():Number" doc="Adds a motion guide layer above the current layer and attaches the current layer to the newly added guide layer, converting the current layer to a layer of type &quot;Guided&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7833.html"/>
				<scope name="addNewLayer" ilk="function" returns="Number" signature="addNewLayer([name] [, layerType [, bAddAbove]]):Number" doc="Adds a new layer to the document and makes it the current layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7832.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name for the new layer. If you omit this parameter, a new default layer name is assigned to the new layer (“Layer n,” where n is the total number of layers). This parameter is optional."/>
					<scope name="layerType" ilk="argument" citdl="String" doc="A string that specifies the type of layer to add. If you omit this parameter, a “Normal” type layer is created. This parameter is optional. Acceptable values are &quot;normal&quot;, &quot;guide&quot;, &quot;guided&quot;, &quot;mask&quot;, &quot;masked&quot;, and &quot;folder&quot;. "/>
					<scope name="bAddAbove" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true (the default), causes Flash to add the new layer above the current layer; false causes Flash to add the layer below the current layer. This parameter is optional."/>
				</scope>
				<scope name="clearFrames" ilk="function" signature="clearFrames([startFrameIndex [, endFrameIndex]])" doc="Deletes all the contents from a frame or range of frames on the current layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7831.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that defines the beginning of the range of frames to clear. If you omit startFrameIndex, the method uses the current selection. This parameter is optional. "/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that defines the end of the range of frames to clear. The range goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="clearKeyframes" ilk="function" signature="clearKeyframes([startFrameIndex [, endFrameIndex]])" doc="Converts a keyframe to a regular frame and deletes its contents on the current layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7830.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that defines the beginning of the range of frames to clear. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that defines the end of the range of frames to clear. The range goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="convertToBlankKeyframes" ilk="function" signature="convertToBlankKeyframes([startFrameIndex [, endFrameIndex]])" doc="Converts frames to blank keyframes on the current layer." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-782f.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the starting frame to convert to keyframes. If you omit startFrameIndex, the method converts the currently selected frames. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the frame at which the conversion to keyframes will stop. The range of frames to convert goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="convertToKeyframes" ilk="function" signature="convertToKeyframes([startFrameIndex [, endFrameIndex]])" doc="Converts a range of frames to keyframes (or converts the selection if no frames are specified) on the current layer." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-782e.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the first frame to convert to keyframes. If you omit startFrameIndex, the method converts the currently selected frames. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the frame at which conversion to keyframes will stop. The range of frames to convert goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="copyFrames" ilk="function" signature="copyFrames([startFrameIndex [, endFrameIndex]])" doc="Copies a range of frames on the current layer to the clipboard." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-782d.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the beginning of the range of frames to copy. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the frame at which to stop copying. The range of frames to copy goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="copyMotion" ilk="function" signature="copyMotion()" doc="Copies motion on selected frames, either from a motion tween or from frame-by-frame animation. You can then use timeline.pasteMotion() to apply the motion to other frames." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f1b.html"/>
				<scope name="copyMotionAsAS3" ilk="function" signature="copyMotionAsAS3()" doc="Copies motion on selected frames, either from a motion tween or from frame-by-frame animation, to the clipboard as ActionScript 3.0 code. You can then paste this code into a script." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa6.html"/>
				<scope name="createMotionTween" ilk="function" signature="createMotionTween([startFrameIndex [, endFrameIndex]])" doc="Sets the frame.tweenType property to motion for each selected keyframe on the current layer, and converts each frame’s contents to a single symbol instance if necessary. This property is the equivalent to the Create Motion Tween menu item in the Flash authoring tool. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7aa8.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the beginning frame at which to create a motion tween. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the frame at which to stop the motion tween. The range of frames goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the startFrameIndex value. This parameter is optional."/>
				</scope>
				<variable name="currentFrame" citdl="Number" doc="The zero-based index for the frame at the current playhead location. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7807.html"/>
				<variable name="currentLayer" citdl="Number" doc="The zero-based index for the currently active layer. A value of 0 specifies the top layer, a value of 1 specifies the layer below it, and so on." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7812.html"/>
				<scope name="cutFrames" ilk="function" signature="cutFrames([startFrameIndex [, endFrameIndex]])" doc="Cuts a range of frames on the current layer from the timeline and saves them to the clipboard. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-782b.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the beginning of a range of frames to cut. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the frame at which to stop cutting. The range of frames goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the startFrameIndex value. This parameter is optional."/>
				</scope>
				<scope name="deleteLayer" ilk="function" signature="deleteLayer([index])" doc="Deletes a layer. If the layer is a folder, all layers within the folder are deleted. If you do not specify the layer index, Flash deletes the currently selected layers. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-782a.html">
					<scope name="index" ilk="argument" citdl="Number" doc="A zero-based index that specifies the layer to be deleted. If there is only one layer in the timeline, this method has no effect. This parameter is optional."/>
				</scope>
				<scope name="expandFolder" ilk="function" signature="expandFolder(bExpand [, bRecurseNestedParents [, index]])" doc="Expands or collapses the specified folder or folders. If you do not specify a layer, this method operates on the current layer." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7829.html">
					<scope name="bExpand" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes the method to expand the folder; false causes the method to collapse the folder."/>
					<scope name="bRecurseNestedParents" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true, causes all the layers within the specified folder to be opened or closed, based on the bExpand parameter. This parameter is optional."/>
					<scope name="index" ilk="argument" citdl="Number" doc="A zero-based index of the folder to expand or collapse. Use ‑1 to apply to all layers (you also must set bRecurseNestedParents to true). This property is equivalent to the Expand All/Collapse All menu items in the Flash authoring tool. This parameter is optional."/>
				</scope>
				<scope name="findLayerIndex" ilk="function" returns="Array" signature="findLayerIndex(name):Array" doc="Finds an array of indexes for the layers with the given name. The layer index is flat, so folders are considered part of the main index. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7828.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the layer to find. "/>
				</scope>
				<variable name="frameCount" citdl="Number" doc="Read-only property; an integer that represents the number of frames in this timeline’s longest layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7811.html"/>
				<scope name="getFrameProperty" ilk="function" returns="value" signature="getFrameProperty(property [, startframeIndex [, endFrameIndex]]):value" doc="Retrieves the specified property’s value for the selected frames. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7827.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the name of the property for which to get the value. See the Property summary for the Frame object for a complete list of properties."/>
					<scope name="startFrameIndex" ilk="argument" citdl="Number" doc="A zero-based index that specifies the starting frame number for which to get the value. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the end of the range of frames to select. The range goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="getGuidelines" ilk="function" returns="String" signature="getGuidelines():String" doc="Method: returns an XML string that represents the current positions of the horizontal and vertical guide lines for a timeline (View &amp;gt; Guides &amp;gt;Show Guides). To apply these guide lines to a timeline, use timeline.setGuidelines()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSA21EAEB2-CD25-4a65-B70B-26D35A9568E9.html"/>
				<scope name="getLayerProperty" ilk="function" returns="value" signature="getLayerProperty(property):value" doc="Retrieves the specified property’s value for the selected layers. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7826.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the name of the property whose value you want to retrieve. For a list of properties, see the Property summary for the Frame object."/>
				</scope>
				<scope name="getSelectedFrames" ilk="function" returns="Frame" doc="With the top layer being the current layer, the following example displays 0,5,10,0,20,25 in the Output panel:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d01.html"/>
				<scope name="getSelectedLayers" ilk="function" returns="value" doc="The following example displays 1,0 in the Output panel:" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7824.html"/>
				<scope name="insertBlankKeyframe" ilk="function" signature="insertBlankKeyframe([frameNumIndex])" doc="Inserts a blank keyframe at the specified frame index; if the index is not specified, the method inserts the blank keyframe by using the playhead/selection. See also timeline.insertKeyframe()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7823.html">
					<scope name="frameNumIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the frame at which to insert the keyframe. If you omit frameNumIndex, the method uses the current playhead frame number. This parameter is optional.If the specified or selected frame is a regular frame, the keyframe is inserted at the frame. For example, if you have a span of 10 frames numbered 1-10 and you select Frame 5, this method makes Frame 5 a blank keyframe, and the length of the frame span is still 10 frames. If Frame 5 is selected and is a keyframe with a regular frame next to it, this method inserts a blank keyframe at Frame 6. If Frame 5 is a keyframe and the frame next to it is already a keyframe, no keyframe is inserted but the playhead moves to Frame 6. "/>
				</scope>
				<scope name="insertFrames" ilk="function" signature="insertFrames([numFrames [, bAllLayers [, frameNumIndex]]])" doc="Inserts the specified number of frames at the specified index. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7822.html">
					<scope name="numFrames" ilk="argument" citdl="Number" doc="An integer that specifies the number of frames to insert. If you omit this parameter, the method inserts frames at the current selection in the current layer. This parameter is optional."/>
					<scope name="bAllLayers" ilk="argument" citdl="Boolean" doc="A Boolean value that, if set to true (the default), causes the method to insert the specified number of frames in the numFrames parameter into all layers; if set to false, the method inserts frames into the current layer. This parameter is optional."/>
					<scope name="frameNumIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the frame at which to insert a new frame. This parameter is optional."/>
				</scope>
				<scope name="insertKeyframe" ilk="function" signature="insertKeyframe([frameNumIndex])" doc="Inserts a keyframe at the specified frame. If you omit the parameter, the method inserts a keyframe using the playhead or selection location." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7821.html">
					<scope name="frameNumIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the frame index at which to insert the keyframe in the current layer. If you omit frameNumIndex, the method uses the frame number of the current playhead or selected frame. This parameter is optional."/>
				</scope>
				<variable name="layerCount" citdl="Number" doc="Read-only property; an integer that represents the number of layers in the specified timeline. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7810.html"/>
				<variable name="layers" citdl="Array" doc="Read-only property; an array of layer objects. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f6f.html"/>
				<variable name="name" citdl="String" doc="A string that specifies the name of the current timeline. This name is the name of the current scene, screen (slide or form), or symbol that is being edited." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-780e.html"/>
				<scope name="pasteFrames" ilk="function" signature="pasteFrames([startFrameIndex [, endFrameIndex]])" doc="Pastes the range of frames from the clipboard into the specified frames." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7820.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the beginning of a range of frames to paste. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the frame at which to stop pasting frames. The method pastes up to, but not including, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the startFrameIndex value. This parameter is optional."/>
				</scope>
				<scope name="pasteMotion" ilk="function" signature="pasteMotion()" doc="Pastes the range of motion frames retrieved by timeline.copyMotion() to the Timeline. If necessary, existing frames are displaced (moved to the right) to make room for the frames being pasted." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fa5.html"/>
				<scope name="removeFrames" ilk="function" signature="removeFrames([startFrameIndex [, endFrameIndex]])" doc="Deletes the frame. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-781f.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the first frame at which to start removing frames. If you omit startFrameIndex, the method uses the current selection; if there is no selection, all frames at the current playhead on all layers are removed. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the frame at which to stop removing frames; the range of frames goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the startFrameIndex value. This parameter is optional."/>
				</scope>
				<scope name="reorderLayer" ilk="function" signature="reorderLayer(layerToMove, layerToPutItBy [, bAddBefore])" doc="Moves the first specified layer before or after the second specified layer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-781e.html">
					<scope name="layerToMove" ilk="argument" citdl="Layer" doc="A zero-based index that specifies which layer to move. "/>
					<scope name="layerToPutItBy" ilk="argument" citdl="Layer" doc="A zero-based index that specifies which layer you want to move the layer next to. For example, if you specify 1 for layerToMove and 0 for layerToPutItBy, the second layer is placed next to the first layer."/>
					<scope name="bAddBefore" ilk="argument" citdl="Layer" doc="Specifies whether to move the layer before or after layerToPutItBy. If you specify false, the layer is moved after layerToPutItBy. The default value is true. This parameter is optional."/>
				</scope>
				<scope name="reverseFrames" ilk="function" signature="reverseFrames([startFrameIndex [, endFrameIndex]])" doc="Reverses a range of frames. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-781d.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the first frame at which to start reversing frames. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the first frame at which to stop reversing frames; the range of frames goes up to, but does not include, endFrameIndex. If you specify only startFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="selectAllFrames" ilk="function" signature="selectAllFrames()" doc="Selects all the frames in the current timeline. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7cff.html"/>
				<scope name="setFrameProperty" ilk="function" signature="setFrameProperty(property, value [, startFrameIndex [, endFrameIndex]])" doc="Sets the property of the Frame object for the selected frames. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-781b.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the name of the property to be modified. For a complete list of properties and values, see the Property summary for the Frame object.You can’t use this method to set values for read-only properties such as frame.duration and frame.elements. "/>
					<scope name="value" ilk="argument" citdl="value" doc="Specifies the value to which you want to set the property. To determine the appropriate values and type, see the Property summary for the Frame object. "/>
					<scope name="startFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the starting frame number to modify. If you omit startFrameIndex, the method uses the current selection. This parameter is optional."/>
					<scope name="endFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the first frame at which to stop. The range of frames goes up to, but does not include, endFrameIndex. If you specify startFrameIndex but omit endFrameIndex, endFrameIndex defaults to the value of startFrameIndex. This parameter is optional."/>
				</scope>
				<scope name="setGuidelines" ilk="function" returns="Boolean" signature="setGuidelines(xmlString):Boolean" doc="Method: replaces the guide lines for the timeline (View &amp;gt; Guides &amp;gt; Show Guides) with the information specified in xmlString. To retrieve an XML string that can be passed to this method, use timeline.getGuidelines()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS3A123678-2F76-42d8-A0A1-947CBAD1A2C3.html">
					<scope name="xmlString" ilk="argument" citdl="String" doc="An XML string that contains information on the guidelines to apply."/>
				</scope>
				<scope name="setLayerProperty" ilk="function" signature="setLayerProperty(property, value [, layersToChange])" doc="Sets the specified property on all the selected layers to a specified value. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-781a.html">
					<scope name="property" ilk="argument" citdl="String" doc="A string that specifies the property to set. For a list of properties, see Layer object."/>
					<scope name="value" ilk="argument" citdl="value" doc="The value to which you want to set the property. Use the same type of value you would use when setting the property in the layer object. "/>
					<scope name="layersToChange" ilk="argument" citdl="String" doc="A string that identifies which layers should be modified. Acceptable values are &quot;selected&quot;, &quot;all&quot;, and &quot;others&quot;. The default value is &quot;selected&quot; if you omit this parameter. This parameter is optional."/>
				</scope>
				<scope name="setSelectedFrames" ilk="function" signature="setSelectedFrames(startFrameIndex, endFrameIndex [, bReplaceCurrentSelection]) timeline.setSelectedFrames(selectionList [, bReplaceCurrentSelection])" doc="Selects a range of frames in the current layer or sets the selected frames to the selection array passed into this method. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7d00.html">
					<scope name="startFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the beginning frame to set. "/>
					<scope name="endFrameIndex" ilk="argument" citdl="Frame" doc="A zero-based index that specifies the end of the selection; endFrameIndex is the frame after the last frame in the range to select. "/>
					<scope name="bReplaceCurrentSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that, if it is set to true, causes the currently selected frames to be deselected before the specified frames are selected. The default value is true. "/>
					<scope name="selectionList" ilk="argument" citdl="Array" doc="An array of three integers, as returned by timeline.getSelectedFrames(). "/>
				</scope>
				<scope name="setSelectedLayers" ilk="function" signature="setSelectedLayers(index [, bReplaceCurrentSelection])" doc="Sets the layer to be selected, and also makes the specified layer the current layer. Selecting a layer also means that all the frames in the layer are selected. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7817.html">
					<scope name="index" ilk="argument" citdl="Number" doc="A zero-based index for the layer to select. "/>
					<scope name="bReplaceCurrentSelection" ilk="argument" citdl="Boolean" doc="A Boolean value that, if it is set to true, causes the method to replace the current selection; false causes the method to extend the current selection. The default value is true. This parameter is optional."/>
				</scope>
				<scope name="showLayerMasking" ilk="function" signature="showLayerMasking([layer])" doc="Shows the layer masking during authoring by locking the mask and masked layers. This method uses the current layer if no layer is specified. If you use this method on a layer that is not of type Mask or Masked, Flash displays an error in the Output panel." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7816.html">
					<scope name="layer" ilk="argument" citdl="Number" doc="A zero-based index of a mask or masked layer to show masking during authoring. This parameter is optional."/>
				</scope>
			</scope>
			<!--

				ToolObj

			-->
			<scope name="ToolObj" ilk="class" doc="A ToolObj object represents an individual tool in the Tools panel. To access a ToolObj object, use properties of the Tools object: either the tools.toolObjs array or tools.activeTool." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f33.html">
				<variable name="depth" citdl="Number" doc="Read-only property; an integer that specifies the depth of the tool in the pop-up menu in the Tools panel. This property is used only when creating extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77dd.html"/>
				<scope name="enablePIControl" ilk="function" signature="enablePIControl(control, bEnable)" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77f5.html">
					<scope name="control" ilk="argument" citdl="String" doc="A string that specifies the name of the control to enable or disable. Legal values depend on the Property inspector invoked by this tool; see toolObj.setPI().A shape Property inspector has the following controls: stroke	fill			A text Property inspector has the following controls:	type	font	pointsize		color	bold	italic direction	alignLeft	alignCenter alignRight	alignJustify	spacing position	autoKern	small rotation	format	lineType selectable	html	border deviceFonts	varEdit	options link	maxChars	target			A movie Property inspector has the following controls: size	publish	background framerate	player	profile			"/>
					<scope name="bEnable" ilk="argument" citdl="Boolean" doc="A Boolean value that determines whether to enable (true) or disable (false) the control. "/>
				</scope>
				<variable name="iconID" citdl="Number" doc="Read-only property; an integer with a value of ‑1. This property is used only when you create extensible tools. An iconID value of ‑1 means that Flash will not try find an icon for the tool. Instead, the script for the tool should specify the icon to display in the Tools panel; see toolObj.setIcon()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77dc.html"/>
				<variable name="position" citdl="Number" doc="Read-only property; an integer that specifies the position of the tool in the Tools panel. This property is used only when you create extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77e7.html"/>
				<scope name="setIcon" ilk="function" signature="setIcon(file)" doc="Identifies a PNG file to use as a tool icon in the Tools panel. This method is used only when you create extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77f4.html">
					<scope name="file" ilk="argument" citdl="String" doc="A string that specifies the name of the PNG file to use as the icon. The PNG file must be placed in the same folder as the JSFL file. "/>
				</scope>
				<scope name="setMenuString" ilk="function" signature="setMenuString(menuStr)" doc="Sets the string that appears in the pop-up menu as the name for the tool. This method is used only when you create extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77f3.html">
					<scope name="menuStr" ilk="argument" citdl="String" doc="A string that specifies the name that appears in the pop-up menu as the name for the tool. "/>
				</scope>
				<scope name="setOptionsFile" ilk="function" signature="setOptionsFile(xmlFile)" doc="Associates an XML file with the tool. The file specifies the options to appear in a modal panel that is invoked by an Options button in the Property inspector. You would usually use this method in the configureTool() function inside your JSFL file. See configureTool()." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77f2.html">
					<scope name="xmlFile" ilk="argument" citdl="String" doc="A string that specifies the name of the XML file that has the description of the tool’s options. The XML file must be placed in the same folder as the JSFL file. "/>
				</scope>
				<scope name="setPI" ilk="function" signature="setPI(pi)" doc="Specifies which Property inspector should be used when the tool is activated. This method is used only when you create extensible tools. Acceptable values are &quot;shape&quot; (the default), &quot;text&quot;, and &quot;movie&quot;. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77f1.html">
					<scope name="pi" ilk="argument" citdl="String" doc="A string that specifies the Property inspector to invoke for this tool. "/>
				</scope>
				<scope name="setToolName" ilk="function" signature="setToolName(name)" doc="Assigns a name to the tool for the configuration of the Tools panel. This method is used only when you create extensible tools. The name is used only by the XML layout file that Flash reads to construct the Tools panel. The name does not appear in the Flash user interface." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77f0.html">
					<scope name="name" ilk="argument" citdl="String" doc="A string that specifies the name of the tool. "/>
				</scope>
				<scope name="setToolTip" ilk="function" signature="setToolTip(toolTip)" doc="Sets the tooltip that appears when the mouse is held over the tool icon. This method is used only when you create extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77ef.html">
					<scope name="toolTip" ilk="argument" citdl="String" doc="A string that specifies the tooltip to use for the tool. "/>
				</scope>
				<scope name="showPIControl" ilk="function" signature="showPIControl(control, bShow)" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77ee.html">
					<scope name="control" ilk="argument" citdl="String" doc="A string that specifies the name of the control to show or hide. This method is used only when you create extensible tools. Valid values depend on the Property inspector invoked by this tool (see toolObj.setPI()toolObj.setPI()). A shape Property inspector has the following controls: stroke	fill			A text Property inspector has the following controls: type	font	pointsize		color	bold	italic direction	alignLeft	alignCenter alignRight	alignJustify	spacing position	autoKern	small rotation	format	lineType selectable	html	border deviceFonts	varEdit	options link	maxChars	target			The movie Property inspector has the following controls:	size	publish	background framerate	player	profile			"/>
					<scope name="bShow" ilk="argument" citdl="Boolean" doc="A Boolean value that determines whether to show or hide the specified control (true shows the control; false hides the control). "/>
				</scope>
				<scope name="showTransformHandles" ilk="function" signature="showTransformHandles(bShow)" doc="Called in the configureTool() method of an extensible tool’s JavaScript file to indicate that the free transform handles should appear when the tool is active. This method is used only when you create extensible tools." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77ed.html">
					<scope name="bShow" ilk="argument" citdl="Boolean" doc="A Boolean value that determines whether to show or hide the free transform handles for the current tool (true shows the handles; false hides them). "/>
				</scope>
			</scope>
			<!--

				Tools

			-->
			<scope name="Tools" ilk="class" doc="The Tools object is accessible from the flash object (fl.tools). The tools.toolObjs property contains an array of ToolObj objects, and the tools.activeTool property returns the ToolObj object for the currently active tool. (See also ToolObj object and the list of Extensible tools in Top-Level Functions and Methods.)" url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fec.html">
				<variable name="activeTool" citdl="ToolObj" doc="Read-only property; returns the ToolObj object for the currently active tool. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f18.html"/>
				<variable name="altIsDown" citdl="Boolean" doc="Read-only property; a Boolean value that identifies if the Alt key is being pressed. The value is true if the Alt key is pressed, and false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d6.html"/>
				<scope name="constrainPoint" ilk="function" returns="Object" signature="constrainPoint(pt1, pt2):Object" doc="Takes two points and returns a new adjusted or constrained point. If the Shift key is pressed when the command is run, the returned point is constrained to follow either a 45º	constrain (useful for something such as a line with an arrowhead) or to constrain an object to maintain its aspect ratio (such as pulling out a perfect square with the Rectangle tool)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d3.html">
					<scope name="pt1, pt2" ilk="argument" citdl="Object" doc="Points that specify the starting-click point and the drag-to point. "/>
				</scope>
				<variable name="ctlIsDown" citdl="Boolean" doc="Read-only property; a Boolean value that is true if the Control key is pressed; false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d4.html"/>
				<scope name="getKeyDown" ilk="function" returns="Number" signature="getKeyDown():Number" doc="Returns the most recently pressed key. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f0e.html"/>
				<variable name="mouseIsDown" citdl="Boolean" doc="Read-only property; a Boolean value that is true if the left mouse button is currently down; false otherwise." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d2.html"/>
				<variable name="penDownLoc" citdl="Object" doc="Read-only property; a point that represents the position of the last mouse-down event on the Stage. The tools.penDownLoc property comprises two properties, x and y, corresponding to the x,y location of the mouse pointer." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d1.html"/>
				<variable name="penLoc" citdl="Object" doc="Read-only property; a point that represents the current location of the mouse pointer. The tools.penLoc property comprises two properties, x and y, corresponding to the x,y location of the mouse pointer. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d0.html"/>
				<scope name="setCursor" ilk="function" signature="setCursor(cursor)" doc="Sets the pointer to a specified appearance. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f05.html">
					<scope name="cursor" ilk="argument" citdl="Number" doc="An integer that defines the pointer appearance, as described in the following list:	"/>
				</scope>
				<variable name="shiftIsDown" citdl="Boolean" doc="Read-only property; a Boolean value that is true if the Shift key is pressed; false otherwise." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77cf.html"/>
				<scope name="snapPoint" ilk="function" returns="Object" signature="snapPoint(pt):Object" doc="Takes a point as input and returns a new point that may be adjusted or snapped to the nearest geometric object. If snapping is disabled in the View menu in the Flash user interface, the point returned is the original point. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77d7.html">
					<scope name="pt" ilk="argument" citdl="Object" doc="Specifies the location of the point for which you want to return a snap point. "/>
				</scope>
				<variable name="toolObjs" citdl="Array" doc="Read-only property; an array of ToolObj objects (see ToolObj object)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f32.html"/>
			</scope>
			<!--

				Vertex

			-->
			<scope name="Vertex" ilk="class" doc="The Vertex object is the part of the shape data structure that holds the coordinate data." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f50.html">
				<scope name="getHalfEdge" ilk="function" returns="HalfEdge" signature="getHalfEdge():HalfEdge" doc="Gets a HalfEdge object that shares this vertex. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77c4.html"/>
				<scope name="setLocation" ilk="function" signature="setLocation(x, y)" doc="Sets the location of the vertex. You must call shape.beginEdit() before using this method." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77c3.html">
					<scope name="x" ilk="argument" citdl="Number" doc="A floating-point value that specifies the x coordinate of where the vertex should be positioned, in pixels. "/>
					<scope name="y" ilk="argument" citdl="Number" doc="A floating-point value that specifies the y coordinate of where the vertex should be positioned, in pixels. "/>
				</scope>
				<variable name="x" citdl="Vertex" doc="Read-only property; the x location of the vertex, in pixels. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77bd.html"/>
				<variable name="y" citdl="Vertex" doc="Read-only property; the y location of the vertex, in pixels. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77be.html"/>
			</scope>
			<!--

				VideoItem

			-->
			<scope name="VideoItem" ilk="class" classrefs="Item" doc="The VideoItem object is a subclass of the Item object." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7f74.html">
				<scope name="exportToFLV" ilk="function" returns="Boolean" signature="exportToFLV(fileURI):Boolean" doc="Exports the specified item to an FLV file. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB6444A20-C3F1-45e4-AE10-1FEAFBD1C426.html">
					<scope name="fileURI" ilk="argument" citdl="String" doc="A string, expressed as a file:/// URI, that specifies the path and name of the exported file."/>
				</scope>
				<variable name="fileLastModifiedDate" citdl="String" doc="Read-only property: a string containing a hexadecimal number that represents the number of seconds that have elapsed between January 1, 1970, and the modification date of the original file (on disk) at the time the file was imported to the library. If the file no longer exists, this value is &quot;00000000&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS08BBF48C-D97F-4ac0-90E3-E05602F4F0A1.html"/>
				<variable name="sourceFileExists" citdl="Boolean" doc="Read-only property: a Boolean value of true if the file that was imported to the Library still exists in the location from where it was imported; false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB4241487-0713-486f-B3D5-8234670FF501.html"/>
				<variable name="sourceFileIsCurrent" citdl="Boolean" doc="Read-only property: a Boolean value of true if the file modification date of the Library item is the same as the modification date (on disk) of the file that was imported; false otherwise. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WSB4C48137-BF6E-4ce0-BACC-38FD74F34F2D.html"/>
				<variable name="sourceFilePath" citdl="String" doc="Read-only property; a string, expressed as a file:/// URI that specifies the path to the video item. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7788.html"/>
				<variable name="videoType" citdl="String" doc="Read-only property; a string that specifies the type of video the item represents. Possible values are &quot;embedded video&quot;, &quot;linked video&quot;, and &quot;video&quot;." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7a60.html"/>
			</scope>
			<!--

				XMLUI

			-->
			<scope name="XMLUI" ilk="class" doc="Flash 8 supports custom dialog boxes written in a subset of the XML User Interface Language (XUL). An XML User Interface (XMLUI) dialog box can be used by several Flash features, such as commands and behaviors, to provide a user interface for features that you build using extensibility. The XMLUI object provides the ability to get and set properties of an XMLUI dialog box, and accept or cancel out of one. The XMLUI methods can be used in callbacks, such as oncommand handlers in buttons." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7fea.html">
				<scope name="accept" ilk="function" signature="accept()" doc="Closes the current XMLUI dialog box with an accept state, which is equivalent to the user clicking the OK button." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77bb.html"/>
				<scope name="cancel" ilk="function" signature="cancel()" doc="Closes the current XMLUI dialog box with a cancel state, which is equivalent to the user clicking the Cancel button." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77ba.html"/>
				<scope name="get" ilk="function" returns="String" signature="get(controlPropertyName):String" doc="Retrieves the value of the specified property of the current XMLUI dialog box." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77b9.html">
					<scope name="controlPropertyName" ilk="argument" citdl="String" doc="A string that specifies the name of the XMLUI property whose value you want to retrieve. "/>
				</scope>
				<scope name="getControlItemElement" ilk="function" returns="Object" signature="getControlItemElement(controlPropertyName):Object" doc="Returns the label and value of the line selected in a ListBox or ComboBox control for the control specified by controlPropertyName." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77b8.html">
					<scope name="controlPropertyName" ilk="argument" citdl="String" doc="A string that specifies the property whose control item element you want to retrieve."/>
				</scope>
				<scope name="getEnabled" ilk="function" returns="Boolean" signature="getEnabled(controlID):Boolean" doc="Returns a Boolean value that specifies whether the control is enabled or disabled (dimmed)." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77b7.html">
					<scope name="controlID" ilk="argument" citdl="String" doc="A string that specifies the ID attribute of the control whose status you want to retrieve."/>
				</scope>
				<scope name="getVisible" ilk="function" returns="Boolean" signature="getVisible(controlID):Boolean" doc="Returns a Boolean value that specifies whether the control is visible or hidden." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-7790.html">
					<scope name="controlID" ilk="argument" citdl="String" doc="A string that specifies the ID attribute of the control whose visibility status you want to retrieve."/>
				</scope>
				<scope name="set" ilk="function" signature="set(controlPropertyName, value)" doc="Modifies the value of the specified property of the current XMLUI dialog box." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77b4.html">
					<scope name="controlPropertyName" ilk="argument" citdl="String" doc="A string that specifies the name of XMLUI property to modify."/>
					<scope name="value" ilk="argument" citdl="String" doc="A string that specifies the value to which you want to set the XMLUI property. "/>
				</scope>
				<scope name="setControlItemElement" ilk="function" signature="setControlItemElement(controlPropertyName, elementItem)" doc="Sets the label and value of the currently selected line in the ListBox or ComboBox control specified by controlPropertyName." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77b3.html">
					<scope name="controlPropertyName" ilk="argument" citdl="String" doc="A string that specifies the control item element to set."/>
					<scope name="elementItem" ilk="argument" citdl="Object" doc="A JavaScript object with a string property named label and an optional string property named value. If the value property does not exist, then it is created and assigned the same value as label."/>
				</scope>
				<scope name="setControlItemElements" ilk="function" signature="setControlItemElements(controlID, elementItemArray)" doc="Clears the values of the ListBox or ComboBox control specified by controlID and replaces the list or menu items with the label, value pairs specified by elementItemArray. " url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77b0.html">
					<scope name="controlID" ilk="argument" citdl="String" doc="A string that specifies the ID attribute of the control you want to set."/>
					<scope name="elementItemArray" ilk="argument" citdl="Array" doc="An array of JavaScript objects, where each object has a string property named label and an optional string property named value. If the value property does not exist, then it is created and assigned the same value as label."/>
				</scope>
				<scope name="setEnabled" ilk="function" signature="setEnabled(controlID, enable)" doc="Enables or disables (dims) a control." url="http://help.adobe.com/en_US/Flash/10.0_ExtendingFlash/WS5b3ccc516d4fbf351e63e3d118a9024f3f-77a3.html">
					<scope name="controlID" ilk="argument" citdl="String" doc="A string that specifies the ID attribute of the control you want to enable or disable."/>
